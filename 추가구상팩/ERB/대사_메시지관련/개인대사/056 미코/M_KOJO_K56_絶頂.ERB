;-------------------------------------------------
;노예의 절정시의 구상
;NOWEX:C절정～3 CVAB절정
;NOWEX:M절정 이중절정
;NOWEX:이중절정 사중절정
;NOWEX:분유
;NOWEX:사정
;NOWEX:방뇨
;適当に組み合わせて使ってください
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K56
CALL PALAM_MESSAGE_B
;-------------------------------------------------
;분유
;기입 체크（=0, 비표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:분유
	PRINTFORMW 「거짓말…!? 가슴에섯 나왓%UNICODE(0x2665)%」
ENDIF
;-------------------------------------------------
;사정
;기입 체크（=0, 비표시、1, 표시）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:사정
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;방뇨
;기입 체크（=0, 비표시、1, 표시）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:방뇨
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;절정
;기입 체크（=0, 비표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;오중절정
	;기입 체크（=0, 비표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:오중절정
		PRINTFORMW 「하우…!? 오゛옷%UNICODE(0x2665)% 응호오゛오゛오゛오゛오゛오゛오゛옷」
		PRINTFORMW 짐승과도 같은 비명을 지르며 절정을 맞이한 미코는、온갖 체액을 줄줄 흘리며 무너져 내렸다。
		PRINTFORMW 「아ー…%UNICODE(0x2665)% 아ー…%UNICODE(0x2665)%」
		PRINTFORMW 정신을 잃을 정도로 강한 쾌락을 맛보며、반쯤 벌어진 입에서는 이성을 잃어버린 헐떡이는 소리와 침이 칠칠치 못하게 흘러나왔다
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;사중절정
	;기입 체크（=0, 비표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:사중절정
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
		SELECTCASE NOWEX:사중절정
			;C＆V＆A＆B절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;C＆V＆A＆M절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;C＆V＆B＆M절정
			CASE 3
				PRINTFORMW 「시럿…%UNICODE(0x2665)%와버렷…와버렷%UNICODE(0x2665)% 앗 앗%UNICODE(0x2665)%」
				PRINTFORMW 질내로 받아들이면서 민감한 부위를 끈적끈적하게 귀여워해져서、미코는 몸을 부들부들 떨고 있다。
				PRINTFORMW 「아햐아앗!? 응하앗、읏아아아아아앙%UNICODE(0x2665)%」
				PRINTFORMW 마지막으로 꽉 껴안은 채로、질내에 페니스를 기세 좋게 찔러올리면 결합부에서 조수를 뿜어져 나오면서 미친 듯이 몸부림쳤다
				RETURN 1
			;C＆A＆B＆M절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;V＆A＆B＆M절정
			CASE 5
				PRINTFORMW 
				RETURN 1
		ENDSELECT
	ENDIF
	;-------------------------------------------------
	;삼중절정
	;기입 체크（=0, 비표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:삼중절정
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
		;[SKIPSTART]
		SELECTCASE NOWEX:삼중절정
			;C＆V＆A절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;B＆V＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;C＆B＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆V＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;C＆V＆M절정
			CASE 5
				PRINTFORMW 
				RETURN 1
			;B＆V＆M절정
			CASE 6
				PRINTFORMW 
				RETURN 1
			;A＆V＆M절정
			CASE 7
				PRINTFORMW 
				RETURN 1
			;C＆B＆M절정
			CASE 8
				PRINTFORMW 
				RETURN 1
			;A＆B＆M절정
			CASE 9
				PRINTFORMW 
				RETURN 1
			;C＆A＆M절정
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;이중절정
	;기입 체크（=0, 비표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:이중절정
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
		;[SKIPSTART]
		SELECTCASE NOWEX:이중절정
			;C＆V절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;C＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;V＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;V＆B절정
			CASE 5
				PRINTFORMW 
				RETURN 1
			;A＆B절정
			CASE 6
				PRINTFORMW 
				RETURN 1
			;A＆M절정
			CASE 7
				PRINTFORMW 
				RETURN 1
			;V＆M절정
			CASE 8
				PRINTFORMW 
				RETURN 1
			;C＆M절정
			CASE 9
				PRINTFORMW 
				RETURN 1
			;B＆M절정
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	IF NOWEX:C절정
		;超強절정C
		IF NOWEX:C절정 == 3
			PRINTFORMW 
		;強절정C
		ELSEIF NOWEX:C절정 == 2
			PRINTFORMW 
		;절정C
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:V절정
		;超強절정V
		IF NOWEX:V절정 == 3
			PRINTFORMW 
		;強절정V
		ELSEIF NOWEX:V절정 == 2
			PRINTFORMW 
		;절정V
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:A절정
		;超強절정A
		IF NOWEX:A절정 == 3
			PRINTFORMW 
		;強절정A
		ELSEIF NOWEX:A절정 == 2
			PRINTFORMW 
		;절정A
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:B절정
		;強절정B
		IF NOWEX:B절정 == 3
			PRINTFORMW 
		;強절정B
		ELSEIF NOWEX:B절정 == 2
			PRINTFORMW 
		;절정B
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:M절정
		;超強절정M
		IF NOWEX:M절정 == 3
			PRINTFORMW 
		;強절정M
		ELSEIF NOWEX:M절정 == 2
			PRINTFORMW 
		;절정M
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ENDIF


