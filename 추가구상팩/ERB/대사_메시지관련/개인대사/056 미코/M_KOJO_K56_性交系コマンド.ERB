;-------------------------------------------------
;立位
;기입 체크（=0, 비표시、1, 표시）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && TFLAG:193
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;処女
		IF TCVAR:파과 == 1
			PRINTFORMW 
			RETURN 0
		ELSE
			PRINTFORMW 
			RETURN 0
		ENDIF
	ENDIF
	;-------------------------------------------------
	;삽입継続
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:100 == PLAYER
		;前回射精した
		IF TCVAR:102
			PRINTFORMW 
			RETURN 0
		ELSE
			PRINTFORMW 
			RETURN 0
		ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 0
	;연모
	ELSEIF TALENT:연모
		PRINTFORMW 
		RETURN 0
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 
		RETURN 0
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 0
	ENDIF
ENDIF
;-------------------------------------------------
;正常位
;기입 체크（=0, 비표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;時姦中
		IF FLAG:70
			PRINTFORMW 얼어붙은 시간 속에서 미코를 정면에서 범하기 시작했다。
			PRINTFORMW %CALLNAME:MASTER%는 천천히 미코의 비부에 음경을 삽입하기 시작했다。
		;処女
		ELSEIF TCVAR:파과 == 1
			PRINTFORMW 「저기…그…」
			PRINTFORMW 「처음이라서…모르겠지만…」
			PRINTFORMW 「상냥하게 해줘야、해…?」
			PRINTFORMW 그렇게 말하고 미코는 몸을 맡겨 왔다。
		ELSE
			PRINTFORMW 「응…」
			PRINTFORMW 「와줘…%CALLNAME:MASTER%…」
			PRINTFORMW 그 비부에 %CALLNAME:MASTER%의 음경이 천천히 삽입되어져 갔다…
		ENDIF
	ENDIF
	;-------------------------------------------------
	;삽입継続
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:100 == PLAYER
		;時姦中
		IF FLAG:70
			PRINTFORMW 
		;前回射精した
		ELSEIF TCVAR:102
			IF RAND:3 == 0
				PRINTFORMW 「아…%UNICODE(0x2665) *1%」
				PRINTFORMW 「읏…앗…%UNICODE(0x2665) *1%…%CALLNAME:MASTER%…%UNICODE(0x2665) *1%」
				PRINTFORMW 미코는 교성을 지르고 있다。
			ELSEIF RAND:2 == 0
				PRINTFORMW 「읏…%UNICODE(0x2665) *1%…앗…%UNICODE(0x2665) *1%」
				PRINTFORMW 「야해…%UNICODE(0x2665) *1%…좀 더 해줘어…%UNICODE(0x2665) *1%」
				PRINTFORMW 미코는 격렬한 행위에 허덕이고 있다。
			ELSE
				PRINTFORMW 「앗…%UNICODE(0x2665) *1%…읏…%UNICODE(0x2665) *1%」
				PRINTFORMW 「응앗…%UNICODE(0x2665) *1%」
				PRINTFORMW 미코는 격렬한 행위에 허덕이고 있다。
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「읏…%UNICODE(0x2665) *1%…앗…%UNICODE(0x2665) *1%」
				PRINTFORMW 「응앗…%UNICODE(0x2665) *1%…히앗…%UNICODE(0x2665) *1%」
				PRINTFORMW 미코는 행위에 허덕이고 있다。
			ELSEIF RAND:2 == 0
				PRINTFORMW 「히앗…%UNICODE(0x2665) *1%…응큿…%UNICODE(0x2665) *1%」
				PRINTFORMW 「앗…%UNICODE(0x2665) *1%…또…%UNICODE(0x2665) *1%」
				PRINTFORMW 미코는 격렬한 행위에 허덕이고 있다。
			ELSE
				PRINTFORMW 「아읏…%UNICODE(0x2665) *1%…읏…%UNICODE(0x2665) *1%」
				PRINTFORMW 「좀더…%UNICODE(0x2665) *1%…가득…%UNICODE(0x2665) *1%」
				PRINTFORMW 미코는 격렬한 행위에 허덕이고 있다。
			ENDIF
		ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 얼어붙은 시간 속에서 미코를 정면에서 범하기 시작했다。
		PRINTFORMW %CALLNAME:MASTER%는 천천히 미코의 비부에 음경을 삽입하기 시작했다。
	;연모
	ELSEIF TALENT:연모
			PRINTFORMW 미코의 가는 팔을 붙잡고、깔아 눕히는 것처럼 밀어 넘어뜨려 삽입했다。
			PRINTFORMW 미코는 저항하는 기색을 보이지 않고、자신의 암컷구멍에 %CALLNAME:MASTER%를 받아들였다。
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		IF RAND:3 == 0
			PRINTFORMW 「앗…%UNICODE(0x2665) *1%…응…%UNICODE(0x2665) *1%」
			PRINTFORMW 「%CALLNAME:MASTER%…%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「읏…응햐…%UNICODE(0x2665) *1%」
			PRINTFORMW 「…좋아…%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「아…」
			PRINTFORMW 「싸줘…%UNICODE(0x2665) *1%」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「응…상관없지만…」
			PRINTFORMW 「아프게 하면 안돼?」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…와줘…」
		ELSE
			PRINTFORMW 「이거어…응…」
		ENDIF
	ENDIF
ENDIF
RETURN 1
