@M_KOJO_MESSAGE_COM_K28_00
;성교 처녀상실을 RETURN 100해
;IF RESULT == 100 으로 정리한다든가
;고통이나 쾌락의 SOURCE를 트리거로서 범용 헐떡임 말을 만든다든가
;여러 가지 이용 방법은 생각됩니다만 적당하게 해 주세요

;-------------------------------------------------
;노예의 절정시의 말
;NOWEX:C절정~3 CVAB 절정
;NOWEX:M절정 이중절정
;NOWEX:삼중절정 삼중절정
;NOWEX:이중절정 사중절정
;NOWEX:분유분유
;NOWEX:사정 사정
;NOWEX:방뇨 방뇨
;적당하게 조합해 사용해 주세요
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K28
CALL PALAM_MESSAGE_B
;-------------------------------------------------
;분유
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL && !FLAG:70 && NOWEX:분유
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;사정
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL && !FLAG:70 && NOWEX:사정
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;방뇨
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL && !FLAG:70 && NOWEX:방뇨
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;절정
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL && !FLAG:70 
	;-------------------------------------------------
	;사중절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:이중절정
		PRINTFORMW 
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;삼중절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:삼중절정
		SELECTCASE NOWEX:삼중절정
			;C＆V＆A절정
			CASE 1
				PRINTFORMW 
				RETURN 0
			;B＆V＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 0
			;C＆B＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 0
			;C＆V＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 0
		ENDSELECT
	ENDIF
	;-------------------------------------------------
	;이중절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:M절정
		SELECTCASE NOWEX:M절정
			;C＆V절정
			CASE 1
				PRINTFORMW 
				RETURN 0
			;C＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 0
			;V＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 0
			;C＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 0
			;V＆B절정
			CASE 5
				PRINTFORMW 
				RETURN 0
			;A＆B절정
			CASE 6
				PRINTFORMW 
				RETURN 0
		ENDSELECT
	ENDIF
	IF NOWEX:C절정
		;강절정 C
		IF NOWEX:C절정 == 2
			PRINTFORMW 
		;절정 C
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:V절정
		;강절정 V
		IF NOWEX:V절정 == 2
