IF LOCAL
	;동시 사정
	IF NOWEX:사정 && NOWEX:MASTER:11
		PRINTFORMW 
		RETURN 1
	;TARGET만 사정
	ELSEIF NOWEX:사정
		;PLAYER에 대하고 있다
		IF TALENT:PLAYER & 2
			PRINTFORMW 
			RETURN 1
		;붙지 않다
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;PLAYER만 사정
	ELSE
		;TARGET에 대하고 있다
		IF TALENT:2 & 2
			PRINTFORMW 
			RETURN 1
		;붙지 않다
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

