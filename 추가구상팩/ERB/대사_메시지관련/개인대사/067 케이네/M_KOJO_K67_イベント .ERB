					PRINTFORMW 「앗……치사해 %CALLNAME:MASTER%, 자기 차례일 때만 도망치고」
					RETURN 1
				ENDIF
			CASE 1
			;抱き着きモード満足終了
				PRINTFORML
				RETURN 1
			CASE 2
			;抱き着きモード強制終了
				PRINTFORMW 「무으……」
				RETURN 1
		ENDSELECT
	ENDIF
	;-------------------------------------------------
	;抜け出す失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
