@M_KOJO_MESSAGE_COM_K68_84
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K68_84_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K68_84_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;기본 세트
	;연모
	IF TALENT:연모
