;6,挿入Ｇスポ責め
;==================================================
@M_KOJO_MESSAGE_SCOM_K34_6
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K34_6_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K34_6_1
;-------------------------------------------------
;記入チェック（=0, 비표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORML 「거깃%UNICODE(0x2764)%기분좋아아%UNICODE(0x2764)%안됫 머리에 울릴 졍도료 조햐아%UNICODE(0x2764)%%UNICODE(0x2764)%」
		RETURN 1
	ENDIF
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:101 == PLAYER
		;前回射精した
		IF TCVAR:104
			PRINTFORML 「방그음%UNICODE(0x2764)%사정했으면서어%UNICODE(0x2764)%」
			PRINTFORML 「거깃 좋앗%UNICODE(0x2764)% 안됏%UNICODE(0x2764)%괴롭히는 거 안햬애%UNICODE(0x2764)%%UNICODE(0x2764)%」
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	IF TCVAR:140
		PRINTFORML 「햐우우%UNICODE(0x2764)%거기 조햐아아%UNICODE(0x2764)%좀뎌 만혀줘어%UNICODE(0x2764)%%UNICODE(0x2764)%」
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「읏%UNICODE(0x2764)%거기는 약하니까아%UNICODE(0x2764)%안돼앳%UNICODE(0x2764)%」
		CASE 1
			PRINTFORML 「기분 좋은 곳만 햬서어%UNICODE(0x2764)%이상해져 버려%UNICODE(0x2764)%%UNICODE(0x2764)%」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K34_{SELECTCOM}_1
RETURN 1

;==================================================
;7,挿入子宮口責め
;==================================================
@M_KOJO_MESSAGE_SCOM_K34_7
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K34_7_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K34_7_1
;-------------------------------------------------
;記入チェック（=0, 비표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORML 「응그우%UNICODE(0x2764)%댸다냇%UNICODE(0x2764)%안쪽까지 쪽쪽 키스당햬 버려써어%UNICODE(0x2764)%」
		RETURN 1
	ENDIF
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:101 == PLAYER
		;前回射精した
		IF TCVAR:104
			PRINTFORML 「햐앙 안돼애%UNICODE(0x2764)%사정한지 얼마 안됬는데 찔리면 이상햬져 퍼려어%UNICODE(0x2764)%%UNICODE(0x2764)%」
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	IF TCVAR:140
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「히아아앗!?　거기, 안쪽까지 닿아서어…%UNICODE(0x2764)%」
			PRINTFORMW 「이거, 엄청 기분 좋아, 아앙%UNICODE(0x2764)%」
			PRINTFORMW 「후아아앗!　머리가 이상해져 버려어%UNICODE(0x2764)%%UNICODE(0x2764)%」
		CASE 1
			PRINTFORMW 「싫, 후앙, 햐앙%UNICODE(0x2764)%」
			PRINTFORMW 「자, 자궁잇, 올라가 버려어%UNICODE(0x2764)%」
			PRINTFORMW 「싫어엇!　이상해져버려어%UNICODE(0x2764)%%UNICODE(0x2764)%」
		ENDSELECT
	ELSEIF TALENT:78
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「싫, 하앙, 자구웅, 올라가버려서어…」
			PRINTFORMW 「히앙, 하앙, 후아앙!」
			PRINTFORMW （이렇게나 큰 거가…　이제 안돼…%UNICODE(0x2764)%）
		CASE 1
			PRINTFORMW 「후와앗!　안까지 닿고 있어어!」
				PRINTFORMW 「좋아앗!　좀더 찔러줘어!」
		CASE 2
			PRINTFORMW 「후아앙!　안이이!　따뜻해져서 기분 좋아아!」
			PRINTFORMW 「후앗, 응햐아아앗!」
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「아앗%UNICODE(0x2764)%그렇게나 깊이이%UNICODE(0x2764)%」
		CASE 1
			PRINTFORML 「안쪼옥%UNICODE(0x2764)%좀 더어%UNICODE(0x2764)%좋아아아%UNICODE(0x2764)%좋아아%UNICODE(0x2764)%%UNICODE(0x2764)%」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
