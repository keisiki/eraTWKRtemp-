	;うふふ==2（開始時）
	;記入チェック（=0, 非表示、1, 表示）
	;押し倒されている時に立つフラグ
	CFLAG:1211 = 1
	;聖に押し倒された回数を加算
	CFLAG:1216 ++
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG
		;-------------------------------------------------
		;MASTERを押し倒した（開始時）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			IF CFLAG:1215 == 0
				;恋慕かつ恋人
				;/普通にプレイしてたら多分見る機会はない。
				IF TALENT:恋慕 && TALENT:恋人
					PRINTFORMW 白蓮はそのままあなたを抱きしめると、胸元に顔をうずめた。
					PRINTFORML
					PRINTFORMW 「……ねえ、私を抱いてくれないかしら」
					PRINTFORMW 「確かに私はここの住職です。それゆえの立場というものもあります」
					PRINTFORMW 「でも、私は、……私たちは、恋人同士じゃありませんか……」
					PRINTFORMW 表情こそ伺えないものの、震える声が彼女の心を示していた。
					PRINTFORMW 「%CALLNAME:MASTER%、私は、あなたが…私の、最愛の人が欲しい」
					PRINTFORMW 白蓮は顔を上げた。激情に染まる頬を、一筋の涙が伝う。
					PRINTFORML 「…もう。なんですか。ここまで言っても手を出してくれない意気地なしに、
					PRINTFORMW 私は惚れたっていうんですか」
				;恋慕
				ELSEIF TALENT:恋慕
					PRINTFORMW 白蓮はあなたの上に跨って、切なげな目で見降ろしている。
					PRINTFORMW 「ねえ、あなたがどうしても嫌というのでなければ…
					PRINTFORMW しばらくそのまま、大人しくしていてくれませんか」
					PRINTFORMW ささやくように言いながら、あなたの胸元へと静かに手を這わせる。 
					PRINTFORMW 「僧侶失格と言われても、…軽蔑してくれても、構いません」
					PRINTFORMW 「でも、私は僧侶である以前に女なんです…」
					PRINTFORMW 白蓮はぐっと顔を近づけ、潤んだ目であなたを見据えた。
					PRINTFORMW 「お願い、私に、……少しの間だけ、夢を見させて…」
				ELSE
					PRINTFORMW 「ふぅ、まったくもう……」
					PRINTFORMW 「なんですか。ひとが大人しくしていたら、あちこち好き放題してくれて……」
					PRINTFORMW 「確かに私は仏門に入った身ではありますが…それでも、限度というものがあります」
					PRINTFORMW 「はぁ…%UNICODE(0x2665)%仕方がありません…お仕置きして差し上げます」
					PRINTFORMW 白蓮は情欲に燃えた目であなたを射すくめる。
					PRINTFORMW 「あなたも、それをお望みなのでしょう？」
					;非恋慕時に初うふふフラグ
					CFLAG:1203 = 1
				ENDIF 
				;MASTERが押し倒したことがないなら、今日を初うふふした日として記録
				;MASTERが押し倒したことがないなら、「MASTERが押し倒されて初うふふ突入」フラグを立てる
				IF CFLAG:1200 == 0
					CFLAG:1200 = 2
				ENDIF
				CFLAG:1215 = 1
				RETURN 1
			;二回目以降
			;恋慕
			ELSEIF TALENT:恋慕
				;あなたの押し倒され回数が50回を超えた（一回のみ）。［淫乱］取得イベント
				;/淫乱を取得したからといって何が変わるわけでもない。趣味。気が向いた人はいろんなコマンドに淫乱分岐でも足すといいよ
				IF CFLAG:1216 == 50
					TALENT:淫乱 = 1
				ENDIF
				;キスするの直後
				IF PREVCOM == 312
					IF RAND:4 == 0 
						PRINTFORMW 「…ぷぁ、…はぁっ…」
						PRINTFORMW 「ねえ、%CALLNAME:MASTER%。いい、でしょ…%UNICODE(0x2665)%」
						PRINTFORMW 情欲に突き動かされるように、白蓮の手があなたの身体を撫でまわす…
					ELSEIF RAND:3 == 0 
						PRINTFORMW 白蓮はあなたを組み敷くと、口唇の間に舌を滑り込ませて熱烈なキスを続けた。
						PRINTFORMW 「んちゅ、ちゅううぅ、ちゅむ、れろぉ、ちゅぴ…れる、ちゅ、ぷは、ぁ…%UNICODE(0x2665)%」
						PRINTFORMW ようやく口を離すと、銀の糸が二人の口許を繋いだ…。
					ELSEIF RAND:2 == 0
						PRINTFORMW 「ねえ、もうキスだけじゃ満足できないんです」
						PRINTFORMW 「ほら、あなただって嫌じゃないでしょう…？」
					ELSE
						PRINTFORMW 「ああ、もどかしい」
						PRINTFORMW 「私はもっと全身で、あなたを感じたいのに…%UNICODE(0x2665)%」
					ENDIF
				;尻を撫でる、胸愛撫、秘所愛撫、アナル愛撫、指挿入れの直後
				ELSEIF GROUPMATCH(PREVCOM, 310, 313, 314, 315,316)
					IF RAND:3 == 0 
						PRINTFORML 「……はぁ。そんな風に触られると、本気になっちゃうじゃないですか…%UNICODE(0x2665)%」
					ELSEIF RAND:2 == 0 
						PRINTFORMW 「もう、好き放題撫でまわしてくれちゃって…」
						PRINTFORMW 「今度は私の番ですからね…%UNICODE(0x2665)%」
						PRINTFORMW 白蓮は欲情に潤んだ瞳であなたを見つめている…
					ELSE
						PRINTFORML 「そうやっていやらしい触り方して、私をその気にさせて…」
						PRINTFORMW 「この責任は、きちんと取ってもらいますからね…♪」
					ENDIF
				;食事をとる・食事を振る舞うの直後
				ELSEIF PREVCOM == 414 || PREVCOM == 415
					IF RAND:2 == 0
						PRINTFORMW 「食後の運動、とでもいかないかしら…？」
					ELSE
						PRINTFORMW 「あら、ここにも御馳走がありましたね…%UNICODE(0x2665)%」
					ENDIF
				;頭を撫でるの直後
				ELSEIF PREVCOM == 309
					IF RAND:2 == 0
						PRINTFORMW 「頭を撫でるだけなんて…意地悪のつもりかしら？」
						PRINTFORMW 「私はもう、こんなに燃え上がっているのに…」
					ELSE
						PRINTFORMW 「ねえ、もう…頭だけじゃ、物足りないの…%UNICODE(0x2665)%」
						PRINTFORMW あなたに跨ったまま、白蓮は淫靡に微笑んだ。
					ENDIF
				;浴場にいる
				ELSEIF CFLAG:現在位置 == 12
					IF RAND:2 == 0 
						PRINTFORML 「…さっきから私を見てるの、気づかないとでも思いました？」
					ELSE
						PRINTFORML 「…ふふ、%CALLNAME:MASTER%。あなたの裸も、中々綺麗なものですね…♪」
						PRINTFORML 「ねぇ、私、我慢できなくなっちゃいました…%UNICODE(0x2665)%」
						PRINTFORMW 白蓮は豊満な裸体をまとわりつかせ、あなたを誘っている…
					ENDIF
				ELSE
					IF RAND:4 == 0 
						PRINTFORML 「…ねえ、%CALLNAME:MASTER%…しましょう？」
						PRINTFORMW 情欲に燃えた白蓮の目があなたを見降ろしている。
					ELSEIF RAND:3 == 0
						PRINTFORML 「仕事柄、いろいろと溜まるものもあってね…」
						PRINTFORMW 「悪いけれど、すこし付き合ってもらいますよ…%UNICODE(0x2665)%」
					ELSEIF RAND:2 == 0
						PRINTFORML 「ああ…ごめんなさい…」
						PRINTFORMW 「でも、もう我慢の限界なんです…%UNICODE(0x2665)%」
					ELSE
						PRINTFORMW 「もう、いけない人…」
						PRINTFORMW 「私をこんな気分にさせて…%UNICODE(0x2665)%」
					ENDIF
				ENDIF
			;恋慕なし
			ELSE
				IF RAND:4 == 0 
					PRINTFORML 「もう、いけない人…」
					PRINTFORML 「私をこんな気分にさせて…」
					PRINTFORMW 「これは、お仕置きが必要ですね…%UNICODE(0x2665)%」
				ELSEIF RAND:3 == 0
					PRINTFORML 「ああ、ごめんなさい…」
					PRINTFORMW 「でももう、我慢できないの…」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「%CALLNAME:MASTER%は不埒に過ぎます」
					PRINTFORMW 「私が懲らしめてあげますからね…」
				ELSE
					PRINTFORML 「まったく仕方のない人ですねえ…」
					PRINTFORMW 「お仕置きです。甘んじてお受けなさい…%UNICODE(0x2665)%」
				ENDIF
			ENDIF
		ENDIF
		;-------------------------------------------------
		;MASTERに抱きついた（開始時）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORML
			RETURN 1
		ENDIF
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;なだめられた（終了時）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		;-------------------------------------------------
		;抜け出した
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			;うふふ状態解除を示すフラグ
			CFLAG:1211 = 0
			;◆初めて
			IF FIRSTTIME(TFLAG:50 + 500)
				PRINTFORMW 「……はっ！」
				PRINTFORMW %CALLNAME:MASTER%が押しとどめると、白蓮は目を見開いた。
				PRINTFORMW 「す、すみません%CALLNAME:MASTER%。私としたことがつい…」
				PRINTFORMW 「でも、あなただって悪いんですよ、あんなふうに私に優しくして…」
				PRINTFORMW 「…すみません、言い訳はやめにしましょう」
				PRINTFORMW 「……私が一時の情欲に堕さぬよう導いてくれたことに感謝します、%CALLNAME:MASTER%」
				PRINTFORML
				PRINTFORMW (ただ、私をこんな気分にさせて放っておくあなたを、一人の女として恨みます…)
			ELSE
				IF RAND:3 == 0 
					PRINTFORML 「あっ…」
					PRINTFORML 白蓮はあなたから飛び退いて、いそいそと着衣を正した。
					PRINTFORMW 「……うう」
				ELSEIF RAND:2 == 0 
					PRINTFORML 「…いじわる」
					PRINTFORMW 白蓮は小さくつぶやいた。
				ELSE
					PRINTFORMW 「…は、ぁ…ご、ごめんなさい」
				ENDIF
			ENDIF
		ENDIF
		RETURN 1
		;-------------------------------------------------
		;抱き着きモード満足終了
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORML
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;抱き着きモード強制終了
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 2
			PRINTFORML
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;抜け出す失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		;-------------------------------------------------
		;好きにして状態から抜け出せなかった
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORML
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;欲求不満が高くて抜け出せなかった
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 2
			PRINTFORML
			RETURN 1
		ENDIF
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

