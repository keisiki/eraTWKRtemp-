				PRINTFORMW 「今日はもう休みましょう、ね？」
			ENDIF
		ENDIF
			PRINTFORML 　
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが自室まで戻ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
