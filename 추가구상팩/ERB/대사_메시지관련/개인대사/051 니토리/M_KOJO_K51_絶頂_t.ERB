@M_KOJO_MESSAGE_PALAMCNG_A_K51_23(ARG)
CALL PALAM_MESSAGE_A
;-------------------------------------------------
;이라마치오
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
