@M_KOJO_MESSAGE_COM_K12_0_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;연모
        IF TALENT:연모
            PRINTFORMW 「%CALLNAME:PLAYER%… 상냥해에… 알고 있었어어?」
        ;굴복각인Lv2 이상
        ELSEIF MARK:괘씸각인 >= 2
            PRINTFORMW 「읏……」
            PRINTFORMW 루미아는 입가에 손을 대고 소리를 죽이고 있다.
        ;이외의 경우
        ELSE
            PRINTFORMW 「저기, 만지는거야?」
        ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;삽입중
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	;V삽입
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;사정했다
		IF NOWEX:PLAYER:11
			PRINTFORMW
			RETURN 1
		ELSE
			PRINTFORMW
			RETURN 1
		ENDIF
	;A삽입
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;사정했다
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;후타나리
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;기본 세트
	;시간중
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;연모
	ELSEIF TALENT:연모
	    IF ABL:0 >= 10
		     PRINTFORMW 「아… 어쩐지이…머리가 둥실둥실해에……」
		ELSE
		     PRINTFORMW 「응 …… 좀 더…, 문질러어……!」
		ENDIF
		RETURN 1
	;굴복 3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 「아…하앗 ……으으읏…」
		RETURN 1
	;굴복각인 2이하
	ELSEIF MARK:괘씸각인 < 3
	    PRINTFORMW 「우우-… 조금 이상하게 될 것같아아ㅅ……」
	    RETURN 1
	;그 이외
	ELSE
		PRINTFORMW 「아, 간지러운거야…」
		RETURN 1
	ENDIF
ENDIF
RETURN 1
