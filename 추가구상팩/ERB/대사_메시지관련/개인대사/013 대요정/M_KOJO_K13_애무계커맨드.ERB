@M_KOJO_MESSAGE_COM_K13_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;時姦中
		IF FLAG:70
			RETURN 1
		ELSE
			PRINTFORMW 「히얏…그렇게 만지면…응읏」
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	;V挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;A挿入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;연모
	ELSEIF TALENT:연모
		IF RAND:3 == 0
			PRINTFORMW 「%UNICODE(0x2665) *1%～거기잇…! 움찔거려서엇%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「거…기는…하앙%UNICODE(0x2665) *1%」
			PRINTFORMW 「상냥하게 해주셔서…기뻐요%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「아응…%UNICODE(0x2665) *1%」
		ENDIF
		RETURN 1
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		IF RAND:3 == 0
			PRINTFORMW 「하앗%UNICODE(0x2665) *1% 시러%UNICODE(0x2665) *1%시러엇%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「내 몸이 아닌 것 같아…%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「이런 걸로…기분 좋아져서....하응%UNICODE(0x2665) *1%」
		ENDIF
		RETURN 1
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「머리 속이…새하얘서...하읏%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…흐읏…아…앙%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「…시러...나...이상해져버려어…%UNICODE(0x2665) *1%」
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1
