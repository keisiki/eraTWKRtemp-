;1,シックスナイン
;==================================================
@M_KOJO_MESSAGE_SCOM_K13_1
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K13_1_1
RETURN RESULT
@M_KOJO_MESSAGE_SCOM_K13_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		;YM에서 이식
        IF TALENT:연모
            PRINTFORM 「…… 이 자세는 부끄러워요……꺄앗!」
            PRINTFORMW 「너무…… 갑작스러워서……응」
        ;それ以外
        ELSE
            PRINTFORMW 「에!? 그, 그런 자세를……」
            PRINTFORMW 「……아, 알겠습니다……. 실례할게요」
        ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		;PRINTFORMW 
		RETURN 1
	;연모
	ELSEIF TALENT:연모
	    IF ABL:봉사정신 >= 20
	        PRINTFORMW 「하응…… 응, 후앗! 응, 츄릅…… 응읏」
            PRINTFORMW 「츄, %CALLNAME:MASTER%…… 어때요…… 아앗!」
        ELSE
		    PRINTFORMW 「츄읍…… 이런 자세, 조금 부끄럽지만……」
            PRINTFORMW 「저도, %CALLNAME:MASTER%도 기분 좋게…… 응앗」 
        ENDIF
		RETURN 1
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 「츄릅…… 응, 아앗! 싫어, 기분이…… 츄읍, 응읏!」 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 「시, 싫어…… 보지마…… 아앙!」 
		RETURN 1
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K13_{SELECTCOM}_1
RETURN 1

