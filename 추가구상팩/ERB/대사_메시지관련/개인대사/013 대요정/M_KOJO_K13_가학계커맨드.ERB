;100,スパンキング
;==================================================
@M_KOJO_MESSAGE_COM_K13_100
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K13_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K13_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;연모
	ELSEIF TALENT:연모
		PRINTFORMW 「시럿…아、아파앗！」
		RETURN 1
	;굴복3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 「시러…기분이 이상해져어..」
		PRINTFORMW 「히잇...!」
		RETURN 1
	;그 이외
	ELSE
		PRINTFORMW 울부짖으며 사과하는 대요정을 무시하고, 하얗고 부드러운 엉덩이를 후려쳤다。
		RETURN 1
	ENDIF
ENDIF
RETURN 1

