;140,イラマチオ
;==================================================
@M_KOJO_MESSAGE_COM_K50_140
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K50_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K50_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF ABL:봉사정신 >= 10
			PRINTFORMW 「입으로, 꼬─옥 하는 거지? 응, 해보겠, 지만」
		ELSE
			PRINTFORMW 「입으로, 하는 거야? 응, 응우우응응응～～읏!」
		ENDIF	
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:연모 && ABL:봉사정신 >= 3
		IF RAND:2 == 0
			PRINTFORMW 「응, 구, 쥬우우. 응웃……응, 우, 괜찮, 아」
		ELSE
			PRINTFORMW 「응구, 응우웃, 쥬읏, 응풋…응웃!?」
			PRINTFORMW 당신이 허리를 내밀자 눈 깜짝할 사이에 목구멍 안쪽까지 페니스가 들어가 버린다
		ENDIF
		RETURN 1
	;봉사정신 3이상
	ELSEIF ABL:봉사정신 >= 3
		PRINTFORMW 「응, 굿, 응웃, 쥬우우……하앗. 응, 응우우……」
		RETURN 1
	;그 이외
	ELSE
		PRINTFORMW 「응굿, 응～～읏, 응───!」
		RETURN 1
	ENDIF
ENDIF
RETURN 1

