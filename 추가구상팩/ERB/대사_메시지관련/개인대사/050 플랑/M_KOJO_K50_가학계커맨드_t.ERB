;==================================================
;104, 눈가리개
;==================================================
@M_KOJO_MESSAGE_COM_K50_104
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K50_104_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K50_104_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;연모
        IF TALENT:연모
            PRINTFORMW 「어랏, 오라버니 어디이!? 거기에 있는 거지!? 어디도 가지 않은 거지!?」
            PRINTFORMW 겁에 질려 외치는 플랑을, %조사처리(CALLNAME:PLAYER,"는")% 꼭 안아 주었다
            PRINTFORMW 「아…… 오라버니다…」
        ;그 외
        ELSE
            PRINTFORMW 「눈 가리기 한다 해도, 별로 상관없어」
        ENDIF
		RETURN 1
	ENDIF
	;기본 세트
	;장착
	IF TEQUIP:눈가리개
		;시간중
		IF FLAG:70
			PRINTFORMW 
			RETURN 1
		;연모
		ELSEIF TALENT:연모
			PRINTFORMW 마스크를 쓰도록 하자, 플랑은 %CALLNAME:PLAYER%의 가슴에 뛰어 들었다
            PRINTFORMW 「에헤헤, 이렇게하면, 새까맣게 되어도 오라버니라는 걸 알 수 있는걸……」
			RETURN 1
		;그 이외
		ELSE
			PRINTFORMW 「또 쓰는 거야아?」
			RETURN 1
		ENDIF
	;해제
	ELSE
		;연모
		IF TALENT:연모
			PRINTFORMW 「응…… 역시 %조사처리(K11CN(),"라")%서, 기뻐」
		;그 외
		ELSE
			PRINTFORMW 「새까만 것쯤이야, 내 내 방에서는 자주 있었던걸」
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

