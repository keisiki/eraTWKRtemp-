;==================================================
;140, 이라마치오
;==================================================
@M_KOJO_MESSAGE_COM_K88_140
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K88_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K88_140_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
;YM
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;연모
        IF TALENT:연모
            PRINTFORMW 「므읍!? 끕!? 콜록!?」
            PRINTFORMW 「어, 억지로 하다니 너무해요…」
        ;その他
        ELSE
            PRINTFORMW 「뭐, 뭘…」
            PRINTFORMW 「끄웁!? 꺽… 켁… 콜록, 콜록…」
        ENDIF
		RETURN 1
	ENDIF
	;기본 세트
	;시간중
	IF FLAG:70
	    ;PRINTFORMW
	    RETURN 1
	;연모
	ELSEIF TALENT:연모
		PRINTFORMW 「끄웁… 콜록, 콜록…」
		RETURN 1
	;그 이외
	ELSE
		PRINTFORMW 「콜록… 끄업… 커헉…」
		RETURN 1
	ENDIF
ENDIF
RETURN 1

