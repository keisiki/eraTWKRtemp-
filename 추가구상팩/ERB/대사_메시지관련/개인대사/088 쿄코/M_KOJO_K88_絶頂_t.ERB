;-------------------------------------------------
;노예의 절정시의 말
;NOWEX:0~4 CVABM 절정
;NOWEX:6 이중절정
;NOWEX:7 삼중절정
;NOWEX:8 사중절정
;NOWEX:9 오중절정
;NOWEX:10 분유
;NOWEX:11 사정
;NOWEX:12 방뇨
;적당하게 조합해 사용해 주세요
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K88
CALL PALAM_MESSAGE_B
;-------------------------------------------------
;분유
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:10
	;연모
    IF TALENT:연모 == 1
        IF RAND:2 == 0
            PRINTFORMW 「응… 흐…」
        ELSE
            PRINTFORMW 「에헤헤헤… 왠지 어머니가 된 것 같아요!」
        ENDIF
    ;굴복각인Lv3
    ELSEIF MARK:괘씸각인 == 3
        IF RAND:2 == 0
            PRINTFORMW 「왜 이렇게…?」
        ELSE
            PRINTFORMW 「젖 나오는 거… 싫어…」
        ENDIF
    ;それ以外
    ELSE
        PRINTFORMW 「왜… 젖 나와…?」
    ENDIF
    RETURN 1
ENDIF
;-------------------------------------------------
;사정
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:11
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;방뇨
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:12
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;절정
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;오중절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:9
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;사중절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:8
		;부위를 지정하지 않고 간이적으로 쓰는 경우,[SKIPSTART]&[SKIPEND]의 코멘트 아웃을 제외해,[SKIPSTART]곳의 코멘트의 사이에 써 주세요
		;[SKIPSTART]
		SELECTCASE NOWEX:8
			;C＆V＆A＆B절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;C＆V＆A＆M절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;C＆V＆B＆M절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆A＆B＆M절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;V＆A＆B＆M절정
			CASE 5
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;삼중절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:7
		;부위를 지정하지 않고 간이적으로 쓰는 경우,[SKIPSTART]&[SKIPEND]의 코멘트 아웃을 제외해,[SKIPSTART]곳의 코멘트의 사이에 써 주세요
		;[SKIPSTART]
		SELECTCASE NOWEX:7
			;C＆V＆A절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;B＆V＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;C＆B＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆V＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;C＆V＆M절정
			CASE 5
				PRINTFORMW 
				RETURN 1
			;B＆V＆M절정
			CASE 6
				PRINTFORMW 
				RETURN 1
			;A＆V＆M절정
			CASE 7
				PRINTFORMW 
				RETURN 1
			;C＆B＆M절정
			CASE 8
				PRINTFORMW 
				RETURN 1
			;A＆B＆M절정
			CASE 9
				PRINTFORMW 
				RETURN 1
			;C＆A＆M절정
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;이중절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:6
		;부위를 지정하지 않고 간이적으로 쓰는 경우,[SKIPSTART]&[SKIPEND]의 코멘트 아웃을 제외해,[SKIPSTART]곳의 코멘트의 사이에 써 주세요
		;[SKIPSTART]
		SELECTCASE NOWEX:6
			;C＆V절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;C＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;V＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;V＆B절정
			CASE 5
				PRINTFORMW 
				RETURN 1
			;A＆B절정
			CASE 6
				PRINTFORMW 
				RETURN 1
			;A＆M절정
			CASE 7
				PRINTFORMW 
				RETURN 1
			;V＆M절정
			CASE 8
				PRINTFORMW 
				RETURN 1
			;C＆M절정
			CASE 9
				PRINTFORMW 
				RETURN 1
			;B＆M절정
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	IF NOWEX:0
		;강절정 C
		IF NOWEX:0 == 2
			PRINTFORMW 「히끅!」
            PRINTFORMW 「가 버려! …안 돼에에에!!!」
		;절정 C(J)
		ELSE
			PRINTFORMW 「아아아아앗!? 기분… 좋아…」
		ENDIF
	ENDIF
	IF NOWEX:1
		;강절정 V
		IF NOWEX:1 == 2
			IF TFLAG:106
			    PRINTFORMW 「흐아아아아… 싫어, 싫어, 싫어…」
                PRINTFORMW 쿄코는 그러면서도 쾌락을 탐하듯 허리를 흔들고 있다.
            ELSE
                PRINTFORMW 「이게 뭐죠…!? 흐으아아아아앗!?」
                PRINTFORMW (지금 게… 절정이란 거야…?)
            ENDIF
		;절정 V
		ELSE
		    IF TFLAG:106
		        PRINTFORMW 「이거 뭐죠!? 몰라요! 이렇게 자지가 기분 좋은 거라니 몰라요!!」
		    ELSE
			    PRINTFORMW 「안 대… 머리가 새하얘져요… 보지가 녹아 버려요…」
			ENDIF
		ENDIF
	ENDIF
	IF NOWEX:2
		;강절정 A
		IF NOWEX:2 == 2
			PRINTFORMW 「엉덩이… %CALLNAME:MASTER% 씨에게 기분 좋아져여어어어어!!!」
            PRINTFORMW 「좀 더… 좀 더… 응아아아아아아!!!」
		;절정 A
		ELSE
			PRINTFORMW 「엉덩이에 넣고… 안 돼… 쿄코, 나쁜 애예요…!!!」
		ENDIF
	ENDIF
	IF NOWEX:3
		;강절정 B
		IF NOWEX:3 == 2
			PRINTFORMW 「쿄코… 가슴으로… 가 버려어어어어!!!」
		;절정 B
		ELSE
		    PRINTFORMW 「시러어… 가슴이 이상해져어어…」
		ENDIF
	ENDIF
	IF NOWEX:4
		;강절정 M
		IF NOWEX:4 == 2
			PRINTFORMW 
		;절정 M
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ENDIF


