@M_KOJO_JP판_MESSAGE_COM_K38_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	IF FIRSTTIME(SELECTCOM)
		IF TALENT:연모
			PRINTFORML 「%CALLNAME:MASTER%のおちんちんを、私の손で？」
