;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;分岐作りに有用そうな変数などのメモ
;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;NOWEX:C절정 NOWEX:V절정 NOWEX:B절정 NOWEX:A절정 NOWEX:M절정
;各部位の절정強度。まず0~3が割り当てられ、多重절정により最大16倍される。つまり単部位なら最大3、5重절정で最大48
;NOWEX:TotalEX
;多重절정補正を受ける前の各部位절정強度の合計値。1~15。このテンプレにおける分岐の基準。

;TEQUIP:50
;Vに誰の竿が入ってるか。TEQUIP:50 = MASTERであなたが挿入中となる。
;TEQUIP:51
;Aに誰の竿が入ってるか。TEQUIP:51 = MASTERであなたが挿入中となる。
;NOWEX:사정
;사정量、最大3。たとえばIF NOWEX:MASTER:사정 > 1だと、あなたが大量사정以上した場合となる。
;SYNCED_ORGASM(TARGET)
;相手とあなたが同時に절정した場合真となる。절정の部位は問わない。

;-------------------------------------------------
;절정時の口上
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K109
#DIM TotalEX
CALL PALAM_MESSAGE_B
;-------------------------------------------------
;분유
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:분유
	PRINTFORMW 
	;CALL HPH_PRINT,@"「앗HPH으웃…HPH 가슴에서 멈추지 않고 흘러나와…HPH」","W"
ENDIF
;-------------------------------------------------
;사정した
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:사정
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;방뇨
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:방뇨
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;절정
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;오중절정
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:오중절정
		IF FLAG:70
			PRINTFORMW 
			RETURN 1
		;사구메일 때
		ELSEIF FLAG:역극 == 110	
		ELSE
			CALL HPH_PRINT,@"「죽어버렷HPH죽어버리니깐HPH그마안HPH…」","L"
			PRINTFORMDL  %마스터는% 도레미의 필사의 간청을 무시하고, 결정타를 찌르기로 했다
			CALL HPH_PRINT,@"「캇HPH가앗HPH가앗HPH가앗HPH가버려엇HPH」","L"
			PRINTFORMDL  도레미는 망가진 스프링클러처럼 조수를 흩뿌리고 있다.
			CALL HPH_PRINT,@"「히깃HPH…호아…하아아아아HPH………오?아……HPH」","L"
			PRINTFORMDW 엄청난 쾌락에 코피가 터진 도레미의 코를 닦아 주었다
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;사중절정
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:사중절정
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
		IF FLAG:70
			PRINTFORMW 
			RETURN 1
		;사구메일 때
		ELSEIF FLAG:역극 == 110	
		ELSE
			SELECTCASE RAND:2
			CASE 0
				CALL HPH_PRINT,@"「옷HPH가아HPH간다아HPH가버려어HPH」","L"
				CALL HPH_PRINT,@"「후우웃HPH…핫HPH훗HPH」","L"
				PRINTFORMDW 쾌락에 과호흡이 되어 있는 도레미를 부둥켜안고 진정되기를 기다렸다
			CASE 1
				CALL HPH_PRINT,@"「앗HPH온닷HPH전부 온닷HPH」","L"
				PRINTFORMDL %마스터는% 손대중하지않았다. 전력으로 스퍼트를 가했다
				CALL HPH_PRINT,@"「오HPH하, 오오옷HPH」","L"
				PRINTFORMDW 도레미는 행복한듯이 아헤가오를 드러내고 있다…
			ENDSELECT
			RETURN 1
		ENDIF
		[SKIPSTART]
		SELECTCASE NOWEX:사중절정
			;C＆V＆A＆B절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;C＆V＆A＆M절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;C＆V＆B＆M절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆A＆B＆M절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;V＆A＆B＆M절정
			CASE 5
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;삼중절정
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:삼중절정
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
		IF FLAG:70
			PRINTFORMW 
			RETURN 1
		;사구메일 때
		ELSEIF FLAG:역극 == 110	
		ELSE
			SELECTCASE RAND:4
			CASE 0
				CALL HPH_PRINT,@"「응응우읏HPH온닷HPH가HPH아아아앗HPH」","L"
				PRINTFORMDW 경련하는 도레미를 꼭 안아 주었다
			CASE 1
				CALL HPH_PRINT,@"「가앗HPH방금 가버렸으니깐HPH옷HPH가버렸어요오HPH」","L"
				PRINTFORMDW 성실하게 보고해 오는 도레미의 머리를 쓰다듬어 주었다
			CASE 2
				CALL HPH_PRINT,@"「앗HPH싫엇HPH너무 대단해…읏HPH간, 다…읏HPH」","L"
				PRINTFORMDW 새우등자세로 절정한 도레미의 몸을 지탱해 주었다
			CASE 3
				CALL HPH_PRINT,@"「옷HPH훗HPH쿠우으으으읏HPH」","L"
				PRINTFORMDW 격렬하게 절정하는 도레미에게 상냥하게 키스했다
			ENDSELECT
			RETURN 1
		ENDIF
		[SKIPSTART]
		SELECTCASE NOWEX:삼중절정
			;C＆V＆A절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;B＆V＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;C＆B＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆V＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;C＆V＆M절정
			CASE 5
				PRINTFORMW 
				RETURN 1
			;B＆V＆M절정
			CASE 6
				PRINTFORMW 
				RETURN 1
			;A＆V＆M절정
			CASE 7
				PRINTFORMW 
				RETURN 1
			;C＆B＆M절정
			CASE 8
				PRINTFORMW 
				RETURN 1
			;A＆B＆M절정
			CASE 9
				PRINTFORMW 
				RETURN 1
			;C＆A＆M절정
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;이중절정
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:이중절정
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
		IF FLAG:70
			PRINTFORMW 
			RETURN 1
		;사구메일 때
		ELSEIF FLAG:역극 == 110	
		ELSE
			SELECTCASE RAND:5
			CASE 0
				CALL HPH_PRINT,@"「쿠앗HPH…좀 더…HPH」","W"
			CASE 1
				CALL HPH_PRINT,@"「그거HPH좋아…앗HPH」","W"
			CASE 2
				CALL HPH_PRINT,@"「앗HPH앗HPH…응우…HPH」","W"
			CASE 3
				CALL HPH_PRINT,@"「쿠웃HPH응응읏…HPH」","W"
			CASE 4
				CALL HPH_PRINT,@"「응응응읏HPH…읏HPH…좀 더…HPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
		[SKIPSTART]
		SELECTCASE NOWEX:이중절정
			;C＆V절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;C＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;V＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;V＆B절정
			CASE 5
				PRINTFORMW 
				RETURN 1
			;A＆B절정
			CASE 6
				PRINTFORMW 
				RETURN 1
			;A＆M절정
			CASE 7
				PRINTFORMW 
				RETURN 1
			;V＆M절정
			CASE 8
				PRINTFORMW 
				RETURN 1
			;C＆M절정
			CASE 9
				PRINTFORMW 
				RETURN 1
			;B＆M절정
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
ENDIF
;-------------------------------------------------
;奴隷の睡眠절정時の口上
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B2_K109
CALL PALAM_MESSAGE_B
;-------------------------------------------------
;睡眠분유
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:분유
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;睡眠사정した
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:사정
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;睡眠방뇨
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:방뇨
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;睡眠절정
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
;合計절정数で分岐
;これだけではどの部位が절정したかは区別されないため、書き分けたければ任意で分岐を加筆されたし。
IF LOCAL
	SELECTCASE NOWEX:TotalEX
		;強
		CASE 5 TO 15
			PRINTFORMW 
		;中
		CASE 2 TO 4
			PRINTFORMW 
		;弱
		CASE 1
			PRINTFORMW 
	ENDSELECT
ENDIF


;-------------------------------------------------
;あなたが사정したときに関する口上の表示
;一時的にTARGETを口上主に設定している
;-------------------------------------------------

@M_KOJO_MESSAGE_PALAMCNG_A_K109_0(ARG)
CALL PALAM_MESSAGE_A
;-------------------------------------------------
;콘돔
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
TRYCALLFORM M_KOJO_COLOR_K109
	IF FLAG:70
			PRINTFORMW 
			RETURN 1
	;사구메일 때
	ELSEIF FLAG:역극 == 110
	ELSEIF TALENT:연모 || TALENT:애욕 || TALENT:섹프 || TALENT:걸레
		IF TALENT:MASTER:농후정액
			SELECTCASE RAND:3
			CASE 0
				IF TALENT:임신소망
					PRINTL 
					PRINTFORML 「……어라…이 콘돔, 구멍이 났는데요…」
					PRINTFORMDL 농후한 정액이 가득 담긴 콘돔 끝부분에서는, 젤리 같은 정액이 조금씩 새어 나오고 있다
					PRINTFORMW 「뭐어, 사소한 일이네요. 오히려 안성맟춤이네요……」
					CALL ADD_SAMEN_V(109, 50)
				ELSE
					PRINTL 
					CALL HPH_PRINT,@"「응…많이 나왔네요…HPH」","L"
					PRINTFORMDL 터질듯이 차서 넘치려한 임신즙을 도레미에게 과시했다
					PRINTFORMDW 만약에 콘돔이 없었다면…라며 귓전에 속삭이자 도레미는 오싹오싹 몸을 떨면서 녹은 미소를 지었다
				ENDIF
			CASE 1
				PRINTL 
				CALL HPH_PRINT,@"「아아…이렇게나 잔뜩…HPH」","L"
				PRINTFORMDL 콘돔에는 젤리 같은 농후 정액이 가득 차 있었다
				PRINTFORMDW 이만한 양을 사정시켰다는 사실에 도레미는 기묘한 충족감을 느꼈다
			CASE 2
				PRINTL 
				CALL HPH_PRINT,@"「그런거, 또렷하게 보여 주지 마세요…HPH」","L"
				PRINTFORMDL 수많은 정자가 헤엄치고 있는 콘돔을 도레미의 눈앞에 가져왔다
				PRINTFORMDW 도레미는 얼굴을 붉히면서도 눈을 뗄 수 없는 것 같다
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 0
				IF TALENT:임신소망
					PRINTL 
					PRINTFORML 「어라…이 콘돔, 구멍이 생겨버렸네」
					PRINTFORMDL 아무래도 불량품인 것 같다. 피임구로서의 역할을 포기해버린 콘돔의 끝에서는 조금씩 정액이 새어 나오고 있다
					PRINTFORMDL 만약 임신해 버렸다면 어떻게 하지……%마스터는% 창백해졌다
					PRINTFORMW 「………후후」
					CALL ADD_SAMEN_V(109, 50)
				ELSE
					PRINTL 
					CALL HPH_PRINT,@"「포동포동해서 좀 귀엽네…HPH」","L"
					PRINTFORMDL 도레미는 정액이 고인 콘돔의 입구를 묶고, 손가락으로 장난쳤다
					PRINTFORMDW 듬뿍 정액이 들어가서 막힌 고무는 꿀단지개미를 방불케 했다
				ENDIF
			CASE 1
				PRINTL 
				CALL HPH_PRINT,@"「기분이 좋았는지 어떤지는…물어볼 것도 없네HPH」","L"
				PRINTFORMDW 빵빵하게 정액이 고인 콘돔을 보고, 도레미는 만족스럽게 웃었다
			CASE 2
				PRINTL 
				CALL HPH_PRINT,@"「굉장했어요…하아…하아…HPH」","L"
				PRINTFORMDL 콘돔을 곁에 두고, 땀범벅의 도레미를 부드럽게 쓰다듬고 키스를 해주었다
				PRINTFORMDW 쪼아대는 듯한 키스를 반복한후, 도레미는 기쁜 듯이 껴안아 왔다
			ENDSELECT
		ENDIF
	ELSE
		IF TALENT:MASTER:농후정액
		SELECTCASE RAND:3
		CASE 0
			PRINTL 
			PRINTFORML 「이렇게나 잔뜩…잘도 나왔네」
			PRINTFORMDW 듬뿍 정액이 고인 콘돔을 도레미는 흥미 있는 듯이 바라보고 있다
		CASE 1
			PRINTL 
			CALL HPH_PRINT,@"「…읏…후우…후우…HPH」","L"
			PRINTFORMDW 빵빵하게 유전자를 모아둔 고무를 도레미에게 보여주니, 그녀는 부끄러운 듯이 눈을 돌렸다
		CASE 2
			PRINTL 
			CALL HPH_PRINT,@"「그렇게나 기분 좋았어…?HPH」","L"
			PRINTFORMDL 아직도 식지 않는 쾌감의 물결에 도레미는 움찔하고 몸을 떨면서 그렇게 물었다
			PRINTFORMDW 너무나 기분 좋았어…그런 말을 꺼내자 그녀는 뭐라고 할 수 없는 표정을 지으며 입을 다물었다
		ENDSELECT
		ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTL 
			PRINTFORML 「…끝났어?」
			PRINTFORMDL 콘돔을 도레미에게 보여주었지만, 그녀는 조금도 동요의 기미를 보이지 않았다
			PRINTFORMDW 하지만 꼬리는 정직해서, 긴장한듯 털이 쭈뼛쭈뼛 곤두서 있었다
		CASE 1
			PRINTL 
			PRINTFORML 「그, 그런거 보여주지 말아줘」
			PRINTFORMDL 도레미는 콘돔에서 얼굴을 돌려 싫은 듯이 하고 있다
			PRINTFORMDW 하지만 흘깃흘깃 눈길을 보내고 있는 것은 뻔했다
		CASE 2
			PRINTL 
			PRINTFORML 「만족했으면…좋았겠지만」
			PRINTFORMDL 도레미는 사무적으로 대응하고 있다
			PRINTFORMDW 다음엔 콘돔 없이…그런 말을 꺼내자 잔뜩 찌푸린 시선으로 노려보았다
		ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

@M_KOJO_MESSAGE_PALAMCNG_A_K109_23(ARG)
CALL PALAM_MESSAGE_A
;-------------------------------------------------
;이라마치오
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
			PRINTFORMW 
