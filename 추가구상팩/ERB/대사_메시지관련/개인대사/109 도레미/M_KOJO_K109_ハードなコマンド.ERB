;==================================================
;150,가슴골을 범한다
;==================================================
@M_KOJO_MESSAGE_COM_K109_150
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K109_150_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K109_150_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;연모
	IF TALENT:연모 || TALENT:애욕 || TALENT:섹프 || TALENT:걸레
		SELECTCASE RAND:3
		CASE 0
			CALL HPH_PRINT,@"「앗HPH 그런、난폭하게…엣HPH」","L"
			PRINTFORMDL %마스터는% 도레미의 가슴을 끌어모으듯이 잡고、허리를 팡팡 부딪쳐왔다
			PRINTFORMDW 동시에 손가락으로 유두를 뭉개듯이 주물러주면、도레미는 난폭하게 가슴을 범해지고 있음에도 불구하고 녹아버린듯한 얼굴을 보이며『사용되어진다』는 행복에 떨었다
		CASE 1
			CALL HPH_PRINT,@"「정말롯 HPH얌전히 있어 주지 않으면 안된다니까요HPH」","L"
			PRINTFORMDL 도레미는 마치 장난꾸러기를 타이르는 것 같은 말투로 봉사를 계속하고 있다
			PRINTFORMDW 버릇을 고쳐주겠다는 듯이 도레미가 가슴을 꽈악 밀어올리면、장대 전체가 유육의 바다에 압박되는 쾌감에 %마스터는% 무심코 신음을 흘렸다
		CASE 2
			CALL HPH_PRINT,@"「응웃HPH 가슴 세게 움켜쥐면 안되요…옷HPH」","L"
			PRINTFORMDL 가슴으로 육봉을 훑어지는 쾌감에 무의식 중에 도레미의 가슴을 꽈악 움켜잡아 버리고 만다
			PRINTFORMDL 도레미는 입으로는 불만이 있는 것처럼 말하지만、뭔가 기쁜 것 같아 보인다…
			CALL HPH_PRINT,@"「북짝북짝 허리를 흔들면서 제 가슴에 쿠퍼액을 문지르고는…어쩔 수 없는 사람이네요오…HPH」","L"
		ENDSELECT
		RETURN 1
	;それ以外
	ELSE
		CALL AUTO_AEGI(NO:TARGET)
		RETURN 1
	ENDIF
ENDIF
RETURN 1
