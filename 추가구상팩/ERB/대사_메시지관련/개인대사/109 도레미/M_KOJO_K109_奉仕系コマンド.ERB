;==================================================
@M_KOJO_MESSAGE_COM_K109_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K109_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K109_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:70
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 「손으로요…？ 잘 할 수 있을지는 모르겠습니다만…」
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		SELECTCASE RAND:3
		CASE 0
			CALL HPH_PRINT,@"「나올 것 같나요？ 참지 않아도 돼요…HPH」","W"
		CASE 1
			CALL HPH_PRINT,@"「자아, 힘 빼시고…HPH」","W"
		CASE 2
			CALL HPH_PRINT,@"「정액 많이 내주세요HPH… 라던지」","W"
		ENDSELECT
	;연모
	ELSEIF TALENT:연모 || TALENT:애욕 || TALENT:섹프 || TALENT:걸레
		SELECTCASE RAND:7
		CASE 0
			CALL HPH_PRINT,@"「이곳은 정직하군요…HPH」","L"
			PRINTFORMDW 경련하는 육봉을 쓰다듬으며, 도레미가 귓가에 속삭였다
		CASE 1
			PRINTFORML 「여기가 약점이군요…？」
			PRINTFORMDW 먼저 손가락을 움직여 귀두를 문지르자, %마스터는% 무심코 한심한 목소리를 흘렸다
		CASE 2
			PRINTFORML 「기분이 좋나요？ 숨이 가빠지고 있어요」
			PRINTFORMDW 이쪽의 얼굴을 보고, 도레미는 짖궂은 미소를 지었다
		CASE 3
			CALL HPH_PRINT,@"「이런 거에 약하시군요…？HPH 후후, 알겠다구요HPH」","L"
			PRINTFORMDL 육봉을 쓰다듬던 도레미는 흔들리는 음낭을 손바닥 위에 올려놓고 구슬을 굴리듯 주물러댔다 
			PRINTFORMDL 능숙한 봉사에 %마스터는% 실없이 헐떡일 수밖에 없다…
		CASE 4
			PRINTFORML 「앗… 끝에서 국물이 흐르기 시작했어요？」
			PRINTFORMDW 도레미가 주는 자극에 국물이 참을 수 없이 나온다
		CASE 5
			CALL HPH_PRINT,@"「봐봐… 열심히 쳐 드릴테니깐, 많이 퓻퓻하고 쏴도 돼요…HPH」","L"
			PRINTFORMDL 도레미는 교묘한 손놀림으로 기분 좋은 곳을 집중적으로 애무해 온다
			PRINTFORMDL 장인의 기술에 %마스터는% 단지 농락당할 뿐이다…
		CASE 6
			CALL HPH_PRINT,@"「이런 거는, 어떠세요？HPH」","L"
			PRINTFORMDL 도레미는 몽혼 같은 걸 귀두에 늘어뜨리고 있었다
			PRINTFORMDW 그대로 찌걱찌걱하며 쓰다듬어졌다. 강한 쾌감에 무심코 허리가 떨렸다…
		ENDSELECT
	;굴복3
	ELSEIF MARK:괘씸각인 == 3
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「후훗…」
			PRINTFORMDW 도레미가 손톱으로 요도를 만지작거려, 무심코 신음하고 말았다
		CASE 1
			PRINTFORML 「앗… 끝에서 국물이 흐르기 시작했어요？」
			PRINTFORMDW 도레미가 주는 자극에 국물이 참을 수 없이 나온다
		CASE 2
			PRINTFORML 「허리가 움직이고 있다구요？」
			PRINTFORMDW 도레미의 호된 솜씨에 절로 허리가 움직였다
		ENDSELECT
	;이외
	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「읏… 이런게 기분 좋은 건가요？」
		CASE 1
			PRINTFORMW 「이 정도 강도면 되나요…？」
		CASE 2
			PRINTFORML 「…조절하는 게, 어렵네요…」
			PRINTFORMDW 도레미는 천천히 쓰다듬고 있다
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

