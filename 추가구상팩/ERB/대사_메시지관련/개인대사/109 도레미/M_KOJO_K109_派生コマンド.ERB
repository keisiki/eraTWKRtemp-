;1,シックスナイン
;==================================================
@M_KOJO_MESSAGE_SCOM_K109_1
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K109_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K109_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORML 「정말로 이 자세로 하시는 건가요…？」
		PRINTFORMDW 도레미는 부끄러운 것처럼 보인다
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		SELECTCASE RAND:3
		CASE 0
			CALL HPH_PRINT,@"「응읏HPH 쥬릇…응큿HPH」","L"
			PRINTFORMDW 도레미의 조갯구멍을 혀로 쑤시면、답례라는 듯이 세차게 빨아올려왔다
		CASE 1
			CALL HPH_PRINT,@"「쥬픗HPH…아、거기、응우웃HPH」","L"
			PRINTFORMDL 도레미의 허리를 세게 끌어안고、조갯구멍을 입에 물고 빨아올렸다
			PRINTFORMDW 갑작스런 강렬한 자극에、도레미는 육봉을 입에서 떼고 헐떡였다
		CASE 2
			CALL HPH_PRINT,@"「훗HPH읏HPH즈루룻HPH」","L"
			PRINTFORMDL 도레미의 격렬한 공격으로 무의식 중에 봉사하는 것도 잊어버리고 말았다
			PRINTFORMDW 허리를 조금씩 흔드는 %CALLNAME:MASTER%의 모습에 기분이 좋아진 도레미는 더욱 격렬하게 몰아세워 갔다…
		ENDSELECT
	;연모
	ELSEIF TALENT:연모 || TALENT:애욕 || TALENT:섹프 || TALENT:걸레
		SELECTCASE RAND:6
		CASE 0
			CALL HPH_PRINT,@"「흠칫흠칫 튀어오르고 있어요…츄릇HPH…읏HPH」","L"
			PRINTFORMDW 도레미의 봉사에 지지 않으려고 조갯구멍을 정성껏 핥아올렸다
		CASE 1
			PRINTFORMDL 도레미에게 육봉을 핥게 하면서、조갯구멍에 혀를 기게 했다
			CALL HPH_PRINT,@"「읏HPH…할짜악HPH…쥬릇HPH」","L"
			PRINTFORMDW 쾌감으로 떨면서도、도레미의 봉사는 멈추지 않았다
		CASE 2
			PRINTFORML 도레미와 %마스터는% 서로 탐하듯이 서로의 성기를 핥고 있다
			PRINTFORMW 육봉에 봉사해가면서、클리토리스를 핥아 올릴 때마다 움찔거리며 떠는 도레미의 반응을 즐겼다
		CASE 3
			CALL HPH_PRINT,@"「응、츗…웅읏…HPH」","L"
			PRINTFORMDW 귀두를 핥고 있는 도레미는 %CALLNAME:MASTER%의 공격으로 허리를 떨고 있다
		CASE 4
			PRINTFORMDL 애액을 맛보면서、살짝 허리를 들어 육봉을 도레미의 입안으로 밀어넣었다
			CALL HPH_PRINT,@"「후긋…으응…응긋HPH」","L"
			PRINTFORMDW 도레미는 조금 수줍어하면서도 육봉에 봉사를 시작했다
		CASE 5
			CALL HPH_PRINT,@"「이런、느낌으로 좋으신가요…？쥬릇HPH」","L"
			PRINTFORMDW 도레미의 헌신적인 봉사에 보답하기 위해、부드럽게 조갯구멍을 핥았다
		ENDSELECT
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		SELECTCASE RAND:3
		CASE 0
			CALL HPH_PRINT,@"「응、츗…웅읏…HPH」","L"
			PRINTFORMDW 귀두를 핥고 있는 도레미는 %CALLNAME:MASTER%의 공격으로 허리를 떨고 있다
		CASE 1
			PRINTFORMDL 애액을 맛보면서、살짝 허리를 들어 육봉을 도레미의 입안으로 밀어넣었다
			CALL HPH_PRINT,@"「후긋…으응…응긋HPH」","L"
			PRINTFORMDW 도레미는 조금 수줍어하면서도 육봉에 봉사를 시작했다
		CASE 2
			CALL HPH_PRINT,@"「이런、느낌으로 좋으신가요…？쥬릇HPH」","L"
			PRINTFORMDW 도레미의 헌신적인 봉사에 보답하기 위해、부드럽게 조갯구멍을 핥았다
		ENDSELECT
	;それ以外
	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「이 자세、부끄럽네요…할짝…」
			PRINTFORMDW 도레미는 아직은 어색하다는 느낌으로 육봉을 핥고 있다
		CASE 1
			PRINTFORML 「응읏…잠ㄲ、격렬해…」
			PRINTFORMDW %CALLNAME:MASTER%의 공격에 도레미는 핥는 것조차도 원하는 대로 되지 않는다
		CASE 2
			PRINTFORML 「후우、으응…츄팟…앗…」
			PRINTFORMDW 도레미가 %CALLNAME:MASTER%에게 걸터앉아、서로의 성기를 서로 핥았다
		ENDSELECT
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K109_{SELECTCOM}_1
RETURN 1

