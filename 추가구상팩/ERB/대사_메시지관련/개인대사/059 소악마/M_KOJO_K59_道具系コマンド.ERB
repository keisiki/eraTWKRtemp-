;40,ローター
;==================================================
@M_KOJO_MESSAGE_COM_K59_40
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K59_40_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K59_40_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;굴복각인 Lv3+연모
        IF MARK:괘씸각인 == 3 && TALENT:연모 == 1
            PRINTFORMW 「아앗, 너무 느껴버려요!」
        ;그 외
        ELSE
            PRINTFORMW 「히야앗! 싫어, 부들부들해서, 아앗!」
        ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		;PRINTFORMW 
		RETURN 1
	;연모 + 괘씸각인 LV 3
	ELSEIF TALENT:연모 == 1 && MARK:괘씸각인 == 3
		PRINTFORMW 「아앙 %CALLNAME:MASTER%! 기분 좋아요!」
		PRINTFORMW 「크리가 움찔움찔해서… 머릿 속이 녹아버려요오!!」
		PRINTFORML %조사처리(CALLNAME:MASTER,"가")% 음핵에 로터를 갖다대자, 소악마는 허리를 띄우며 로터를 눌러, 
		PRINTFORMW 스스로 강한 쾌락을 얻으려 음란하게 움직였다
		RETURN 1
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 「찌릿찌릿해서, 머리까지 기분 좋아져버려!」
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 「그만, 그만, 아앙!」
		RETURN 1
	ENDIF
ENDIF
RETURN 1

