;==================================================
;44,バイブ
;==================================================
@M_KOJO_MESSAGE_COM_K59_44
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K59_44_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K59_44_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TCVAR:파과 == 1
			;연모＋순종A
            IF TALENT:연모 == 1 && ABL:순종 >= 20
                PRINTFORMW 「…%CALLNAME:MASTER%, 제게 막이 있다는 거 알고 있어요?」
            ;그 외
            ELSE
                PRINTFORMW 「싫엇, 그만둬주세요!」
            ENDIF
			RETURN 1
		ELSE
			;연모
			IF TALENT:연모
                PRINTFORML 「이거 넣는거예요?」
                PRINTFORMW 「괜찮습니다만, %CALLNAME:MASTER%의 자지쪽을 원하는데, 같은…」
            ;그 외
            ELSE
                PRINTFORMW 「에 또, 혹시나 하는데 그거, 넣는거예요……?」
            ENDIF
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	;装着
	IF TEQUIP:바이브
		;時姦中
		IF FLAG:70
			PRINTFORMW 
			RETURN 1
		;恋慕
		ELSEIF TALENT:연모
			PRINTFORML 「안에서 빙글빙글 움직여서 기분 좋네요」
            PRINTFORMW 「하지만, 당신의 자지가 가장 기분 좋아요, 앙♪」
            PRINTFORMW 소악마는 뜨거운 눈을 %CALLNAME:MASTER%에게 향하는 것도, 밀려드는 쾌감에 밀러, 허리를 떨고 있었다
			RETURN 1
		;V 감각 C 이상
		ELSEIF ABL:1 >= 10
			PRINTFORMW 「네, 넣는 거네요」
            PRINTFORMW 「아핫, 기분 좋아……응앗!」
			RETURN 1
		;それ以外
		ELSE
			PRINTFORMW 「앗, 안에서 움직여서…응응읏!」
			RETURN 1
		ENDIF
	;解除
	ELSE
		;연모
		IF TALENT:연모
			PRINTFORML 「앙, 뽑아버리는 거네요」
			PRINTFORMW 「그럼 다음은…… 당신의, 차례네요?」
			PRINTFORMW 거품이 일어, 백탁한 애액투성이가 된 음부를 숨기려 하지도 않고, 소악마는 음탕하게 미소를 지었다.
		;그 외
		ELSE
			PRINTFORML 「앗, 응읏……자, 잠깐만 기다려주세요…」
			PRINTFORMW %조사처리(CALLNAME:MASTER,"는")% 소악마가 숨을 가다듬을 때까지 기다려 주었다.
			PRINTFORMW 「이상한 곳에서 상냥하시네요……」
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

