
;FileName_TRACHECK.ERB ----------------------------- Rev1. 00
;V조교의 고통, 반발 보정(피스트 등 격렬한 것은 별처리)
;CALL		USER
;ARG		0:캐릭터 NO
;RETURN		VOID
;COMMENT	
;-----------------------------------------------------------
@PAIN_CHECK_V(ARG)
;EXP:V경험을 본다(처녀의 반감은 별도 처리필 모두의로 생략)
IF 삽입불가(ARG) == 1
	TIMES SOURCE:ARG:고통 , 1.50
	TIMES SOURCE:ARG:반감 , 1.20
ELSEIF 삽입불가(ARG) == 2
	TIMES SOURCE:ARG:고통 , 2.50
	TIMES SOURCE:ARG:반감 , 2.00
ENDIF
	
IF EXP:ARG:V경험 < EXPLV:1
	TIMES SOURCE:ARG:고통 , 2.50
	TIMES SOURCE:ARG:반감 , 1.50
ELSEIF EXP:ARG:V경험 < EXPLV:2
	TIMES SOURCE:ARG:고통 , 1.50
	TIMES SOURCE:ARG:반감 , 1.20
ELSEIF EXP:ARG:V경험 < EXPLV:3
	TIMES SOURCE:ARG:고통 , 1.00
ELSEIF EXP:ARG:V경험 < EXPLV:4
	TIMES SOURCE:ARG:고통 , 0.50
ELSEIF EXP:ARG:V경험 < EXPLV:5
	TIMES SOURCE:ARG:고통 , 0.30
ELSEIF EXP:ARG:V경험 >= EXPLV:5
	TIMES SOURCE:ARG:고통 , 0.10
ENDIF

;PALAM:윤활을 본다
IF PALAM:ARG:윤활 < PALAMLV:1
	TIMES SOURCE:ARG:고통 , 1.20
	TIMES SOURCE:ARG:반감 , 1.50
ELSEIF PALAM:ARG:윤활 < PALAMLV:2
	TIMES SOURCE:ARG:고통 , 0.70
	TIMES SOURCE:ARG:반감 , 1.20
ELSEIF PALAM:ARG:윤활 < PALAMLV:3
	TIMES SOURCE:ARG:고통 , 0.40
ELSEIF PALAM:ARG:윤활 < PALAMLV:4
	TIMES SOURCE:ARG:고통 , 0.10
ELSEIF PALAM:ARG:윤활 >= PALAMLV:4
	TIMES SOURCE:ARG:고통 , 0.10
ENDIF

;FileName_TRACHECK.ERB ----------------------------- Rev1. 00
;애널 조교의 고통, 반발 보정
;CALL		USER
;ARG		0:캐릭터 NO
;RETURN		VOID
;COMMENT	
;-----------------------------------------------------------
@PAIN_CHECK_A(ARG)
IF 삽입불가(ARG) == 1
	TIMES SOURCE:ARG:고통 , 1.50
	TIMES SOURCE:ARG:반감 , 1.20
ELSEIF 삽입불가(ARG) == 2
	TIMES SOURCE:ARG:고통 , 2.50
	TIMES SOURCE:ARG:반감 , 2.00
ENDIF
;EXP:A경험을 본다
IF EXP:ARG:A경험 < EXPLV:1
	TIMES SOURCE:ARG:고통 , 3.00
	TIMES SOURCE:ARG:반감 , 3.00
ELSEIF EXP:ARG:A경험 < EXPLV:2
	TIMES SOURCE:ARG:고통 , 2.00
	TIMES SOURCE:ARG:반감 , 2.00
ELSEIF EXP:ARG:A경험 < EXPLV:3
	TIMES SOURCE:ARG:고통 , 1.00
	TIMES SOURCE:ARG:반감 , 1.50
ELSEIF EXP:ARG:A경험 < EXPLV:4
	TIMES SOURCE:ARG:고통 , 0.60
	TIMES SOURCE:ARG:반감 , 1.20
ELSEIF EXP:ARG:A경험 < EXPLV:5
	TIMES SOURCE:ARG:고통 , 0.40
ELSEIF EXP:ARG:A경험 >= EXPLV:5
	TIMES SOURCE:ARG:고통 , 0.20
ENDIF

;PALAM:윤활을 본다
IF PALAM:ARG:윤활 < PALAMLV:1
	TIMES SOURCE:ARG:고통 , 1.20
	TIMES SOURCE:ARG:반감 , 2.00
ELSEIF PALAM:ARG:윤활 < PALAMLV:2
	TIMES SOURCE:ARG:고통 , 1.00
	TIMES SOURCE:ARG:반감 , 1.50
ELSEIF PALAM:ARG:윤활 < PALAMLV:3
	TIMES SOURCE:ARG:고통 , 0.60
ELSEIF PALAM:ARG:윤활 < PALAMLV:4
	TIMES SOURCE:ARG:고통 , 0.30
ELSEIF PALAM:ARG:윤활 >= PALAMLV:4
	TIMES SOURCE:ARG:고통 , 0.20
ENDIF