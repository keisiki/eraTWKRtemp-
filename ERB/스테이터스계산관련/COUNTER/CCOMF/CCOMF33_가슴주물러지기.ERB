;*********************************************************************************************
;カウンターコマンド：胸を揉ませる
;ARG = 対象のキャラ番号
;*********************************************************************************************
@CCOMF33(ARG)

	;몸의자세の変更
	;=============================
	CALL TOUCH_SET(SET_FROM_B,3,ARG)
	TFLAG:MASTER의손사용중 ++

	;ソース計算および内部処理系統
	;=============================
	SOURCE:ARG:유혹 += 350
	SOURCE:ARG:도발 += 200
	SOURCE:ARG:강요 += 200
	SOURCE:ARG:쾌B += 50

	;マスターカウンター制御
	;(ビット立ちで不可 0=엉덩이를 쓰다듬는다 1=껴안는다 2=키스한다 3=가슴애무 4=애널애무 5=클리애무 6=指挿入れ 7=치마를 넘긴다)
	;=============================
	SIF GROUPMATCH(TEQUIP:MASTER:체위, 1, 3, 4, 5)
		CALL 카운터삽입됨지속처리(ARG)

RETURN 1

;*********************************************************************************************
;カウンター実行可否出力関数
;ARG = 対象のキャラ番号
;*********************************************************************************************
@CCOM_ABLE33(ARG)

	SIF TFLAG:61
		RETURN 0
	SIF TEQUIP:MASTER:체위 == 2
		RETURN 0

RETURN 1

;*********************************************************************************************
;カウンター実行判定値算出関数
;ARG = 対象のキャラ番号
;*********************************************************************************************
@CCOM_ORDER33(커맨드번호, ARG, 기본치)
#DIM 커맨드번호
#DIM 기본치
#DIMS 카운터타입

	;카운터타입設定
	;=============================
	카운터타입 = 착의

	;実行判定値算出式
	;=============================
	LOCAL = 기본치 + CFLAG:ARG:유혹 - CFLAG:ARG:적극성

	;카운터타입別計算処理 実質実行判定処理を含む
	;=============================
	LOCAL = MAX(LOCAL + COUNTER_SIKOU:ARG:커맨드번호, 1)
	LOCAL *= COUNTER_TYPE(카운터타입, ARG)

RETURN LOCAL
