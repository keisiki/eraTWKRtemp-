IF TALENT:(ARG:2):일선을넘지않음
	IF !TALENT:(ARG:2):연모
		TIMES LOCAL:11 , 2.00
		TIMES LOCAL:12 , 2.00
	ELSE
		TIMES LOCAL:11 , 0.40
		TIMES LOCAL:12 , 0.40
	ENDIF
ENDIF
;A감도
IF TALENT:(ARG:2):A감도 > 0
	TIMES LOCAL:11 , 0.50
	TIMES LOCAL:12 , 0.50
ENDIF
RETURNF LOCAL:(ARG:1)
