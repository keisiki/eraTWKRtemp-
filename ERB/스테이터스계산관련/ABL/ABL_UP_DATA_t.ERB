;C감각
;≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡
;--------------------------------------------------
;ARG 패턴
;JUEL:(ARG:1)가 필요하면 LOCAL:(ARG:1)에 대입해 그 값을 돌려준다
;내친 김에 패턴의 인식이나 필요한 ABL등의 처리
;--------------------------------------------------
@JUEL_DEMAND_0(ARG, ARG:1, ARG:2)
#FUNCTION
SIF ARG > 0
	RETURNF -1
VARSET LOCAL
SELECTCASE ABL:(ARG:2):C감각
	CASE 0
		LOCAL:0 = 1
	CASE 1
		LOCAL:0 = 20
	CASE 2
		LOCAL:0 = 400
	CASE 3
		LOCAL:0 = 5000
	CASE 4
		LOCAL:0 = 15000
	CASE 5
		LOCAL:0 = 30000
	CASE 6
		LOCAL:0 = 40000
	CASE 7
		LOCAL:0 = 50000
	CASE 8
		LOCAL:0 = 60000
	CASE 9
		LOCAL:0 = 70000
	CASEELSE
		LOCAL:0 = 20000 * (ABL:(ARG:2):C감각 - 6)
ENDSELECT
;경험 보정
LOCAL:0 = LOCAL:0 * 10 / (5 + GETEXPLV(EXP:(ARG:2):C경험, 8))
LOCAL:0 = LOCAL:0 * (1 + ABL:(ARG:2):C감각) / (1 + GETEXPLV(EXP:(ARG:2):C절정경험, 8))
;일선을넘지않음
IF TALENT:(ARG:2):일선을넘지않음
	IF !TALENT:(ARG:2):연모
		TIMES LOCAL:0 , 2.00
	ELSE
		TIMES LOCAL:0 , 0.50
	ENDIF
ENDIF

SIF ARG:2 == MASTER
	LOCAL:0 *= (ABL:(ARG:2):C감각 + 1)

RETURNF LOCAL:(ARG:1)

