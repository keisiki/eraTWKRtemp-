;EXP:M경험을 본다
IF EXP:PLAYER:M경험 < EXPLV:1
	TIMES SOURCE:ARG:여쾌M , 0.30
ELSEIF EXP:PLAYER:M경험 < EXPLV:2
	TIMES SOURCE:ARG:여쾌M , 0.50
ELSEIF EXP:PLAYER:M경험 < EXPLV:3
	TIMES SOURCE:ARG:여쾌M , 0.80
ELSEIF EXP:PLAYER:M경험 < EXPLV:4
	TIMES SOURCE:ARG:여쾌M , 1.00
ELSEIF EXP:PLAYER:M경험 < EXPLV:5
	TIMES SOURCE:ARG:여쾌M , 1.20
ELSEIF EXP:PLAYER:M경험 >= EXPLV:5
	TIMES SOURCE:ARG:여쾌M , 1.50
ENDIF


LOCAL = SOURCE:ARG:여쾌M
LOCAL:1 = SOURCE:ARG:여쾌M

;PALAM:욕정을 본다
IF PALAM:PLAYER:욕정 < PALAMLV:1
	TIMES LOCAL , 0.60
ELSEIF PALAM:PLAYER:욕정 < PALAMLV:3
	TIMES LOCAL , 0.80
ELSEIF PALAM:PLAYER:욕정 < PALAMLV:5
	TIMES LOCAL , 1.00
ELSEIF PALAM:PLAYER:욕정 < PALAMLV:7
	TIMES LOCAL , 1.20
ELSEIF PALAM:PLAYER:욕정 >= PALAMLV:7
	TIMES LOCAL , 1.50
ENDIF

;ABL:욕망을 본다
LOCAL:1 = LOCAL:1 * SOURCE_REVISION2(ABL:PLAYER:욕망) / 100

;난쟁이 체형
IF TALENT:PLAYER:체형 < -2
	TIMES LOCAL , 0.80
	TIMES LOCAL:1 , 1.20
ENDIF
TCVAR:MASTER:4 += GET_REVISION2(LOCAL, 500, 20000)
CUP:PLAYER:쾌M += LOCAL
CUP:PLAYER:욕정 += LOCAL:1
;FileName_SOURCE1.ERB ------------------------------ Rev1.00
;그 외계 소스 처리
;CALL		USER
;ARG		캐릭터 NO
;RETURN		VOID
;COMMENT	
;-----------------------------------------------------------
@SOURCE_CVAB_EXTRA(ARG)
VARSET LOCAL
;-------------------------------------------------
;기력 0에 의한 상하의 처리(쾌락계)
;-------------------------------------------------
;IF BASE:ARG:기력 <= 0
;	;UP:0, 1,2,3은 절정에 관련되므로 먼저 처리되어 있다
;	CUP:ARG:쾌C /= 2
;	CUP:ARG:쾌V /= 2
;	CUP:ARG:쾌A /= 2
;	CUP:ARG:쾌B /= 2
;	CUP:ARG:쾌M /= 2
;ENDIF
;-------------------------------------------------
;궁합에 의한 상하의 처리(쾌락계)
;-------------------------------------------------
IF RELATION:ARG:R != 0
	CUP:ARG:쾌C *= RELATION:ARG:R
	CUP:ARG:쾌C /= 100
	CUP:ARG:쾌V *= RELATION:ARG:R
	CUP:ARG:쾌V /= 100
	CUP:ARG:쾌A *= RELATION:ARG:R
	CUP:ARG:쾌A /= 100
	CUP:ARG:쾌B *= RELATION:ARG:R
	CUP:ARG:쾌B /= 100
	CUP:ARG:쾌M *= RELATION:ARG:R
	CUP:ARG:쾌M /= 100
ENDIF
;-------------------------------------------------
;쾌감에 부정은, 쾌소스의 수치에 응해, 억울증가(음란해 무효)
;-------------------------------------------------
SIF TALENT:ARG:쾌감응답 < 0 && !TALENT:ARG:음란
	SOURCE:ARG:울굴 += (CUP:ARG:쾌C + CUP:ARG:쾌V + CUP:ARG:쾌A + CUP:ARG:쾌B)/10

;-------------------------------------------------
;소질등에 의한 상하의 처리(쾌락계)
;-------------------------------------------------
;미약
IF TCVAR:ARG:춘약 || TCVAR:ARG:발정
	TIMES CUP:ARG:쾌C , 1.30
	TIMES CUP:ARG:쾌V , 1.30
	TIMES CUP:ARG:쾌A , 1.30
	TIMES CUP:ARG:쾌B , 1.30
	TIMES CUP:ARG:쾌M , 1.30
ENDIF
	;이뇨제
IF TCVAR:ARG:이뇨제
	TIMES CUP:ARG:쾌C , 0.70
	TIMES CUP:ARG:쾌V , 0.70
	TIMES CUP:ARG:쾌A , 0.70
	TIMES CUP:ARG:쾌B , 0.70
	TIMES CUP:ARG:쾌M , 0.70
ENDIF
;仙香玉兎、ガシングガーデン
IF IsInsenceValid(ARG)
	TIMES CUP:ARG:쾌C , 1.30
	TIMES CUP:ARG:쾌V , 1.30
	TIMES CUP:ARG:쾌A , 1.30
	TIMES CUP:ARG:쾌B , 1.30
	TIMES CUP:ARG:쾌M , 1.30
ELSEIF IsGardenValid(ARG)
	TIMES CUP:ARG:쾌C , 1.20
	TIMES CUP:ARG:쾌V , 1.20
	TIMES CUP:ARG:쾌A , 1.20
	TIMES CUP:ARG:쾌B , 1.20
	TIMES CUP:ARG:쾌M , 1.20
ENDIF

;-------------------------------------------------
;수면중의 상하의 처리(쾌락계)
;-------------------------------------------------
	;수면
IF CFLAG:ARG:수면
	TIMES CUP:ARG:쾌C , 0.50
	TIMES CUP:ARG:쾌V , 0.50
	TIMES CUP:ARG:쾌A , 0.50
	TIMES CUP:ARG:쾌B , 0.50
	TIMES CUP:ARG:쾌M , 0.50
ENDIF
;-------------------------------------------------
;함락 소질
;-------------------------------------------------
;음란
IF TALENT:ARG:음란
	TIMES CUP:ARG:쾌C , 1.20
	TIMES CUP:ARG:쾌V , 1.20
	TIMES CUP:ARG:쾌A , 1.20
	TIMES CUP:ARG:쾌B , 1.20
	TIMES CUP:ARG:쾌M , 1.10
ENDIF
;-------------------------------------------------
;소질
;-------------------------------------------------
;자제심
IF TALENT:ARG:자제심
	TIMES CUP:ARG:쾌C , 0.70
	TIMES CUP:ARG:쾌V , 0.90
	TIMES CUP:ARG:쾌A , 1.00
	TIMES CUP:ARG:쾌B , 0.70
	TIMES CUP:ARG:쾌M , 1.10
ENDIF
	;즉각함락
IF TALENT:ARG:즉각함락
	TIMES CUP:ARG:쾌C , 2.00
	TIMES CUP:ARG:쾌V , 2.00
	TIMES CUP:ARG:쾌A , 2.00
	TIMES CUP:ARG:쾌B , 2.00
	TIMES CUP:ARG:쾌M , 1.80
ENDIF
;자위중독
SIF TALENT:ARG:자위중독
	TIMES CUP:ARG:쾌C , 1.20
;음호
SIF TALENT:ARG:음호
	TIMES CUP:ARG:쾌V , 1.20
;항문광
SIF TALENT:ARG:항문광
	TIMES CUP:ARG:쾌A , 1.20
;음유
SIF TALENT:ARG:음유
	TIMES CUP:ARG:쾌B , 1.20
	;남자
IF !(TALENT:ARG:2 & 1)
	TIMES CUP:ARG:쾌A , 1.30
SIF TALENT:ARG:키스마
	TIMES CUP:ARG:쾌M , 1.30
ENDIF
;-------------------------------------------------
;커맨드
;-------------------------------------------------
;커맨드 보정
CUP:ARG:쾌C = CUP:ARG:쾌C * (100 + TCVAR:ARG:커맨드C보정) / 100
CUP:ARG:쾌V = CUP:ARG:쾌V * (100 + TCVAR:ARG:커맨드V보정) / 100
CUP:ARG:쾌A = CUP:ARG:쾌A * (100 + TCVAR:ARG:커맨드A보정) / 100
CUP:ARG:쾌B = CUP:ARG:쾌B * (100 + TCVAR:ARG:커맨드B보정) / 100
CUP:ARG:쾌M = CUP:ARG:쾌M * (100 + TCVAR:ARG:커맨드M보정) / 100

;-------------------------------------------------
;난이도에 의한 상하의 처리(쾌락계)
;-------------------------------------------------
CUP:ARG:쾌C = CUP:ARG:쾌C * (16 - 4 * FLAG:4) / 10
CUP:ARG:쾌V = CUP:ARG:쾌V * (16 - 4 * FLAG:4) / 10
CUP:ARG:쾌A = CUP:ARG:쾌A * (16 - 4 * FLAG:4) / 10
CUP:ARG:쾌B = CUP:ARG:쾌B * (16 - 4 * FLAG:4) / 10
CUP:ARG:쾌M = CUP:ARG:쾌M * (16 - 4 * FLAG:4) / 10
;-------------------------------------------------
;애액 처리
;-------------------------------------------------
LOCAL:1 = CUP:ARG:쾌C + CUP:ARG:쾌V + CUP:ARG:쾌A + CUP:ARG:쾌B + CUP:ARG:쾌M
;쾌C＋쾌V＋쾌A＋쾌B가 합해 100이상 상승이라면 그20%의 액체의 소스가 더해진다(남자는 안 돼)
IF LOCAL:1 > 100
	;젖기 쉬우면 젖기 어려운은 여기서 처리
	LOCAL:1 = LOCAL:1 * (10 + 5 * TALENT:ARG:젖기쉬움) / 10
	SOURCE:ARG:액체 += LOCAL:1 / 5
ENDIF