
;-------------------------------------------------
;바이브 등 붙이고 있을 뿐 아이템의 체크
;-------------------------------------------------
@EQUIP_CHECK(ARG)
;클리캡 장착중
SIF TEQUIP:ARG:클리캡
	CALLFORM EQUIP_COM{COM클리캡}(ARG)

;오나홀 장착중
SIF TEQUIP:ARG:오나홀
	CALLFORM EQUIP_COM{COM오나홀}(ARG)

;바이브 장착중
SIF TEQUIP:ARG:바이브
	CALLFORM EQUIP_COM{COM바이브}(ARG)

;애널바이브 장착중
SIF TEQUIP:ARG:애널바이브
	CALLFORM EQUIP_COM{COM애널바이브}(ARG)

;애널비즈 사용중
SIF TEQUIP:ARG:애널비즈
	CALLFORM EQUIP_COM{COM애널비즈}(ARG)

;유두캡 장착중
SIF TEQUIP:ARG:유두캡
	CALLFORM EQUIP_COM{COM유두캡}(ARG)

;착유기 장착중
SIF TEQUIP:ARG:착유기
	CALLFORM EQUIP_COM{COM착유기}(ARG)

;눈가리개 장착중
SIF TEQUIP:ARG:눈가리개
	CALLFORM EQUIP_COM{COM눈가리개}(ARG)

;밧줄로 긴박중
SIF TEQUIP:ARG:밧줄
	CALLFORM EQUIP_COM{COM밧줄}(ARG)

;볼개그 장착중
SIF TEQUIP:ARG:볼개그
	CALLFORM EQUIP_COM{COM볼개그}(ARG)

;관장＋애널 플러그 장착중
SIF TEQUIP:ARG:관장
	CALLFORM EQUIP_COM{COM관장}(ARG)

;확장벌룬 장착중
SIF TEQUIP:ARG:확장벌룬
	CALLFORM EQUIP_COM{COM확장벌룬}(ARG)

;애널전극 장착중
SIF TEQUIP:ARG:애널전극
	CALLFORM EQUIP_COM{COM애널전극}(ARG)

;비디오촬영중
SIF ARG > 0 && TEQUIP:MASTER:비디오촬영
	CALLFORM EQUIP_COM{COM비디오촬영}(ARG)

;수치플레이중
;SIF TEQUIP:ARG:30
;	CALL EQUIP_COM186(ARG)

;목욕탕플레이중
SIF TEQUIP:ARG:목욕탕플레이
	CALLFORM EQUIP_COM{COM목욕탕플레이}(ARG)

;샤워 사용중
SIF TEQUIP:ARG:샤워
	CALLFORM EQUIP_COM{COM샤워}(ARG)

;새댁플레이중
SIF TEQUIP:ARG:새댁플레이
	CALLFORM EQUIP_COM{COM새댁플레이}(ARG)

;V삽입중
SIF TEQUIP:ARG:50 >= 0 && !SOURCE:ARG:쾌V
	CALL EQUIP_INCERT_V(ARG)

;A삽입중
SIF TEQUIP:ARG:51 >= 0 && !SOURCE:ARG:쾌A
	CALL EQUIP_INCERT_A(ARG)

;한우산을씀중
SIF TEQUIP:ARG:한우산을씀 && OUTROOF(CFLAG:ARG:현재위치) && ARG == TARGET
	CALLFORM EQUIP_COM{COM한우산을씀}(ARG)
RETURN 1
