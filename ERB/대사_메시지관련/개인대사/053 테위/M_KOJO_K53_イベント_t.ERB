;0그리고 V, 1으로 A
#DIM 주입량
#DIM 회수

SELECTCASE 전희1
	;감각 C>=B
	CASE 1
		PRINTFORML C자극
	;감각 B>C
	CASE 2
		PRINTFORML B자극
	;키스미경험 or확률
	CASE 3
		PRINTFORML M자극
		IF TALENT:53:키스미경험
			PRINTFORM 첫키스를 빼앗아
		ELSE
			PRINTFORM 입맞춤
		ENDIF
ENDSELECT


