				ENDIF
			ENDIF
		;이라마치오
		CASE 8
			IF K53_PVOICE_FRAG:8
				SELECTCASE RAND:9
					CASE 0
						LOCALS += ""
					CASE 1
						LOCALS += "우훅"
					CASE 2
						LOCALS += ""
