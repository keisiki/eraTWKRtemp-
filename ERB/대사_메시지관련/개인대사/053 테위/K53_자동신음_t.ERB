										LOCALS += "그만두어"
									CASE 2
										LOCALS += "이제(벌써) 그만두어"
								ENDSELECT
							ELSEIF K53_자극LV("반감") == 1 || K53_자극LV("공포") == 1
								SELECTCASE RAND:3
									CASE 0
										LOCALS += "있고, 아니…"
									CASE 1
										LOCALS += "이제(벌써) 싫어…"
									CASE 2
										LOCALS += "고례나…"
								ENDSELECT
							ENDIF
						K53_대사사용가능 = 0
					ENDSELECT
					
				ENDIF
			ENDIF
