						CASE 5
							LOCALS += ""
						CASE 6
							LOCALS += "하"
						CASE 7
							LOCALS += ""
						CASE 8
							LOCALS += ""	
					ENDSELECT
					SELECTCASE RAND:3
						CASE 0
							LOCALS += "…"
						CASE 1
							LOCALS += "……"
						CASE 2
							LOCALS += K53_하트부여()
					ENDSELECT
				ENDIF
			ENDIF
		;한숨
		CASE 7
			IF K53_PVOICE_FRAG:7
				IF GETPALAMLV(PALAM:욕정, 15) >= 4
					SELECTCASE RAND:9
						CASE 0
							LOCALS += "응"
						CASE 1
							LOCALS += "응"
						CASE 2
							LOCALS += "으응"
						CASE 3
							LOCALS += "후훗"
						CASE 4
