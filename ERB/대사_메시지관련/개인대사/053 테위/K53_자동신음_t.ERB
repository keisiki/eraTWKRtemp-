						CASE 0
							LOCALS += K53_하트부여()
						CASE 1
							LOCALS += "~~"
						CASEELSE
							LOCALS += ""
					ENDSELECT
				ELSEIF ABL:TARGET:혀 >= 3
					SELECTCASE RAND:9
						CASE 0
							LOCALS += "우훅"
						CASE 1
							LOCALS += "쮸웁"
						CASE 2
							LOCALS += ""
						CASE 3
							LOCALS += "쪽"
						CASE 4
							LOCALS += ""
						CASE 5
							LOCALS += ""
						CASE 6
							LOCALS += "쮸웁"
						CASE 7
