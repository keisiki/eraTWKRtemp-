						CASE 8
							LOCALS += "하앗"
					ENDSELECT
					SELECTCASE RAND:4
						CASE 0
							LOCALS += ","
						CASE 1
							LOCALS += "…"
						CASEELSE
							LOCALS += K53_하트부여()
					ENDSELECT
				ELSE
					SELECTCASE RAND:9
						CASE 0
							LOCALS += "응"
						CASE 1
							LOCALS += "응"
						CASE 2
							LOCALS += "으응"
						CASE 3
