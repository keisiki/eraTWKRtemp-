;82,パイズリ
;==================================================
@M_KOJO_MESSAGE_COM_K83_82
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K83_82_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K83_82_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K83_UFUFU(83)
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:시간정지
		PRINTFORML 「, 가슴……」
		PRINTFORMW 「%CALLNAME:MASTER%……빗대고？」
		RETURN 1
	ENDIF
	IF FLAG:시간정지
	;イきそう
	ELSEIF NOWEX:MASTER:사정
		PRINTFORML 「후후,%CALLNAME:MASTER%, 이 올 것 같애……」
		CALL HPH_PRINT(@"「すごく気持ちよさそう……嬉しいな……HPH」", "W")
	ELSE
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMW 「……, 어렵다……」
		CASE 2
			PRINTFORMW 「후~…후~……뜨겁다……」
		CASEELSE
			IF TALENT:가슴사이즈 < 0
				PRINTFORMW 「……좀 더 귀댁도 참 좋았는데……미안해요……」
			ELSE
				PRINTFORMW 「어떨까……나, 잘 할 수 있고 있다？」
			ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

