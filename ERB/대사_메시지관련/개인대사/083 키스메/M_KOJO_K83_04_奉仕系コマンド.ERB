;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:시간정지
		PRINTFORMW 「, 가랑이……. 좋아……해 본다……」
		RETURN 1
	ENDIF
	IF FLAG:시간정지
	;イきそう
	ELSEIF NOWEX:MASTER:사정
		PRINTFORML 「후~…후~……%CALLNAME:MASTER%, 이크의？」
		CALL HPH_PRINT(@"「じゃあこのままイっちゃおうか……HPH」", "W")
	ELSE
		SELECTCASE RAND:4
		CASE 1
			PRINTFORMW 「후~…후~……아, 이것……기분 좋다……」
		CASE 2
			PRINTFORML 「%CALLNAME:MASTER%…………？」
			PRINTFORMW 「 나도……응, 와, 기분 좋아……」
		CASE 3
			PRINTFORMW 「만난다……, 후~, 미끈미끈 하고 있어……좋다……」
		CASEELSE
			PRINTFORMW 「응…아, 응……. (은)는, 들어가버릴 것 같다……」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
