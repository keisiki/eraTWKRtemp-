;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K83_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K83_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K83_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K83_UFUFU(83)
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:시간정지
		;未成熟,粗チン
		IF TALENT:MASTER:형상 == 1 || TALENT:MASTER:형상 == 2
			PRINTFORMW 「잡아 비비어……？」
		ELSE
			PRINTFORMW 「, 귀의……. 잡아 비비어……？」
		ENDIF
		PRINTFORMW 「개, 이러한가……？ 어때……？」
		RETURN 1
	ENDIF
	IF FLAG:시간정지
	;イきそう
	ELSEIF NOWEX:MASTER:사정
		SELECTCASE RAND:2
		CASE 1
			PRINTFORML 「대단히 부풀어 오르고 있다……이 올 것 같애……」
			CALL HPH_PRINT(@"「いいよ……いっぱいぴゅっぴゅして……HPH」", "W")
		CASEELSE
			PRINTFORML 「이 기초？ 자, 좀 더 빨리 하는군……」
			CALL HPH_PRINT(@"「いいよ……いっぱいぴゅっぴゅして……HPH」", "W")
		ENDSELECT
	ELSE
		SELECTCASE RAND:5
		CASE 1
			PRINTFORML 「아무쪼록인……？」
			PRINTFORMW 「가득 기분 좋아져……」
		CASE 2
			PRINTFORML 「어루만지고 어루만지고 해 준다……」
			PRINTFORMW 「후후……귀엽다……」
		CASE 3
			PRINTFORML 「잘 할 수 있고 있다？ 정말？」
			PRINTFORMW 「좋았다……. 좀 더 노력한다……」
		CASE 4
			PRINTFORML 「쫄깃쫄깃, 기분 좋다……？」
			PRINTFORMW 「좋아, 좀 더 기분 좋아져……」
		CASEELSE
			;未成熟,粗チン
			IF TALENT:MASTER:형상 == 1 || TALENT:MASTER:형상 == 2
				PRINTFORML 「%CALLNAME:MASTER%의 자지……작고 귀엽다……」
				PRINTFORMW 「괜찮아, 나의 신체에는 딱 맞아……」
			;発光,イボ付き
			ELSEIF TALENT:MASTER:형상 == 4 || TALENT:MASTER:형상 == 5
				PRINTFORML 「%CALLNAME:MASTER%의 자지……그……」
				PRINTFORMW 「에, 이상하다고 생각한다……. 역시……」
			ELSE
				PRINTFORML 「%CALLNAME:MASTER%의 자지……귀의군요……」
				PRINTFORMW 「이렇게 귀의의 것이……나중에……고크리」
			ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

;==================================================
;81,フェラチオ
;==================================================
@M_KOJO_MESSAGE_COM_K83_81
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K83_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K83_81_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K83_UFUFU(83)
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:시간정지
		PRINTFORML 「, 귀의……이런 건 들어가지 않을지도……」
		PRINTFORMW 「으응, 이, 노력한다……！」
		RETURN 1
	ENDIF
	IF FLAG:시간정지
	;イきそう
	ELSEIF NOWEX:MASTER:사정
		PRINTFORML 「, 응…….」
		CALL HPH_PRINT(@"「イきそう？ んじゃあこのまま、おくちにちょーうだい……HPH」", "W")
	ELSE
		SELECTCASE RAND:5
		CASE 1
			PRINTFORML 「응응,……후~」
			PRINTFORMW 「 , 미안, 역시 국화라고……」
		CASE 2
			PRINTFORML 「………」
			PRINTFORMW 「후~…후~……. 」
		CASE 3
			PRINTFORML 「구, 입으로 하고 든다……. 짠다………」
			PRINTFORMW 「와, 기분 좋다……？」
		CASE 4
			PRINTFORML 「………………」
			PRINTFORMW 「, 히응히응, 두고 히 있고……」
		CASEELSE
			PRINTFORML 「………」
			PRINTFORMW 「와, 기분이야 샀다？ 네에에……」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

