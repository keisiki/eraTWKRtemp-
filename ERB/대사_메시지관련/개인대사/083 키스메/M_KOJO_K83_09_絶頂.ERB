;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;分岐作りに有用そうな変数などのメモ
;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;NOWEX:C절정 NOWEX:V절정 NOWEX:B절정 NOWEX:A절정 NOWEX:M절정
;各部位の絶頂強度。まず0~3が割り当てられ、多重絶頂により最大16倍される。つまり単部位なら最大3、5重絶頂で最大48
;NOWEX:TotalEX
;多重絶頂補正を受ける前の各部位絶頂強度の合計値。1~15。このテンプレにおける分岐の基準。

;TEQUIP:50
;Ｖに誰の竿が入ってるか。TEQUIP:50 = MASTERであなたが挿入中となる。
;TEQUIP:51
;Ａに誰の竿が入ってるか。TEQUIP:51 = MASTERであなたが挿入中となる。
;NOWEX:사정
;射精量、最大3。たとえばIF NOWEX:MASTER:사정 > 1だと、あなたが大量射精以上した場合となる。
;SYNCED_ORGASM(TARGET)
;相手とあなたが同時に絶頂した場合真となる。絶頂の部位は問わない。

;-------------------------------------------------
;絶頂時の口上
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K83
#DIM TotalEX
CALL PALAM_MESSAGE_B
SIF FLAG:시간정지
	RETURN 1
;-------------------------------------------------
;噴乳
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:분유
	PRINTFORMW 「이나, 나다……\/\/\/ 젖가슴이……%M_KOJO_K83_AUTO("単刺激…！")%」
ENDIF
;;-------------------------------------------------
;;射精した
;;記入チェック（=0, 非表示、1, 表示）
;LOCAL = 0
;;-------------------------------------------------
;IF LOCAL && NOWEX:사정
;	PRINTFORMW 
;ENDIF
;-------------------------------------------------
;放尿
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:방뇨
	PRINTFORMW 「……오줌 나와버린다……！」
ENDIF
;-------------------------------------------------
;絶頂
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;合計絶頂数で分岐
;これだけではどの部位が絶頂したかは区別されないため、書き分けたければ任意で分岐を加筆されたし。
IF LOCAL
	IF M_KOJO_K83_UFUFU(83)
		SELECTCASE NOWEX:TotalEX
		;強
		CASE 5 TO 15
			SELECTCASE RAND:3
			CASE 1 
				CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("二連刺激")%、らめぇっ、%M_KOJO_K83_AUTO("単刺激")%！ イクイクイクっHPH あああぁぁっHPHHPH」", "W")
			CASE 2
				CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("二連刺激")%、きちゃうっ、%M_KOJO_K83_AUTO("単刺激")%！ んぁぁっHPH イっちゃう～～っHPHHPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("二連刺激")%、ひゃんっ、%M_KOJO_K83_AUTO("単刺激")%！ イっちゃう～っHPH ごめんなさい～～っHPHHPH」", "W")
			ENDSELECT
		;中
		CASE 2 TO 4
			PRINTFORM 「%M_KOJO_K83_AUTO("二連刺激")%%M_KOJO_K83_AUTO("単刺激")% 
			SELECTCASE RAND:3
			CASE 1 
				CALL HPH_PRINT(@"イっちゃうッ！ イクぅっHPH」", "W")
			CASE 2
				CALL HPH_PRINT(@"ら、らめぇっ！ イきゅぅっHPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"イクぅっ！ んぁぁっHPH」", "W")
			ENDSELECT
		;弱
		CASE 1
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")% 이…！」
			CASE 2
				PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")% (이)다, 안돼…！」
			CASEELSE
				PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")% 있고, 이크…！」
			ENDSELECT
		ENDSELECT
	ELSE
		SELECTCASE NOWEX:TotalEX
		;強
		CASE 5 TO 15
			PRINTFORMW 「%M_KOJO_K83_AUTO("二連刺激")%%M_KOJO_K83_AUTO("単刺激")% 이크！」
		;中
		CASE 2 TO 4
			PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")% 이크！」
		;弱
		CASE 1
			PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激…！")% 이크…！」
		ENDSELECT
	ENDIF
ENDIF
