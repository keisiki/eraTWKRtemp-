@M_KOJO_MESSAGE_PALAMCNG_A_K83_20(ARG)
CALL PALAM_MESSAGE_A
;-------------------------------------------------
;手淫フェラ
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K83_UFUFU(83)
;-------------------------------------------------
IF LOCAL
	;精飲
	IF EXP_UP(15) == NOWEX:PLAYER:사정 * 3
		SELECTCASE RAND:3
		CASE 1
			PRINTFORML 「응응！ ……매우 응」
			CALL HPH_PRINT(@"「ぷぁ……全部飲めた……HPH」", "W")
		CASE 2
			PRINTFORML 「응-！ …………」
			CALL HPH_PRINT(@"「ぷぁ……おいしかったHPH」", "W")
		CASEELSE
			PRINTFORML 「응！ 응―…………매우 응」
			CALL HPH_PRINT(@"「ぷぁ……ごちそうさまHPH」", "W")
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 1
			CALL HPH_PRINT(@"「んぐっ…！ ぷぁ……いっぱい出たね……HPH」", "W")
		CASE 2
			PRINTFORMW 「응응…！ ……%CALLNAME:MASTER%, 기분이야 유혹한다……」
		CASEELSE
			PRINTFORMW 「응-…！ ……조금 씁쓸하다……」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF
RETURN 1
