IF LOCAL
	SELECTCASE RAND:5
	CASE 1
		CALL HPH_PRINT(@"「ふふ、ぐちゅぐちゅになっちゃった……HPH」", "W")
	CASE 2
		CALL HPH_PRINT(@"「ふぅ……き、気持ちよかった……HPH」", "W")
	CASE 3
		CALL HPH_PRINT(@"「ふぇぇ……つ、疲れた……%CALLNAME:MASTER%、ちゅーして…HPH」", "W")
	CASE 4
		PRINTFORMW 「후~, 후~……%CALLNAME:MASTER%, 기분이야 유혹한다……」
	CASEELSE
		IF TALENT:처녀 == 1 || TALENT:처녀 == -1
			PRINTFORMW 「후~, 후~……, 실전 하고 있는 것 같다……\/\/\/」
		ELSE
			CALL HPH_PRINT(@"「はぁ、はぁ……つ、次は中にちょーだい……HPH」", "W")
		ENDIF
	ENDSELECT
	RETURN 1
ENDIF
RETURN 1

