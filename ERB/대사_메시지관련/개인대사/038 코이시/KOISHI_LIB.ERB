;-------------------------------------------------
;코이시용 랜덤 키스효과 생성기
;-------------------------------------------------
@KOISHI_RANDKISS(ARG)
PRINTFORM 「
CALL KOISHI_RANDKISS_MAIN(ARG)
PRINTFORML 」

@KOISHI_RANDKISS_MAIN(ARG)
IF 1
	LOCAL:1 = ARG + 1
	REPEAT LOCAL:1
		SELECTCASE RAND:11
			CASE 0
				PRINTFORM 쪽
			CASE 1
				PRINTFORM 츕
			CASE 2
				PRINTFORM 츄웁
			CASE 3
				PRINTFORM 낼름
			CASE 4
				PRINTFORM 푸하
			CASE 5
				PRINTFORM 쯉
			CASE 6
				PRINTFORM 하읍
			CASE 7
				PRINTFORM 츄우
			CASE 8
				PRINTFORM 쪼옥
			CASE 9
				PRINTFORM 쮸웁
			CASE 10
				PRINTFORM 으응
		ENDSELECT
		SELECTCASE RAND:21
			CASE 0 TO 10
				PRINTFORM ...
			CASE 11 TO 19
				PRINTFORM ..
			CASE 20
				PRINTFORM 
		ENDSELECT
	REND
ENDIF

;---------------------------------------------
;코이시용 랜덤 만취 대사
;---------------------------------------------
@KOISHI_DRUNKEN
IF 1
    SELECTCASE RAND:5
        CASE 0
            PRINTFORML 「에헤헤~...기분죠아아~」
        CASE 1
            PRINTFORML 「우흐흐...」
        CASE 2
            PRINTFORML 「아하핫...마시써어...」
        CASE 3
            PRINTFORML 「...머리 아파아...무울...」
        CASE 4
            PRINTFORML 「으으응...언니이...」
    ENDSELECT
ENDIF

;코이시용 랜덤 신음 발생기
;AUTO_AEGI가 있지만 그런건 가볍게 무시하자
@KOISHI_CLIMAX(ARG)
#DIM POST_CURR
#DIM NOW_CURR
IF 1
	POST_CURR = -1
	REPEAT ARG + 1
		$절정리롤
		NOW_CURR = RAND:16
		IF NOW_CURR == POST_CURR
			GOTO 절정리롤
		ELSE
			POST_CURR = NOW_CURR
		ENDIF
		SELECTCASE NOW_CURR
			CASE 0
				PRINTFORM 읏
			CASE 1
				PRINTFORM 앗
			CASE 2
				PRINTFORM 흐읏
			CASE 3
				PRINTFORM 히잇
			CASE 4
				PRINTFORM 히극
			CASE 5
				PRINTFORM 응히잇
			CASE 6
				PRINTFORM 응
			CASE 7
				PRINTFORM 으읏
			CASE 8
				PRINTFORM 하앙
			CASE 9
				PRINTFORM 으긋
			CASE 10
				PRINTFORM 하아
			CASE 11
				PRINTFORM 으응
			CASE 12
				PRINTFORM 흐으읏
			CASE 13
				PRINTFORM 하아아
			CASE 14
				PRINTFORM 후읏
			CASE 15
				PRINTFORM 힛
		ENDSELECT
		SELECTCASE RAND:6
			CASE 0
				PRINTFORM ...
			CASE 1
				PRINTFORM ! 
			CASE 2
				PRINTFORM ..
			CASE 3
				PRINTFORM ...! 
			CASE 4
				PRINTFORM , 
			CASE 5
				IF TALENT:연인
					PRINTFORM ♥ 
				ELSE
					PRINTFORM , 
				ENDIF
		ENDSELECT
	REND
ENDIF

;################################################################
; 코이시 전용 거절 함수
; 근데 몇번 안씀
;
;################################################################
@KOISHI_REFUSE
;커맨드 실패시 나오게 할 대사
	IF MARK:반발각인 == 3
		IF TALENT:연인
			SELECTCASE RAND:2
				CASE 0
					PRINTFORMW 「읏...저리가...」
					PRINTFORMW 코이시는 몸을 떼어놓았다
				CASE 1
					PRINTFORMW 「하지말라구...」
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORMW 「...뭐하는짓이야...」
				CASE 1
					PRINTFORMW 「...기분나빠, 저리가줘...」
					PRINTFORMW 코이시는 %마스터를% 노려보았다
			ENDSELECT
		ENDIF
	ELSEIF TALENT:연인
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「...음, 나중에 하면 안될까?」
			CASE 1
				PRINTFORMW 「...왠지 싫은걸, 다음에 해줘...」
			CASE 2
				PRINTFORMW 「싫어...그건 싫다구」
		ENDSELECT
	ELSEIF TALENT:연모||TALENT:사모||TALENT:애욕
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「......어...음...그런건 좀...」
			CASE 1
				PRINTFORMW 「에에...」
				PRINTFORMW 코이시는 싫어하고 있다
		ENDSELECT
	ELSEIF TALENT:섹프
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「...그런건 재미없는걸」
			CASE 1
				PRINTFORMW 「...다른거 하면 안될까?」
			CASE 2
				PRINTFORMW 「아무리 그래도 이건 싫어」
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「...어...음...」
				PRINTFORMW 코이시는 곤란한듯한 표정을 지었다
			CASE 1
				PRINTFORMW 「싫은걸? 그런거 말고 다른건 없는거야?」
		ENDSELECT
	ENDIF

;-----------------------------------
;코이시용 펠라치오 소리 생성기
;-----------------------------------
@KOISHI_ORALSEX_MAIN(ARG)
#DIM POST_CURR
#DIM NOW_CURR
POST_CURR = -1
REPEAT ARG+RAND:2
	$펠라리롤
	NOW_CURR = RAND:8
	IF NOW_CURR == POST_CURR
		GOTO 펠라리롤
	ELSE
		POST_CURR = NOW_CURR
	ENDIF
	SELECTCASE NOW_CURR
		CASE 0
			PRINTFORM 츄읍
		CASE 1
			PRINTFORM 츕
		CASE 2
			PRINTFORM 하압
		CASE 3
			PRINTFORM 낼름
		CASE 4
			PRINTFORM 하읍
		CASE 5
			PRINTFORM 흐읍
		CASE 6
			PRINTFORM 츄웁
		CASE 7
			PRINTFORM 쪽
	ENDSELECT
	SELECTCASE RAND:3
		CASE 0
			PRINTFORM ...
		CASE 1
			PRINTFORM , 
		CASE 2
			PRINTFORM  
	ENDSELECT
REND

@KOISHI_ORALSEX(ARG)
PRINTFORM 「
CALL KOISHI_ORALSEX_MAIN(ARG)
PRINTFORML  」

;------
;절정 아닌 신음
;------
@KOISHI_SCREAM(ARG)
LOCAL:1 = ARG+1
IF 1
	IF MARK:반발각인 == 3&&!TALENT:연인
		SELECTCASE RAND:2
			CASE 0
				PRINTFORM 「
				CALL KOISHI_CLIMAX(RAND:(LOCAL:1)+3)
				PRINTFORM 시럿, 조금만 천천히...
				PRINTFORML 」
			CASE 1
				PRINTFORM 「
				CALL KOISHI_CLIMAX(RAND:(LOCAL:1)+2)
				PRINTFORM 잠까안...너무 격렬, 해...
				PRINTFORML 」
		ENDSELECT
	ELSEIF TALENT:연인
		SELECTCASE RAND:2
			CASE 0
				PRINTFORM 「
				CALL KOISHI_CLIMAX(RAND:(LOCAL:1)+4)
				PRINTFORM 좀 더어...%UNICODE(0x2764)%
				PRINTFORML 」
			CASE 1
				PRINTFORM 「
				CALL KOISHI_CLIMAX(RAND:(LOCAL:1)+3)
				PRINTFORM 흐에에...%UNICODE(0x2764)%
				PRINTFORML 」
		ENDSELECT
	ELSEIF TALENT:섹프
		SELECTCASE RAND:2
			CASE 0
				PRINTFORM 「
				CALL KOISHI_CLIMAX(RAND:(LOCAL:1)+3)
				PRINTFORM 더어...더 세게 해줘어...%UNICODE(0x2764)%
				PRINTFORML 」
			CASE 1
				PRINTFORM 「
				CALL KOISHI_CLIMAX(RAND:(LOCAL:1)+3)
				PRINTFORM 너무 기분죠아앗...!
				PRINTFORML 」
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTFORM 「
				CALL KOISHI_CLIMAX(RAND:(LOCAL:1)+2)
				PRINTFORM 기분 좋아아...
				PRINTFORML 」
			CASE 1
				PRINTFORM 「
				CALL KOISHI_CLIMAX(RAND:(LOCAL:1)+2)
				PRINTFORM 하으으...
				PRINTFORML 」
		ENDSELECT
	ENDIF
ENDIF

;----------------------------------------------------
;코이시 중복 회화 처리용 함수
;ARGS로 상황 불러옴
;----------------------------------------------------
@KOISHI_DUP_TALK(ARGS,ARG=0)
SELECTCASE ARGS
	CASE "회임첫대사-고개숙인코이시" ;회임 첫대사 + 무지 + 무자각임신 + 연인
		PRINTFORMW 코이시는 고개를 숙였다...
		PRINTFORML [1] %마스터의% 능력을 밝힌다
		PRINTFORML [2] 그저 위로해줄뿐
		INPUT
		IF RESULT == 1
			CALL KOISHI_DUP_TALK("회임첫대사-능력시범",1)
			PRINTFORMW 「...으으, 그런건 좀 빨리 말해주지...」
			PRINTFORMW 「괜시리 걱정했다구우...버려질까봐...」
			PRINTFORMW 코이시는 희미하게 웃으며 눈물을 닦았다...
			PRINTFORMW 「...%마스터의% 아이라아...분명 귀엽겠지...히힛...」
			CFLAG:2506 = 1 ;시간정지 자각
			CFLAG:5502 = 0 ;이번 아이 책임 X
		ELSE
			PRINTFORMW %마스터는% 울고 있는 코이시를 토닥토닥 다독여줬다
			PRINTFORMW ...재밌는게 생각났다, 내 애라곤 안할거지만, 책임은 져주겠다고 말하면 어떨까?
			PRINTFORML [1] 나쁘지 않은데?
			PRINTFORML [2] 그래도 책임지긴 싫은데...
			INPUT
			IF RESULT == 1
				PRINTFORMW %마스터는% 코이시의 눈물을 닦아주며, 그 아이를 책임져 주겠다고 말했다
				PRINTFORMW 「...어..? 책임져 주는거야...?」
				PRINTFORMW 「그 말은...버리지 않겠다는거야...?」
				PRINTFORMW 코이시는 놀란 눈으로 %마스터를% 바라봤다
				PRINTFORMW 「...흐윽...기뻐...그리고 미안해...\@ARG ? 처음엔#\@ %마스터의% 아이를 꼭 낳아주고 싶었는데...」
				PRINTFORMW \@ARG ? ...사실 #\@뱃속에 %마스터의% 아이가 있단걸 알면 무슨 생각을 할까...
				CFLAG:호감도 += 50
				CFLAG:신뢰도 += 25
			ELSE
				PRINTFORMW 관두자, 애초에 그럴거면 코이시한테 그런짓 하지 말았어야지
				CFLAG:5502 = 1 ;이번 아이 책임 X
				CFLAG:6000++ ;배드엔딩 플래그
			ENDIF
		ENDIF
	CASE "회임첫대사-능력시범"
		IF ARG == 1 ;무지 + 무자각임신 + 연인
			PRINTFORMW %마스터는% 코이시에게 시간정지의 시범을 보여주었다
			PRINTFORMW 고개를 들게 하고, 시간정지로 등 뒤로 간뒤 뒤에서 포옹해주자 깜짝 놀란 표정을 했다...
			PRINTFORMW 「...그러니까, 이 애는 %마스터의% 애라구?」
			PRINTFORMW 「......」
			PRINTFORMW %마스터가% 자신의 아이임을 알려주자 코이시는 얼굴을 붉혔다
		ENDIF
		;일단 능력의 시범을 보였으니 시간정지 자각을 합니다
		CFLAG:2506 = 1
	CASE "회임첫대사-연모취득" ;TODO 조건확인요망
		;0b1 = 깨끗한교제 생성여부 0b10 애욕 변동 여부 0b100 연인까지 가는지 여부
		IF ARG & 0b10 && TALENT:애욕 ;애욕이 있으면 사모가 없는것으로 상정
			PRINTFORMW 코이시는 몸도 마음도 %CALLNAME:MASTER%에게 채워지는 것에 즐거움을 느끼고 있다...
			PRINTFORMW 문란한 관계속에 여문 연정, 그것도 또 하나의 사랑의 형태...
			PRINTFORMW 코이시의 [애욕]은 [연모]로 변했습니다
			PRINTFORML
			TALENT:애욕 = 0
		ENDIF
		IF TALENT:사모||!TALENT:연인
			PRINTFORMW 코이시는 %마스터와% 보내는 시간에 행복을 느끼고 있는것 같다...
			PRINTFORMW 코이시의 [사모]는 [연모]가 되었다
			PRINTFORML
			IF ARG & 1 && !MARK:시간각인
				CFLAG:깨끗한교제 = 1
			ENDIF
			TALENT:사모 = 0
		ENDIF
		TALENT:연모 = 1

		IF ARG & 0b100
			PRINTFORMW 코이시는 뺨을 발그레하게 물들이며 %마스터를% 받아들였다...
			PRINTFORMW 코이시와 %마스터는% [연인]이 되었다
			TALENT:연인 = 1
		ENDIF
	CASE "회임첫대사-누군가의해코지"
		PRINTFORMW %마스터는% 아마도 누군가가 코이시가 자는동안 해코지 했던가 한 것 같다고 답했다
		PRINTFORMW 「...흑...싫은데...얼굴도 모르는 사람의 아이같은건...」
		PRINTFORMW 「...차라리 %CALLNAME:MASTER% 애면 기쁠텐데...흑...」
		PRINTFORMW 코이시는 낙담한듯 고개를 숙이곤 흘끔흘끔 %마스터를% 바라봤다
		PRINTFORMW 「...저기, %CALLNAME:MASTER%...나 얘 어떻게 해야해...? 낳아야해..?」
		PRINTFORML [1] 같이 길러줄테니 낳자
		PRINTFORML [2] 뭐 그럴수밖에, 잘해봐
		INPUT
		IF RESULT == 1
			PRINTFORMW 「...어? 같이 길러준다고...? 왜..?」
			PRINTFORMW %마스터는% 코이시를 안아주며 그 아이도 내 아이인 셈 치고 같이 키우자고 말했다
			PRINTFORMW 코이시는 얼굴을 붉혔다...
			PRINTFORMW 「으응...미안해...폐 끼쳐서...흐윽...%마스터의% 애면 좋을텐데...」
			PRINTFORMW 「...저기, 이 애 낳은 뒤에...%마스터의% 애도 만들어주면 안될까...? 적어도 또 모르는 사람의 애는 갖고 싶지 않은걸...」
			PRINTFORMW 「차라리 %마스터가% 아빠인 아이가 훨씬 나아...그러니깐...」
			PRINTFORMW 코이시는 부끄러운지 말을 끝맺지 못했다...
			PRINTFORML
			CALL KOISHI_DUP_TALK("회임첫대사-연모취득", 0b10) ;TODO 조건확인요망
			CFLAG:6000 -= 2
			CFLAG:5502 = 0
		ELSE
			PRINTFORMW 「...훌쩍...으읏...우에에엥...」
			PRINTFORMW 코이시는 훌쩍훌쩍 울었으나 %마스터는% 애써 그것을 듣지 않았다
			CFLAG:6000++ ;배드엔딩 플래그
			CFLAG:5502 = 1 ;이번 아이 책임 X
		ENDIF
	CASE "회임첫대사-같이길러줄래"
		PRINTFORMW 「근데...얘 어떻게 해야해...? 낳아야 해? ...같이 길러줄거야?」
		PRINTFORML [1] 당연하지
		PRINTFORML [2] 아니, 기를 생각은 없는데...
		INPUT
		IF RESULT == 1
			PRINTFORMW 「...으으음...그러며언..나도 좋아! 같이 기르자~」
			PRINTFORMW 「아이 이름은 어떻게 할래? 다음에 또 만들래? 아니아니 일단 사귀는거부터 해야하나? 응?」
			PRINTFORMW 코이시는 왠지 들떠보인다
			PRINTFORMW 「앗...그렇지이...일단은...이렇게...%UNICODE(0x2764)%」
			PRINTFORMW 코이시는 얼굴을 들어 %마스터의% 볼에 진한 키스를 했다
			PRINTFORMW 「히히...좋아해...%CALLNAME:MASTER%...」
			CALL KOISHI_DUP_TALK("회임첫대사-연모취득", 0b11) ;TODO 조건확인요망
			PRINTFORMW 코이시는 뺨을 발그레하게 물들이며 %마스터를% 받아들였다...
			PRINTFORMW 코이시와 %마스터는% [연인]이 되었다
			TALENT:연인 = 1
			CFLAG:6000 -= 2
		ELSE
			PRINTFORMW 「에에...방금까진 %마스터가% 아빠라며?」
			PRINTFORMW 「...책임지긴 싫다구? 으으...그럼 얜 어쩌라구우...」
			PRINTFORMW %마스터는% 알아서 하라며 손을 내저었다
			PRINTFORMW 「...우으읏...너무해...어째서 그러는거야...응?」
			PRINTFORMW 「...훌쩍...으읏...멋대로 아기만들어놓곤...왜에...책임은 안져주는데...」
			PRINTFORMW 코이시는 훌쩍훌쩍 울었으나 %마스터는% 애써 그것을 듣지 않았다
			CFLAG:6000++ ;배드엔딩 플래그
			CFLAG:5502 = 1 ;이번 아이 책임 X
		ENDIF
	CASE "회임첫대사-애아빠할래"
		PRINTFORMW 「...\@ARG == 1 ? 우우...#\@저기, %CALLNAME:MASTER%~...%마스터가% 얘 아빠 해주면 안돼?」
		PRINTFORML [1] 그러지 뭐
		PRINTFORML [2] 미쳤어?
		INPUT
		IF RESULT == 1
			PRINTFORMW 「앗- 고마워~」
			PRINTFORMW 「후후...그럼 이제 뭐 할까 여보? 에헷...」
			PRINTFORMW 소꿉놀이하는 아이마냥 싱글벙글 다가왔다...
			PRINTFORMW ...것보다 딱히 아이가 생긴건 신경쓰지 않는것 같다...
			CFLAG:5502 = 0
			CFLAG:호감도 += 300
			CFLAG:신뢰도 += 100
			ABL:친밀 += 1
		ELSE
			PRINTFORMW 「...칫...야박해...」
			PRINTFORMW 코이시는 볼을 부풀렸다
			PRINTFORMW 「뭐어, 어쩔수 없나...언니보고 키워달래야지...」
			IF CFLAG:49:현재위치 == CFLAG:현재위치
