;=============================
@DIARY_TEXT_K30, PAGENUM, MODE, PAGECOUNT
;PAGENUM   =일기のパターン
;MODE      =데일리:一日종료 시の데일리イベントでの読み上げ
;          =コマンド:[일기を読む]コマンドでの読み上げ
;PAGECOUNT =何ページ目に書かれているか
;=============================
#DIM PAGENUM
#DIMS MODE
#DIM PAGECOUNT
#DIM 중복방지
#DIM 날짜
#DIMS 표정
#DIMS 台詞
#DIMS 복장
#DIMS 이펙트
VARSET LOCALS
복장 = 服
이펙트 =
TFLAG:192 = 0
;台詞색깔＠コマンドで呼び出された時、色を付ける
SETCOLOR 0x337D5C
;일기内容
;グループマッチに일기のIDを入れて、表示させたい顔絵を表示する。
IF MODE == "デイリー"
	;一時的に拡張顔絵に変更
	VARSET LOCAL
	LOCAL = FLAG:스탠딩종류
	FLAG:스탠딩종류 = 1
	LOCAL:1 = CFLAG:30:대체적용
	CFLAG:30:대체적용 = 1
	;カジュアルデーor패션ブームなら素敵なＴシャツ
	SIF YOUBI_MATCH(DAY, "금") || PAGENUM == 7
		복장 = 服_変Ｔ
	;ページごとに표정の指定
	IF GROUPMATCH(PAGENUM, 1, 2, 32)
		IF K30_DIARY_DATA(PAGENUM, 2) == "恋"
			표정 = 笑顔
			이펙트 = ダミー
			TFLAG:192 = -7
		ELSE
			표정 = %\@ PAGENUM == 1 ? 発情 # 睡眠 \@%
		ENDIF
	ELSEIF GROUPMATCH(PAGENUM, 3, 4, 9, 11, 13, 14, 16, 17, 18, 20, 21, 22, 24, 32, 33, 50)
		표정 = 通常
	ELSEIF GROUPMATCH(PAGENUM, 5, 20, 25)
		표정 = 反発
	ELSEIF GROUPMATCH(PAGENUM, 6, 7, 12, 34)
		표정 = 笑顔
	ELSEIF GROUPMATCH(PAGENUM, 8, 10)
		표정 = 怒り
	ELSEIF GROUPMATCH(PAGENUM, 15, 31, 35)
		SELECTCASE K30_DIARY_DATA(PAGENUM, 5)
			CASE "怒"
				표정 = 怒り
			CASE "喜"
				표정 = LOVE
				이펙트 = 照れ
			CASEELSE
				표정 = 赤面
				이펙트 = 照れ
		ENDSELECT
	ELSEIF PAGENUM == 23
		표정 = 絶頂
		복장 = 服_下着
		이펙트 = ダミー
		TFLAG:192 = -9
	ELSEIF PAGENUM == 30
		표정 = LOVE
		이펙트 = ダミー
		TFLAG:192 = -7
	ENDIF
	PRINTFORML 
	CALL PRINT_FACE,30, 표정, 복장, "", 이펙트
	;스탠딩종류を元に戻す
	FLAG:스탠딩종류 = LOCAL
	CFLAG:30:대체적용 = LOCAL:1
ENDIF
IF MODE == "コマンド"
	;처음봄
	IF !STRCOUNT(CSTR:30:0, "일기처음봄")
		PRINTFORMDW 어떤 것을 쓰고 있는지 궁금해서 휙휙 페이지를 넘겨 보았다
		PRINTFORML 
		PRINTFORML ○월 Ｘ일 □요일　맑음
		PRINTFORML 
		PRINTFORML 오전 6시 기상. 컨디션-양호. 오늘 비번. 대신 순회 후보지로 인간마을……
		PRINTFORML …오전 11시 인간마을 근처에서 일을 빠진 코마치를 발견. 지난번 건도 아울러 캐묻는다…
		PRINTFORML ……오후 1시. 질책을 끝내고 그대로 인간마을을 순회………
		PRINTFORMW ………………
		PRINTFORMDW 일기라기 보다는 업무일보 같지만, 덕분에 만났을 때의 일도 쉽게 발견되었다.
		PRINTFORML
		CALLF FIRSTTIME("일기처음봄")
	;2回目以降、最初のページの前に
	ELSEIF 중복방지 != DAY
		;適当にお설교の対象として이름だけ。神様に当たったら코마치에게 
		LOCAL = RAND(RANDOM_CHARANUM - 1) + 1
		SIF TALENT:LOCAL:신령
			LOCAL = 76
		PRINTFORMDL 우선 적당한 페이지를 넘겨 본다……
		IF LOCAL == 76 || RAND:3 == 1
			PRINTDATADW
				DATAFORM 당연한 일이지만, 코마치의 땡땡이는 염마님에게 전부 파악되고 있는 것 같다
				DATAFORM 아침과 점심과 심야에 코마치에게 설교한 일이 적혀 있었다
				DATAFORM 3시간에 이르는 코마치에 대한 설교에 대해서 적혀있었다
				DATAFORM 나는 그 일이 바쁘다는 것을 설명했지만 특별히 불만스럽게 여기지는 않았다
			ENDDATA
		ELSE
			PRINTDATADW
				DATAFORM %CALLNAME:LOCAL%에게 설교를 했을 때의 일이 적혀있었다
				DATAFORM 『%K30_C_NAME(LOCAL,3)%에게 할 말』이 쭉 늘어져 있었다
				DATAFORM 여전히 업무일보 같은 일기가 쓰여져 있었다
				DATAFORM %CALLNAME:LOCAL%의 원래 설교를 할 계획이 쓰여져 있었다
				DATAFORM 부하에 대한 감사와 위로의 말이 적혀 있었다
			ENDDATA
		ENDIF
		PRINTFORMDL ―――――――――――――――――――――――――――――
		PRINTFORMDL 조금 더 읽다보니 자신에 대해 쓰여져 있는 부분을 발견했다
		PRINTFORMW 
	ENDIF
	중복방지 = DAY
ENDIF
PRINTFORMDL - %CALLNAME:30%의 일기 --------------------------------------------------------------------------------------------------------
PRINTL
;CSTRに保存した날짜を体裁整えて表示
날짜 = TOINT(K30_DIARY_DATA(PAGENUM, 0))
IF 날짜 != 0
	SPLIT PRINT_DATE_F(날짜), " ", LOCALS
	PRINTFORML %REPLACE(LOCALS:1, "달의", "달 ")% %GET_DAY(날짜)%요일　%K30_DIARY_DATA(PAGENUM, 1)%
;不具合出たときの保険。記号バージョンも併記
ELSE
	LOCAL:1 = PAGENUM % 4
	LOCAL:2 = PAGENUM % 9
	LOCAL:3 = PAGENUM % 7
	PRINTFORML %UNICODE(9829 + LOCAL:1)%의 달 %UNICODE(9824 + LOCAL:2)%일 %UNICODE(945 + LOCAL:3)%요일　%K30_DIARY_DATA(PAGENUM, 1)%
ENDIF
PRINTFORML 
SELECTCASE PAGENUM
	;ファーストキス＠オプション次第でTINKOにキスでも発生。ぼかした書き方にしておく
	CASE 1
		;연모と장소で簡単な分岐
		LOCAL = (K30_DIARY_DATA(PAGENUM, 2) == "사랑")
		PRINTFORML 자신\@ K30_DIARY_DATA(PAGENUM, 5) == "봉함" ? 이 한 # 이 당한 \@ 일의 대담함에 아직 가슴이 두근거리는 모양이다.
		IF GROUPMATCH(K30_DIARY_DATA(PAGENUM, 4), "시비곡직청", "자기 방", "에이키의 방")
			PRINTFORML 조금 눈을 감고 침착하려고 해도, 저 감촉과 함께 여러가지 일이 머리에 소용돌이쳐 온다
			PRINTFORML 그렇다고 눈을 뜨고 주위를 둘러보면 하던 때의 풍경까지 또렷이 떠올리게 된다
			PRINTFORML 이래서는 오늘 잠자리에 드는 것도 무리라는 생각이 든다.
		ELSE
			PRINTFORML %K30_DIARY_DATA(PAGENUM, 4)%. 한동안 그곳을 지날 때마다 이런저런 생각이 들 것 같다.
			PRINTFORML 당분간은 그 근처에서 가르침을 설파하지 말자. 도저히 평범하게 말할 자신이 없다.
			PRINTFORML 자신이 이렇게까지 흔들릴 수 있을 거라고는 생각도 못했다.
		ENDIF
		PRINTFORML 
		PRINTFORML 딱딱한 염마\@ LOCAL ? 에서 벗어나 겨우 말길이 닿은 # 인 채로 헤매기만 하는  \@나를 %조사처리(K30_DIARY_DATA(PAGENUM, 3),"는")% 어떻게 생각하고 있을까.
		PRINTFORM 상스럽다고는 생각되지 않았을까.
		IF LOCAL
			PRINTFORML 아니면 내 기분에 부응해 줄 것인가.
		ELSE
			PRINTFORML 만약 환멸을 당하면 어떻게 해야 할까?
		ENDIF
		PRINTFORMW 무슨 일이 있을 때마다 생각에 잠겨 버리는 자신의 성격이 조금 원망스러워진다.
		표정 = 赤面
		TFLAG:192 = -6
		台詞 = 「…………………………」/「…………………………」
	;처녀喪失
	CASE 2
		;陥落、積極具合で簡単な分岐。사모以下はキャンセル구상前提
		LOCALS = %K30_DIARY_DATA(PAGENUM, 2)%
		SELECTCASE LOCALS
			CASE "無"
				PRINTFORML 역시 내겐 육신따윈 필요 없었던 것 아닌가.
				PRINTFORMW 이런 괴로운 생각을 하는 것도, 나에게 어울리지 않는 것을 가졌기 때문일텐데.
			CASE "思"
				PRINTFORML %K30_DIARY_DATA(PAGENUM, 3)%에게 처음을 빼앗기고 말았다.
				PRINTFORML 염마로서 넘어서는 안될 선을 넘어서고 말았다.
				PRINTFORML 하지만 그런 식으로 빼앗겼는데도 지금의 나는 그 사람을 진심으로 부정할 수 없어.
				PRINTFORMW 괴로워. 너무 괴로워.
			CASE "恋"
				PRINTFORML 마침내 %조사처리(K30_DIARY_DATA(PAGENUM, 3),"와")% 맺어질 수 있었다.
				PRINTFORML 생각해보면 그 사람을 꽤나 기다리게 하고 말았다. 잘 버리지 못하던 것들이다.
				PRINTFORML 오늘도 자신의 일에 힘쓰고, %조사처리(K30_DIARY_DATA(PAGENUM, 3),"가")% 어땠는지도 기억나지 않는다.
				PRINTFORML 
				PRINTFORML 앞으로는 좀 더 몸도 마음도 그 사람에게 의지해 가자.
				PRINTFORML %조사처리(K30_DIARY_DATA(PAGENUM, 3),"가")% 원하는 것, 기뻐해주는 것을 찾아주고 싶다.
				PRINTFORMW 그걸 해주는 것이 나의 기쁨이 될 것 같다
		ENDSELECT
		IF MODE == "コマンド"
			IF LOCALS == "恋" 
				TFLAG:192 = -6
				표정 = LOVE
				台詞 = 「…………………………」/「………………써 있는 대로예요」
			ELSE
				표정 = 睡眠
				{
				台詞 = 「…할 말이 없는 건 아닙니다」
					/「하지만…지금의 제 생각에 %조사처리(K30_C_NAME(MASTER),"가")% 성실하게만 있어준다면」
					/「그걸로 괜찮습니다. 죄를 용서할 수 있습니다」
				}
			ENDIF
		ENDIF
	;탄막승부에이키님勝ち
	CASE 3
		PRINTFORML 오늘은 %K30_DIARY_DATA(PAGENUM, 4)%에서 %K30_DIARY_DATA(PAGENUM, 3)%에게 탄막승부를 도전 받았다.
		IF GETBIT(CFLAG:30:1910, 0)
			PRINTFORML 단순히 실력을 재는 것뿐이지만 아슬아슬한 지경까지 몰리고 말았다.
			PRINTFORML 단련을 소홀히 했던 건 아니지만 염마로서 이래서는 한심스럽다.
		ELSE
			PRINTFORML 과연 뒤떨어지는 일은 없었지만, 위험했던 곳도 몇 번인가 있었다
			PRINTFORML 단련하면 아직 늘어날 것이다. 이겼다고는 하지만 헛되이 지낼 수는 없다.
		ENDIF
		PRINTFORMW 나도 더욱 정진이 필요하다
		IF MODE == "コマンド"
			PRINTFORML 
			PRINTFORMDL 이 다음에도 탄막승부에 대한 반성과 분석이 빽빽이 적혀 있다
			PRINTFORMDW 염마님의 힘의 비밀은 이 착실함이 틀림없다…
			표정 = 通常
			IF RAND:2 == 1
				{
				台詞 = 「승부에 있어서 전력을 다하는 것은 피차 당연」
				/「그렇다면 사전 준비나 전술에서 차이를 둘 수 밖에 없습니다」
				/「이 정도는 해 두어야 마땅한 일입니다」
				}
			ELSE
				{
				台詞 = 「%\@ GETBIT(CFLAG:1910, 0) ? 첫 승부에서 거기까지 몰렸습니다 # 결과는 어떻든 완승이라고는 생각하지 않습니다 \@%」
				/「적당히 봐주는 건 생각할 수도 없습니다」
				/%\@ CFLAG:30:탄막승부승리 ? 「게다가 결국에는 당신에게는 져버리고 말았습니다…」/「저도 아직 많이 미숙하군요」
				#「첫째, 내가 져버리면 염마의 위엄에 관계됩니다」/「몇 번이고 당신의 도전은 물리쳐 보이겠습니다」 \@%
				}
			ENDIF
		ENDIF
	;탄막승부에이키님負け
	CASE 4
		LOCAL = GETBIT(CFLAG:30:1910, 1)
		PRINTFORML 오늘은 %조사처리(K30_DIARY_DATA(PAGENUM, 3),"와")%의 탄막승부.
		PRINTFORML 원래 비범한 힘을 지녔다고 생각했지만, 
		IF LOCAL
