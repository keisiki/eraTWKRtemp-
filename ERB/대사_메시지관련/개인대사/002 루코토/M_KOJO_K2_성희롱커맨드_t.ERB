IF LOCAL
	IF K2_BASIC_CONDITION_IN_MATING()
		IF CFLAG:우후후 != 2
			IF TALENT:연모 || TALENT:애욕
				IF BASE:무드 >= 750 && BASE:이성 < 500
					;명째가 있을까 옥외
					IF EYEWITNESS() || OPENPLACE(CFLAG:MASTER:현재위치) >= 2
						;하반신노출인가 노팬티 치마, 한편 랜덤 3분의 2
						IF TEQUIP:하반신착의상황 <= 1 && RAND:3
							;사람이 있다, 한편 랜덤 2분의 1
