					SELECTCASE RAND:2
					CASE 1
						CALL K2_HPH_PRINT,@"「쭙…? HPH……할짝……하앙 HPH」","W"
					CASEELSE
						CALL K2_HPH_PRINT,@"「……쮸웁………쪽…? HPH」","W"
					ENDSELECT
				ELSEIF ABL:친밀 >= 5 || ABL:욕망 >= 5
					SELECTCASE RAND:2
					CASE 1
						PRINTFORMW 「응…응…쪽……으음」
					CASEELSE
						PRINTFORMW 「아……쮸웁………하아」
					ENDSELECT
				ELSE
					PRINTFORMW 「아……으음…쪽…」
				ENDIF
			ELSE
				IF TALENT:연모 || ( TALENT:애욕 && ( ABL:친밀 >= 5 || ABL:욕망 ) )
					PRINTFORMW 「아…응…… 갑자기 지나요」
				ELSE
					PRINTFORMW 「아앙!큐, 갑자기입니까 」
				ENDIF
			ENDIF
		ENDIF
	ENDIF
