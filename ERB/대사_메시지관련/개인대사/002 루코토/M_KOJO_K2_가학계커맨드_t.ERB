;가학계 커맨드
;※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;100, 스팬킹
;==================================================
@M_KOJO_MESSAGE_COM_K2_100
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2_100_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF

	$SPANKING_LOOP
	IF ( TALENT:연모 || TALENT:애욕 || TALENT:사모 || TALENT:섹프 ) && K2_BASIC_CONDITION_IN_MATING()
		IF ( ABL:마조끼 >= 4 || ( TALENT:마조 && ABL:마조끼 >= 2 ) ) && ( !TALENT:새드 || ABL:욕망 >= 20 )
			SELECTCASE RAND:4
			CASE 1
				IF CFLAG:1001 && !RAND:3
					IF TALENT:연모 || TALENT:연인
						CALL K2_HPH_PRINT,@"「%CALLNAME:MASTER%님에게 처녀를 바치지 않고 미안해요 HPH」","L"
						CALL K2_HPH_PRINT,@"「여러 사람의 자지 맛본 중고 로봇를 용서(허락)해 주세요 HPH」","W"
					ELSEIF ABL:청소기능 + ABL:요리기능 < 4
						CALL K2_HPH_PRINT,@"「일 기억하지 않고, 보지 하는 일만 기억한 파렴치 메이드에게 벌 해 주세요 HPH」","W"
					ELSE
						CALL K2_HPH_PRINT,@"「응, 하앙 HPH 누구와도 보지 하는 음란 메이드로 미안해요 HPH」","W"
					ENDIF
				ELSE
					GOTO SPANKING_LOOP
				ENDIF
			CASE 2
				CALL K2_HPH_PRINT,@"「아하HPH 안 돼 메이드에게 좀 더 벌 해 주세요 HPH」","W"
			CASE 3
				CALL K2_HPH_PRINT,@"「흐응 HPH 하아 HPH 아픈 것 대단하군요 HPH」","W"
			CASEELSE
				CALL K2_HPH_PRINT,@"「아히HPH 하악, 응 HPH 하앙, 하아… 벌써…끝입니까? HPH」","W"
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「…… 엉덩이 팡팡 아픕니다」
			CASEELSE
				PRINTFORMW 「으응…… 엉덩이 찡하고 합니다」
			ENDSELECT
		ENDIF
	ELSE
		SELECTCASE RAND:2
		CASE 1
			PRINTFORMW 「쿠웃…… 아픕니다 , 그만두어 주세요」
		CASEELSE
			PRINTFORMW 「있던 , 응, 하아, 하아…… 그, 그만둬, 주세요……」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

