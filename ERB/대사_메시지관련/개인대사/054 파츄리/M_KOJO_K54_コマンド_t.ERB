;여러 가지 이용 방법은 생각됩니다만 적당하게 해 주세요

;-------------------------------------------------
;노예의 절정시의 구상
;NOWEX:C절정～3 CVAB 절정
;NOWEX:M절정 이중절정
;NOWEX:삼중절정 삼중절정
;NOWEX:이중절정 사중절정
;NOWEX:분유 분유
;NOWEX:사정 사정
;NOWEX:방뇨 방뇨
;적당하게 짜맞춰 사용해 주세요
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K54
CALL PALAM_MESSAGE_B
;-------------------------------------------------
;분유
;기입 체크（=0, 비표시, 1, 표시）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:분유
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;사정
;기입 체크（=0, 비표시, 1, 표시）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:사정
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;방뇨
;기입 체크（=0, 비표시, 1, 표시）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:방뇨
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;절정
;기입 체크（=0, 비표시, 1, 표시）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;사중절정
	;기입 체크（=0, 비표시, 1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:이중절정
		PRINTFORMW 
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;삼중절정
	;기입 체크（=0, 비표시, 1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:삼중절정
		SELECTCASE NOWEX:삼중절정
			;C＆V＆A절정
			CASE 1
				PRINTFORMW 
				RETURN 0
			;B＆V＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 0
			;C＆B＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 0
			;C＆V＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 0
		ENDSELECT
	ENDIF
	;-------------------------------------------------
	;이중절정
	;기입 체크（=0, 비표시, 1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:M절정
		SELECTCASE NOWEX:M절정
			;C＆V절정
			CASE 1
				PRINTFORMW 
				RETURN 0
			;C＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 0
			;V＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 0
			;C＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 0
			;V＆B절정
			CASE 5
				PRINTFORMW 
				RETURN 0
			;A＆B절정
			CASE 6
				PRINTFORMW 
				RETURN 0
		ENDSELECT
	ENDIF
	IF NOWEX:C절정
		;강절정C
		IF NOWEX:C절정 == 2
			PRINTFORMW 
		;절정C
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:V절정
		;강절정V
		IF NOWEX:V절정 == 2
			PRINTFORMW 
		;절정V
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:A절정
		;강절정A
		IF NOWEX:A절정 == 2
			PRINTFORMW 
		;절정A
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:B절정
		;강절정B
		IF NOWEX:B절정 == 2
			PRINTFORMW 
		;절정B
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ENDIF


