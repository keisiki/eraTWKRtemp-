
@KICKOUT_4()
#FUNCTION
SELECTCASE CHK_INHOME(CFLAG:MASTER:현재위치)
	CASE 20
		RETURNF 419
	CASE 41
		RETURNF 434
	CASE 54
		RETURNF 465
ENDSELECT

@BATHROOM_4()
#FUNCTION
IF CFLAG:MASTER:현재위치 >= 401 && CFLAG:MASTER:현재위치 <= 430
	RETURNF 420
ELSE
	RETURNF 441
ENDIF
RETURNF 0

@BATHCHECK_4(ARG)
#FUNCTION
SELECTCASE CHK_INHOME(ARG)
	CASE 20,41,54
		RETURNF 1
	CASEELSE
		RETURNF 0
ENDSELECT
RETURNF 0

@MAP_UNIQUEMESSAGE_4
IF WITH_MOB()
	IF FLAG:70
		PRINTFORML 이나바들이 정지해 있다
	ELSEIF GROUPMATCH(TIME:2,1,6,7)
		PRINTFORML 이나바들이 자고 있다…
	ELSEIF !DAY:3 % 10
		IF !TALENT:MASTER:2 & 2
			PRINTFORML 발정중의 이나바들은 괴로워하고 있다……
		ELSE
			PRINTFORML 발정중의 이나바들로부터 강렬한 시선을 느낀다……
		ENDIF
	ELSE
		SELECTCASE RAND:5
			CASE 0
				PRINTFORML 이나바들이 배회하고 있다
			CASE 1
				PRINTFORML 이나바들이 놀고 있다
			CASE 2
				PRINTFORML 이나바들이 서로 이야기하고 있다
			CASE 3
				PRINTFORML 이나바와 이나바가 개응 넘고 있다
			CASE 4
				PRINTFORML 이나바들이 겹겹이 쌓이고 있다
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF

