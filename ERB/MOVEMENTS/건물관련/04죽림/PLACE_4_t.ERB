;비자택시에 갈아넣는 장소 ID를 돌려주는 함수
;ARG, 대상ID
;RETURNF, 갈아넣고가 끝난 대상ID
;-----------------------------------------

@MAP_SEARCH_REPLACEMENT_4(ARG)
#FUNCTION
SELECTCASE ARG
	CASE 401 TO 406
		RETURNF 450
	CASE 407 TO 430
		RETURNF 480
	CASE 431
		RETURNF 440
	CASE 432,433,442,445
		RETURNF 430
	CASE 434,435,440,441
		RETURNF 420
	CASE 436,437
		RETURNF 460
	CASE 438,439
		RETURNF 470
	CASE 450 TO 466
		RETURNF 490
	CASE 443,444,446,447
		RETURNF 410
ENDSELECT
RETURNF 0



@MAP4_AREANAME(PLACE_ID)
#FUNCTIONS
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM PLACE_ID
IF MAIN_MAP == 4
	SIF INRANGE(PLACE_ID, 401, 430)
		RETURNF "영원정"
	SIF INRANGE(PLACE_ID, 436, 437)
		RETURNF "무명의언덕"
	SIF PLACE_ID == 438
		RETURNF "태양의밭"
	SIF INRANGE(PLACE_ID, 450, 466)
		RETURNF "몽환관"
	RETURNF "미혹의 죽림"
ELSE
	SELECTCASE PLACE_ID
	CASE 450, 480
		RETURNF "영원정"
	CASE 460
		RETURNF "무명의언덕"
	CASE 470
		RETURNF "태양의밭"
	CASE 490
		RETURNF "몽환관"
	CASEELSE
		RETURNF "미혹의 죽림"
	ENDSELECT
ENDIF
