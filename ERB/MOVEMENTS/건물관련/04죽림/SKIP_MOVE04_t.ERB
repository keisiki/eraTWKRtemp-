#DIM CONST SPTてゐ私室 = 410
#DIM CONST SPT鈴仙私室 = 411
#DIM CONST SPT空き部屋 = 412
#DIM CONST SPT永琳私室 = 413
#DIM CONST SPT輝夜私室 = 414
#DIM CONST SPT広間 = 415
#DIM CONST SPT居間 = 416
#DIM CONST SPT台所 = 417
#DIM CONST SPT厠 = 418
#DIM CONST SPT脱衣所 = 419
#DIM CONST SPT風呂 = 420
