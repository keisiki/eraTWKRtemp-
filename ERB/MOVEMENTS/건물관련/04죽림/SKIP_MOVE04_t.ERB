#DIM CONST SPT影狼の家 = 432
#DIM CONST SPT斜角の竹林 = 433
#DIM CONST SPT迷いの小道 = 434
#DIM CONST SPT井戸 = 435
#DIM CONST SPT無明の丘 = 436
#DIM CONST SPTメディスンの家 = 437
#DIM CONST SPT太陽の畑 = 438
#DIM CONST SPTラルバの家 = 439
#DIM CONST SPT廃屋 = 440
#DIM CONST SPT温泉 = 441
#DIM CONST SPT空き小屋 = 442
#DIM CONST SPTミスティアの家 = 443
#DIM CONST SPT夜雀の屋台 = 444
#DIM CONST SPT妹紅の家 = 445
#DIM CONST SPT祠 = 446
#DIM CONST SPT竹林入口 = 447

#DIM CONST SPT玄関 = 450
#DIM CONST SPTくるみ私室 = 451
#DIM CONST SPTエリー私室 = 452
#DIM CONST SPTダイニング = 453
#DIM CONST SPTバス = 454
#DIM CONST SPTトイレ = 455
#DIM CONST SPT夢幻館倉庫 = 456
#DIM CONST SPT夢月私室 = 457
#DIM CONST SPT夢幻館廊下 = 458
#DIM CONST SPT幻月私室 = 459
#DIM CONST SPT夢幻館空き部屋 = 460
#DIM CONST SPTリビング = 461
#DIM CONST SPTテラス = 462
#DIM CONST SPT幽香私室 = 463
#DIM CONST SPTバルコニー = 464
#DIM CONST SPTホール = 465
#DIM CONST SPT夢幻館庭 = 466

;현재위치 취득
현재의위치 = CFLAG:MASTER:현재위치
SIF ARG:1
	현재의위치 = ARG:1

;잠정 목적지에 직접 이동할 수 있는 경우
IF CAN_MOVE(LOCAL, ARG) & 1
	;거기에 이동
	RETURN ARG
ENDIF
[IF_DEBUG]
	PRINTFORML 스킵무브 개시	
[ENDIF]

;막다른 골목으로부터 갈 수 있는 장소는 고정이므로, 거기를 목표로 한다
SELECTCASE 현재의위치
CASE SPT来客用トイレ
	RETURN SPT待合室
CASE SPT厠, SPT地下室
	RETURN SPT廊下
CASE SPT風呂
	RETURN SPT脱衣所
CASE SPT物置
	RETURN SPT縁側
CASE SPT倉庫, SPT二階空き部屋
	RETURN SPT二階廊下
CASE SPT兎の洞穴, SPT影狼の家
	RETURN SPT斜角の竹林
CASE SPTメディスンの家
	RETURN SPT無明の丘
CASE SPTラルバの家
	RETURN SPT太陽の畑
CASE SPT温泉
	RETURN SPT迷いの小道
CASE SPTくるみ私室, SPTエリー私室
	RETURN SPT玄関
CASE SPTダイニング, SPTバス, SPTトイレ, SPT夢幻館倉庫, SPT夢幻館空き部屋
	RETURN SPTホール
CASE SPT夢月私室, SPT幻月私室
	RETURN SPT夢幻館廊下
CASE SPTバルコニー
	RETURN SPT幽香私室
ENDSELECT

[IF_DEBUG]
	PRINTFORML 막다른 곳 처리를 통과	
[ENDIF]

;직접 이동할 수 없는 경우,  1개가까운 위치를 잠정 목적지로 다시 설정해 루프
;지점 추가시 요점 보충(전케이스 수정하는 것)
SELECTCASE ARG
	CASE SPT永遠亭入口
		SELECTCASE 현재의위치
		CASE SPT広間 TO SPT台所, SPT庭, SPT裏庭
			RETURN SPT調剤室
		CASE SPT診察室 TO SPT地下室
			RETURN SPT待合室
		CASE SPT兎の洞穴, SPT影狼の家, SPT空き小屋, SPTミスティアの家, SPT夜雀の屋台
			RETURN SPT斜角の竹林
		CASEELSE
			RETURN SPT井戸
		ENDSELECT
	CASE SPT待合室
		SELECTCASE 현재의위치
		CASE SPT調剤室, SPT台所, SPT裏庭
			RETURN SPT診察室
		CASE SPT中廊下 TO SPT地下室
			RETURN SPT廊下
		CASEELSE
			RETURN SPT永遠亭入口
		ENDSELECT
	CASE SPT診察室
		SELECTCASE 현재의위치
		CASE SPT台所, SPT裏庭, SPT居間, SPT広間, SPT庭
			RETURN SPT調剤室
		CASEELSE
			RETURN SPT待合室
		ENDSELECT
	CASE SPT調剤室
		SELECTCASE 현재의위치
		CASE SPT庭
			RETURN SPT裏庭
		CASE SPT中廊下 TO SPT居間, SPT物置
			RETURN SPT台所
		CASE SPT待合室, SPT来客用トイレ, SPT廊下, SPT厠 TO SPT風呂, SPT二階廊下 TO SPT地下室
			RETURN SPT診察室
		CASEELSE
			RETURN SPT永遠亭入口
		ENDSELECT
	CASE SPT来客用トイレ
		RETURN SPT待合室
	CASE SPT廊下
		SELECTCASE 현재의위치
		CASE SPT風呂
			RETURN SPT脱衣所
		CASE SPT倉庫, SPT二階空き部屋
			RETURN SPT二階廊下
		CASE SPT庭, SPT裏庭, SPT物置
			RETURN SPT縁側
		CASE SPTてゐ私室 TO SPT台所
			RETURN SPT中廊下
		CASEELSE
			RETURN SPT待合室
		ENDSELECT
	CASE SPT中廊下
		SELECTCASE 현재의위치
		CASE SPT台所, SPT裏庭, SPT調剤室, SPT物置
			RETURN SPT居間
		CASEELSE
			RETURN SPT廊下
		ENDSELECT
	CASE SPT縁側
		SELECTCASE 현재의위치
		CASE SPT台所, SPT広間, SPT調剤室
			RETURN SPT居間
		CASE SPT裏庭
			RETURN SPT庭
		CASEELSE
			RETURN SPT廊下
		ENDSELECT
	CASE SPTてゐ私室, SPT鈴仙私室
		RETURN SPT中廊下
	CASE SPT空き部屋, SPT永琳私室, SPT輝夜私室
		SELECTCASE 현재의위치
		CASE SPT庭, SPT裏庭, SPT倉庫
			RETURN SPT縁側
		CASEELSE
			RETURN SPT中廊下
		ENDSELECT
	CASE SPT広間
		SELECTCASE 현재의위치
		CASE SPT縁側, SPT物置, SPT庭
			RETURN SPT居間
		CASE SPT待合室, SPT廊下, SPTてゐ私室 TO SPT輝夜私室, SPT厠 TO SPT風呂, SPT二階廊下 TO SPT地下室
			RETURN SPT中廊下
		CASEELSE
			RETURN SPT台所
		ENDSELECT
	CASE SPT居間
		SELECTCASE 현재의위치
		CASE SPT物置, SPT庭
			RETURN SPT縁側
		CASE SPT待合室, SPT廊下, SPTてゐ私室 TO SPT輝夜私室, SPT厠 TO SPT風呂, SPT二階廊下 TO SPT地下室
			RETURN SPT中廊下
		CASEELSE
			RETURN SPT台所
		ENDSELECT
	CASE SPT台所
		SELECTCASE 현재의위치
		CASE SPT庭
			RETURN SPT裏庭
		CASE SPT廊下 TO SPT風呂, SPT物置 TO SPT地下室
			RETURN SPT居間
		CASEELSE
			RETURN SPT調剤室
		ENDSELECT
	CASE SPT厠, SPT脱衣所
		RETURN SPT廊下
	CASE SPT風呂
		RETURN SPT脱衣所
	CASE SPT庭
		SELECTCASE 현재의위치
		CASE SPT待合室, SPT診察室, SPT来客用トイレ TO SPT居間, SPT厠 TO SPT風呂, SPT物置 TO SPT地下室
			RETURN SPT縁側
		CASEELSE
			RETURN SPT裏庭
		ENDSELECT
	CASE SPT裏庭
		SELECTCASE 현재의위치
		CASE SPT中廊下, SPTてゐ私室 TO SPT居間
			RETURN SPT台所
		CASE SPT廊下, SPT縁側, SPT厠 TO SPT風呂, SPT物置 TO SPT地下室
			RETURN SPT庭
		CASEELSE
			RETURN SPT調剤室
		ENDSELECT
	CASE SPT物置
		RETURN SPT縁側
	CASE SPT二階廊下, SPT地下室
		RETURN SPT廊下
	CASE SPT倉庫, SPT二階空き部屋
		RETURN SPT二階廊下

	CASE SPT兎の洞穴, SPT影狼の家
		RETURN SPT斜角の竹林
	CASE SPT斜角の竹林
		SELECTCASE 현재의위치
		CASE SPT廃屋, SPT温泉, SPT妹紅の家 TO SPT竹林入口
			RETURN SPT迷いの小道
		CASEELSE
			RETURN SPT永遠亭入口
		ENDSELECT
	CASE SPT迷いの小道
		SELECTCASE 현재의위치
		CASE SPT兎の洞穴, SPT影狼の家, SPT空き小屋 TO SPT夜雀の屋台
			RETURN SPT斜角の竹林
		CASEELSE
			RETURN SPT井戸
		ENDSELECT
	CASE SPT井戸
		SELECTCASE 현재의위치
		CASE SPT待合室 TO SPT地下室
			RETURN SPT永遠亭入口
		CASE SPTメディスンの家
			RETURN SPT無明の丘
		CASE SPTラルバの家, SPT玄関 TO SPT夢幻館庭
			RETURN SPT太陽の畑
		CASE SPT兎の洞穴, SPT影狼の家, SPT空き小屋 TO SPT夜雀の屋台
			RETURN SPT斜角の竹林
		CASEELSE
			RETURN SPT迷いの小道
		ENDSELECT
	CASE SPT無明の丘
		SELECTCASE 현재의위치
		CASE SPTラルバの家, SPT玄関 TO SPT夢幻館庭
			RETURN SPT太陽の畑
		CASEELSE
			RETURN SPT井戸
		ENDSELECT
	CASE SPTメディスンの家
		RETURN SPT無明の丘
	CASE SPT太陽の畑
		SELECTCASE 현재의위치
		CASE SPTメディスンの家
			RETURN SPT無明の丘
		CASE SPTくるみ私室 TO SPTリビング, SPT幽香私室 TO SPTホール
			RETURN SPT玄関
		CASE SPTテラス
			RETURN SPT夢幻館庭
		CASEELSE
			RETURN SPT井戸
		ENDSELECT
	CASE SPTラルバの家
		RETURN SPT太陽の畑
	CASE SPT廃屋, SPT温泉
		RETURN SPT迷いの小道
	CASE SPT空き小屋, SPTミスティアの家
		SELECTCASE 현재의위치
		CASE SPT祠, SPT竹林入口
			RETURN SPT夜雀の屋台
		CASEELSE
			RETURN SPT斜角の竹林
		ENDSELECT
	CASE SPT夜雀の屋台
		SELECTCASE 현재의위치
		CASE SPT廃屋, SPT妹紅の家
			RETURN SPT竹林入口
		CASEELSE
			RETURN SPT斜角の竹林
		ENDSELECT
	CASE SPT妹紅の家
		SELECTCASE 현재의위치
		CASE SPT夜雀の屋台, SPT廃屋, SPT祠, SPTミスティアの家
			RETURN SPT竹林入口
		CASEELSE
			RETURN SPT迷いの小道
		ENDSELECT
	CASE SPT祠
		SELECTCASE 현재의위치
		CASE SPTミスティアの家, SPT空き小屋
			RETURN SPT夜雀の屋台
		CASE SPT廃屋, SPT妹紅の家
			RETURN SPT竹林入口
		CASEELSE
			RETURN SPT迷いの小道
		ENDSELECT
	CASE SPT竹林入口
		SELECTCASE 현재의위치
		CASE SPTミスティアの家, SPT空き小屋
			RETURN SPT夜雀の屋台
		CASEELSE
			RETURN SPT迷いの小道
		ENDSELECT

	CASE SPT玄関
		SELECTCASE 현재의위치
		CASE SPTダイニング TO SPTバルコニー
			RETURN SPTホール
		CASEELSE
			RETURN SPT太陽の畑
		ENDSELECT
	CASE SPTくるみ私室, SPTエリー私室
		RETURN SPT玄関
	CASE SPTダイニング, SPTバス, SPTトイレ, SPT夢幻館倉庫, SPT夢幻館空き部屋
		RETURN SPTホール
	CASE SPT夢月私室, SPT幻月私室
		RETURN SPT夢幻館廊下
	CASE SPT夢幻館廊下
		SELECTCASE 현재의위치
		CASE SPTバルコニー
			RETURN SPT幽香私室
		CASE SPTテラス, SPT夢幻館庭
			RETURN SPTリビング
		CASEELSE
			RETURN SPTホール
		ENDSELECT
	CASE SPTリビング
		SELECTCASE 현재의위치
		CASE SPT夢月私室, SPT幻月私室, SPT幽香私室, SPTバルコニー
			RETURN SPT夢幻館廊下
		CASE SPT夢幻館庭
			RETURN SPTテラス
		CASEELSE
			RETURN SPTホール
		ENDSELECT
	CASE SPTテラス
		SELECTCASE 현재의위치
		CASE SPTダイニング TO SPT夢幻館空き部屋, SPT幽香私室 TO SPTホール
			RETURN SPTリビング
		CASEELSE
			RETURN SPT夢幻館庭
		ENDSELECT
	CASE SPT幽香私室
		RETURN SPT夢幻館廊下
	CASE SPTバルコニー
		RETURN SPT幽香私室
	CASE SPTホール
		SELECTCASE 현재의위치
		CASE SPTテラス
			RETURN SPTリビング
		CASE SPT夢月私室, SPT幻月私室, SPT幽香私室, SPTバルコニー
			RETURN SPT夢幻館廊下
		CASEELSE
			RETURN SPT玄関
		ENDSELECT
	CASE SPT夢幻館庭
		SELECTCASE 현재의위치
		CASE SPTくるみ私室 TO SPT夢幻館倉庫, SPT夢幻館空き部屋
			RETURN SPT玄関
		CASE SPT夢月私室 TO SPT幻月私室, SPTリビング TO SPTバルコニー
			RETURN SPTテラス
		CASEELSE
			RETURN SPT太陽の畑
		ENDSELECT
	CASEELSE
		RETURN SPT竹林入口
ENDSELECT