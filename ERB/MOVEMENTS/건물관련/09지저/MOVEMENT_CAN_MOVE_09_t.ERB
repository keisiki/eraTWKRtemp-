
;지점 관련성 응답 처리
;CALL		USER
;ARG		0:현재지점, 1:비교처 지점
;RETURN		1:이동가능/안보이는 2:이동 불가/보이는 3:이동가능/보인다
;-----------------------------------------------------------
@CAN_MOVE_9(ARG, ARG:1)
#FUNCTION
#DIM 참조처
참조처 = ARG:1 % 100
SELECTCASE ARG % 100
	;지령전 전
	;2현관, 31 큰 길에 이동 가능
	CASE 1
		SIF 참조처 == 31
			RETURNF 1
		SIF 참조처 == 2
			RETURNF 1
	;현관
	;1지령전 앞, 3 응접실, 4 넓은 방, 51층복도에 이동 가능
	CASE 2
		SIF 참조처 == 1
			RETURNF 1
		SIF 참조처 == 3
			RETURNF 1
		SIF 참조처 == 4
			RETURNF 1
		SIF 참조처 == 5
			RETURNF 1
	;응접실
	;2현관으로 이동 가능
	CASE 3
		SIF 참조처 == 2
			RETURNF 1
	;넓은 방
	;2현관, 51층복도에 이동 가능
	CASE 4
		SIF 참조처 == 2
			RETURNF 1
		SIF 참조처 == 5
			RETURNF 1
	;1층복도
	;2현관, 4 넓은 방, 61층 화장실, 9 주방, 11오쿠방, 12오린방, 13 식당, 14 2층복도, 23 뒷마당, 26 안뜰에 이동 가능
	CASE 5
		SIF 참조처 == 2
			RETURNF 1
		SIF 참조처 == 4
			RETURNF 1
		SIF 참조처 == 6
			RETURNF 1
		SIF 참조처 == 9
			RETURNF 1
		SIF 참조처 == 11
			RETURNF 1
		SIF 참조처 == 12
			RETURNF 1
		SIF 참조처 == 13
			RETURNF 1
		SIF 참조처 == 14
			RETURNF 1
		SIF 참조처 == 23
			RETURNF 1
		SIF 참조처 == 26
			RETURNF 1
	;1층 화장실
	;1층복도에 이동 가능
	CASE 6
		SIF 참조처 == 5
			RETURNF 1
;	CASE 7
;		SIF GROUPMATCH(참조처, 5,14,15)
;			RETURNF 1
;	CASE 8
;		SIF GROUPMATCH(참조처, 5,14,15)
;			RETURNF 1
	;주방
	;51층복도, 10 식료고, 13 식당, 23 뒷마당, 25 장미원에 이동 가능
	CASE 9
		SIF 참조처 == 5
			RETURNF 1
		SIF 참조처 == 10
			RETURNF 1
		SIF 참조처 == 13
			RETURNF 1
		SIF 참조처 == 23
			RETURNF 1
		SIF 참조처 == 25
			RETURNF 1
	;식료고
	;9주방에 이동 가능
	CASE 10
		SIF 참조처 == 9
			RETURNF 1
	;오쿠 방
	;51층복도에 이동 가능
	CASE 11
		SIF 참조처 == 5
			RETURNF 1
	;오린 방
	;51층복도에 이동 가능
	CASE 12
		SIF 참조처 == 5
			RETURNF 1
	;식당
	;51층복도, 9 주방, 23 뒷마당, 26 안뜰에 이동 가능
	CASE 13
		SIF 참조처 == 5
			RETURNF 1
		SIF 참조처 == 9
			RETURNF 1
		SIF 참조처 == 23
			RETURNF 1
		SIF 참조처 == 26
			RETURNF 1
	;2층복도
	;51층복도, 15 발코니,  16창고, 17 2층 화장실,  18코이시방, 19 사토리방, 20 서재, 22 탈의실에 이동 가능
	CASE 14
		SIF GROUPMATCH(참조처, 5,15,16,17,18,19,20,22)
			RETURNF 1
	;발코니,  16창고, 17 2층 화장실,  18코이시방
	;142층복도에 이동 가능
	CASE 15,16,17,18
		SIF 참조처 == 14
			RETURNF 1
	;사토리방
	;142층복도, 20 서재에 이동 가능
	CASE 19
		SIF 참조처 == 14
			RETURNF 1
		SIF 참조처 == 20
			RETURNF 1
	;서재
	;142층복도, 19 사토리방에 이동 가능
	CASE 20
		SIF GROUPMATCH(참조처, 14,19)
			RETURNF 1
	;목욕탕
	;22탈의실에 이동 가능
	CASE 21
		SIF 참조처 == 22
			RETURNF 1
	;탈의실
	;142층복도, 21 목욕탕에 이동 가능
	CASE 22
		SIF 참조처 == 14
			RETURNF 1
		SIF 참조처 == 21
			RETURNF 1
	;뒷마당
	;51층복도, 9 주방, 13 식당, 25 장미원에 이동 가능
	CASE 23
		SIF 참조처 == 5
			RETURNF 1
		SIF 참조처 == 9
			RETURNF 1
		SIF 참조처 == 13
			RETURNF 1
		SIF 참조처 == 25
			RETURNF 1
	;장미원
	;뒷마당에 이동 가능
	CASE 25
		SIF 참조처 == 23
			RETURNF 1
	;안뜰
	;51층복도, 13 식당, 27 작열지옥터 입구에 이동 가능
	CASE 26
		SIF 참조처 == 5
			RETURNF 1
		SIF 참조처 == 13
			RETURNF 1
		SIF 참조처 == 27
			RETURNF 1
	;작열지옥터 입구
	;26안뜰, 28 작열지옥터에 이동 가능
	CASE 27
		SIF 참조처 == 26
			RETURNF 1
		SIF 참조처 == 28
			RETURNF 1
	;작열지옥터
	;27작열지옥터 입구, 29 지저 도시 가장 깊은 곳에 이동 가능
	CASE 28
		SIF 참조처 == 27
			RETURNF 1
		SIF 참조처 == 29
			RETURNF 1
	;지저 도시 가장 깊은 곳
	;28작열지옥터에 이동 가능, 30 지옥의 인공 태양과 직통
	CASE 29
		SIF 참조처 == 30
			RETURNF 3
		SIF 참조처 == 28
			RETURNF 1
	;지옥의 인공 태양
	;29지저 도시 가장 깊은 곳과 직통
	CASE 30
		SIF 참조처 == 29
			RETURNF 1
	;큰 길
	;1지령전 전, 32 뒷골목, 33 술집, 37 빈 가게, 44방법에 이동 가능
	CASE 31
		SIF 참조처 == 1
			RETURNF 1
		SIF 참조처 == 32
			RETURNF 1
		SIF 참조처 == 33
			RETURNF 1
		SIF 참조처 == 37
			RETURNF 1
		SIF 참조처 == 44
			RETURNF 1
	;뒷골목
	;31큰 길, 33 술집, 34 온천, 35 숙소, 36유기의 집, 38 오니의 집합소,  39골목길 뒤에 이동 가능, 1 지령전 전에 일방통행
	CASE 32
		SIF GROUPMATCH(참조처, 1,31,33,34,35,36,38,39)
			RETURNF 1
	;술집
	;31큰 길, 32 뒷골목에 이동 가능
	CASE 33
		SIF 참조처 == 31
			RETURNF 1
		SIF 참조처 == 32
			RETURNF 1
	;온천, 숙소, 유기의 집, 오니의 집합소, 골목길 뒤
	;32뒷골목에 이동 가능
	CASE 34,35,36,38,39
		SIF 참조처 == 32
			RETURNF 1
	;빈 가게
	;31큰 길에 이동 가능
	CASE 37
		SIF 참조처 == 31
			RETURNF 1
	;파르시, 야마메, 키스메의 집
	;45강가에 이동 가능
	CASE 40,41,42
		SIF 참조처 == 45
			RETURNF 1
	;황폐한 집
	;44대로에 이동 가능
	CASE 43
		SIF 참조처 == 44
			RETURNF 1
	;대로
	;31큰 길, 43 황폐한 집, 45 강가에 이동 가능, 46 다리와 직통
	CASE 44
		SIF 참조처 == 46
			RETURNF 3
		SIF 참조처 == 31
			RETURNF 1
		SIF 참조처 == 43
			RETURNF 1
		SIF 참조처 == 45
			RETURNF 1
	;강가
	;40파르시의 집,  41야마메의 집, 42 키스메의 집, 44방법, 46 다리에 이동 가능
	CASE 45
		SIF 참조처 == 40
			RETURNF 1
		SIF 참조처 == 41
			RETURNF 1
		SIF 참조처 == 42
			RETURNF 1
		SIF 참조처 == 44
			RETURNF 1
		SIF 참조처 == 46
			RETURNF 1
	;교
	;31큰 길에 일방통행, 45 강가에 이동 가능, 44방법,  48수혈의 바닥과 직통
	CASE 46
		SIF 참조처 == 44
			RETURNF 3
		SIF 참조처 == 48
			RETURNF 3
		SIF 참조처 == 31
			RETURNF 1
		SIF 참조처 == 45
			RETURNF 1
	;수혈의 바닥
	;46다리와 직통, 49 지상이 보인다
	CASE 48
		SIF 참조처 == 46
			RETURNF 3
		SIF 참조처 == 49
			RETURNF 2
		;SIF 참조처 == 47
		;	RETURNF 1
	;지상
	;일방통행, EX캐릭터 대기용
	CASE 49
		SIF 참조처 == 48
			RETURNF 1
ENDSELECT
RETURNF 0