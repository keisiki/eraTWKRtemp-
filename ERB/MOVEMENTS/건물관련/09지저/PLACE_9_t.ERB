;비자택시에 갈아넣는 장소 ID를 돌려주는 함수
;ARG, 대상ID
;RETURNF, 갈아넣고가 끝난 대상ID
;-----------------------------------------

@MAP_SEARCH_REPLACEMENT_9(ARG)
#FUNCTION
SELECTCASE ARG
	CASE 948
	;환상풍혈
		RETURNF 910
	CASE 940 TO 946
	;지옥의깊은길
		RETURNF 920
	CASE 934
	;옛지옥온천
		RETURNF 940
	CASE 935
	;러브호텔
		RETURNF 950
	CASE 931 TO 939
	;옛지옥큰길
		RETURNF 930
	CASE 901 TO 926
	;지령전
		RETURNF 960
	CASE 927 TO 930
	;작열지옥터
		RETURNF 970
ENDSELECT
RETURNF 0



@MAP9_AREANAME(PLACE_ID)
#FUNCTIONS
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM PLACE_ID
IF MAIN_MAP == 9
	SIF INRANGE(PLACE_ID, 901, 926)
		RETURNF "지령전"
	SIF INRANGE(PLACE_ID, 927, 930)
		RETURNF "옛지옥"
	SIF INRANGE(PLACE_ID, 931, 944)
		RETURNF "구도"
	RETURNF "지저"
ELSE
	SELECTCASE PLACE_ID
	CASE 930, 940, 950
		RETURNF "구도"
	CASE 960
		RETURNF "지령전"
	CASE 970
		RETURNF "옛지옥"
	CASEELSE
		RETURNF "지저"
	ENDSELECT
ENDIF
