
;지점 관련성 응답 처리
;-----------------------------------------------------------
@CAN_MOVE_13(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	;봉당
	CASE 1301
		SIF GROUPMATCH(ARG:1, 1304,1305)
			RETURNF 1
		SIF ARG:1 == MAXROOM
			RETURNF 2
	;거실
	CASE 1302
		SIF GROUPMATCH(ARG:1, 1303,1304)
			RETURNF 1
	;침실
	CASE 1303
		SIF GROUPMATCH(ARG:1, 1302,1305)
			RETURNF 1
	;식사
	CASE 1304
		SIF GROUPMATCH(ARG:1, 1301,1302,1305)
			RETURNF 1
	;응접실
	CASE 1305
		SIF GROUPMATCH(ARG:1, 1303,1304,1301,1306)
			RETURNF 1
	;정원
	CASE 1306
		SIF GROUPMATCH(ARG:1, 1305,1307,1308)
			RETURNF 1
	;창고
	CASE 1307
		SIF GROUPMATCH(ARG:1, 1306)
			RETURNF 1
	;욕실
	CASE 1308
		SIF GROUPMATCH(ARG:1, 1306)
			RETURNF 1
	CASE MAXROOM
		SIF ARG:1 == 1301
			RETURNF 1
ENDSELECT
RETURNF 0