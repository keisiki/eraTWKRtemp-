@MAP_PLACENAME_13(ARG)
#FUNCTIONS
#DIM PLACEID

IF GET_MAPID(ARG) == MAIN_MAP
	SELECTCASE ARG
		CASE 1301 TO (MAXROOM - 1)
			RETURNF STR:(ARG + 8000)
		CASE 1398
			RETURNF OMANEKI_PLACE()
		CASE 1399
			RETURNF "스키마 공간"
		CASE MAXROOM
			RETURNF "주택 밖"
		CASEELSE
			RETURNF "불명"
	ENDSELECT
ELSE
	RETURNF "불명"
ENDIF

@MAP_VIEWING_13(AA,COLOR)
#DIMS REF AA, 0
#DIMS REF COLOR, 0
IF !TFLAG:맵변환
	TFLAG:맵변환 = 1
ENDIF
CALL DRAW_INFORMATIONLINE(@"%MAPNAME:(TFLAG:맵변환)%")
PRINTFORML 
;TRYCALLFORM MAP_OWN1_{TFLAG:맵변환}(AA)

IF RECOLOREDMAPS
	TRYCCALLFORM COLOREDMAP_OWN1_{TFLAG:맵변환}(AA,COLOR)
	CATCH
		CLEARLINE 2
		RECOLOREDMAPS = 0
		CALL DRAW_MAP
		RECOLOREDMAPS = 1
	RETURN
	ENDCATCH
ELSE
	TRYCALLFORM MAP_OWN1_{TFLAG:맵변환}(AA)
ENDIF


