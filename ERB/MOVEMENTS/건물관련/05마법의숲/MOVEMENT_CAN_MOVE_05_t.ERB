
;지점 관련성 응답 처리
;-----------------------------------------------------------
@CAN_MOVE_5(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	;입구
	;2향림당,  34엘렌 마법의 가게에 이동 가능, 3 갈림길과 직통, 숲의 밖이 보인다
	CASE 501
		SIF GROUPMATCH(ARG:1,503)
			RETURNF 3
		SIF GROUPMATCH(ARG:1,502,534)
			RETURNF 1
		SIF ARG:1 == MAXROOM
			RETURNF 2
	;코우린도
	;1마법의숲 입구에 이동 가능
	CASE 502
		SIF ARG:1 == 501
			RETURNF 1
	;갈림길
	;11거실, 8 마법의 광장, 15 창고, 20 인형의 관앞, 40 고목의큰나무에 이동 가능, 1 마법의숲 입구와 직통
	CASE 503
		SIF GROUPMATCH(ARG:1,501)
			RETURNF 3
		SIF GROUPMATCH(ARG:1,508,511,515,520,540)
			RETURNF 1
	;무연총
	;9재고의길과 직통
	CASE 506
		SIF ARG:1 == 509
			RETURNF 3
	;버섯
	;8마법의 광장, 20 인형의 관앞, 40 고목의큰나무에 이동 가능
	CASE 507
		SIF GROUPMATCH(ARG:1,508,520,540)
			RETURNF 1
	;광장
	;3갈림길, 7 버섯 군생지, 9 재고의길,  10키리사메마법점, 44 나루미의 집에 이동 가능
	CASE 508
		SIF GROUPMATCH(ARG:1,503,507,509,510,544)
			RETURNF 1
	;재고
	;8마법의 광장에 이동 가능, 6 무연총과 직통
	CASE 509
		SIF GROUPMATCH(ARG:1,506)
			RETURNF 3
		SIF GROUPMATCH(ARG:1,508)
			RETURNF 1
;마리사하우스
	;마법점
	;8마법의 광장, 11 거실에 이동 가능
	CASE 510
		SIF GROUPMATCH(ARG:1,508,511)
			RETURNF 1
	;거실
	;3갈림길, 10 마법점, 12 침실, 13 연구부가게, 14 목욕탕, 15 창고에 이동 가능
	CASE 511
		SIF GROUPMATCH(ARG:1,503,510,512,513,514,515)
			RETURNF 1
	;각부가게
	;11거실에 이동 가능
	CASE 512, 513, 514
		SIF ARG:1 == 511
			RETURNF 1
	;souko
	;3갈림길, 11 거실에 이동 가능
	CASE 515
		SIF GROUPMATCH(ARG:1,503,511)
			RETURNF 1
;인형의 관
	;관앞
	;3갈림길, 7 버섯 군생지, 21 앨리스의 방, 22신키의 방, 25 리빙, 40 고목의큰나무에 이동 가능
	CASE 520
		SIF GROUPMATCH(ARG:1,503,507,521,522,525,540)
			RETURNF 1
	;목욕탕
	;25리빙에 이동 가능
	CASE 526
		SIF ARG:1 == 525
			RETURNF 1
	;앨리스, 신키의 방
	;20관앞, 25 리빙에 이동 가능
	CASE 521,522
		SIF GROUPMATCH(ARG:1,525,520)
			RETURNF 1
	;유메코의 방, 연구실,
	;25리빙에 이동 가능, 30 2층에는 일방통행
	CASE 523,524
		SIF GROUPMATCH(ARG:1,525,530)
			RETURNF 1
	;리빙
	;20관앞, 21 앨리스의 방, 22신키의 방,  23유메코의 방, 24 연구실, 26 목욕탕, 29 발코니, 30 2층에 이동 가능
	CASE 525
		SIF GROUPMATCH(ARG:1,520,521,522,523,524,526,529,530)
			RETURNF 1
	;창고, 빈 방
	;302층에 이동 가능
	CASE 528,531
		SIF GROUPMATCH(ARG:1,530)
			RETURNF 1
	;발코니
	;25리빙, 30 2층에 이동 가능
	CASE 529
		SIF GROUPMATCH(ARG:1,525,530)
			RETURNF 1
	;2층
	;25리빙, 28 창고, 29 발코니, 31 빈 방에 이동 가능
	CASE 530
		SIF GROUPMATCH(ARG:1,525,528,529,531)
			RETURNF 1
;엘렌 하우스
	;점
	;1입구, 35 엘렌방, 36 창고, 37 목욕탕에 이동 가능
	CASE 534
		SIF GROUPMATCH(ARG:1,501,535,536,537)
			RETURNF 1
	;엘렌방, 창고, 목욕탕
	;34가게에 이동 가능
	CASE 535 TO 537
		SIF ARG:1 == 534
			RETURNF 1
	;고목의큰나무
	;3갈림길, 7 버섯 군생지, 20관앞,  41토키코, 42 리글, 43 루미아의 사는 곳에 이동 가능
	CASE 540
		SIF GROUPMATCH(ARG:1,503,507,520,541,542,543)
			RETURNF 1
	;토키코, 리글, 루미아의 사는 곳
	;40고목의큰나무에 이동 가능
	CASE 541 TO 543
		SIF ARG:1 == 540
			RETURNF 1
	;나루미의 집
	;8마법의 광장에 이동 가능
	CASE 544
		SIF ARG:1 == 508
			RETURNF 1
	;숲의 밖
	;1입구에 일방통행
	CASE MAXROOM
		SIF ARG:1 == 501
			RETURNF 1
ENDSELECT
RETURNF 0