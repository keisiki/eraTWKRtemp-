@GETMAP_5(ARG)
KEHAI = 0
FOR LOCAL, 1, CHARANUM
	IF CFLAG:LOCAL:현재위치 == ARG
		KEHAI = CHECK_HEHAI(LOCAL)
		SIF KEHAI > 1
			BREAK
	ENDIF
	IF ARG == 525
		CALL GETMAP_BUILDING(LOCAL,521,531)
	ELSEIF ARG == 510
		CALL GETMAP_BUILDING(LOCAL,510,515)
	ELSEIF ARG == 534
		CALL GETMAP_BUILDING(LOCAL,534,537)
	ENDIF
	SIF KEHAI > 1
		BREAK
NEXT
CALL SET_KEHAI_COLOR(KEHAI)
