@MAP_SEARCH_REPLACEMENT_5(ARG)
#FUNCTION
SELECTCASE ARG
;숲의입구
	CASE 501,534 TO 537, 544
		RETURNF 510
;향림당
	CASE 502
		RETURNF 520
;고목의큰나무
	CASE 540 TO 545
		RETURNF 530
;키리사메마법점
	CASE 510 TO 515
		RETURNF 540
;인형양관
	CASE 520 TO 533
		RETURNF 550
;무연총
	CASE 506
		RETURNF 570
;재고의길
	CASEELSE
		RETURNF 560
ENDSELECT
RETURNF 0


@MAP5_AREANAME(PLACE_ID)
#FUNCTIONS
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM PLACE_ID
IF MAIN_MAP == 5
	SIF INRANGE(PLACE_ID, 510, 515)
		RETURNF "마리사의 가게"
	SIF INRANGE(PLACE_ID, 520, 531)
		RETURNF "인형양관"
	SIF INRANGE(PLACE_ID, 534, 537)
		RETURNF "엘렌의 가게"
	SIF PLACE_ID == 502
		RETURNF "향림당"
	SIF PLACE_ID == 506
		RETURNF "무연총"
	SIF PLACE_ID == 509
		RETURNF "재고의길"
	;SIF INRANGE(PLACE_ID, 343, 348)
	RETURNF "마법의 숲"
ELSE
	SELECTCASE PLACE_ID
	CASE 520
		RETURNF "향림당"
	CASE 540
		RETURNF "마리사의 가게"
	CASE 550
		RETURNF "인형양관"
	CASE 560
		RETURNF "재고의길"
	CASE 570
		RETURNF "무연총"
	CASEELSE
		RETURNF "마법의 숲"
	ENDSELECT
ENDIF
