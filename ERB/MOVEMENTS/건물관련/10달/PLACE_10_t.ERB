@KICKOUT_10()
#FUNCTION
SELECTCASE CHK_INHOME(CFLAG:MASTER:현재위치)
	CASE 30
		RETURNF 1029
	CASE 54
		RETURNF 1053
	CASE 67
		RETURNF 1066
ENDSELECT

@BATHROOM_10()
#FUNCTION
SELECTCASE CHK_INHOME(CFLAG:MASTER:현재위치)
	CASE 20 TO 49
		RETURNF 30
	CASE 50 TO 59
		RETURNF 54
	CASE 60 TO 72
		RETURNF 67
ENDSELECT
RETURNF 0

@BATHCHECK_10(ARG)
#FUNCTION
SELECTCASE CHK_INHOME(ARG)
	CASE 30, 54, 67
		RETURNF 1
	CASEELSE
		RETURNF 0
ENDSELECT
RETURNF 0

@MAP_UNIQUEMESSAGE_10
IF WITH_MOB()
	IF FLAG:시간정지
		IF GROUPMATCH(CFLAG:MASTER:현재위치, 20, 23)
			PRINTFORML 문지기들이 멈춰 있다
		ELSE
			PRINTFORML 옥토끼들이 멈춰 있다
		ENDIF
	ELSE
		SELECTCASE CFLAG:MASTER:현재위치
		;옥토거리,광장
		CASE 1003, 1004
			PRINTFORML 옥토끼들로 붐빈다
		;복숭아숲
		CASE 1014
			PRINTFORML 옥토끼들이 훈련 중이다
		;綿月亭 門
		CASE 1020
			PRINTFORML 문지기들이 지키고 있다
		;門番詰め所
		CASE 1023
			PRINTFORML 문지기들이 쉬고 있다
		;玉兎食堂
		CASE 1024
			PRINTFORML 옥토끼들이 식사 중이다
		;玉兎詰め所
		CASE 1025
			PRINTFORML 옥토끼들이 쉬고 있다
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

