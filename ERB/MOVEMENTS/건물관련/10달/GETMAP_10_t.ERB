@GETMAP_10(ARG)
KEHAI = 0
FOR LOCAL, 1, CHARANUM
	IF CFLAG:LOCAL:현재위치 == ARG
		KEHAI = CHECK_HEHAI(LOCAL)
		SIF KEHAI > 1
			BREAK
	ENDIF
	IF ARG == 1020
		CALL GETMAP_BUILDING(LOCAL,1020, 1049)
	ELSEIF ARG == 1050
		CALL GETMAP_BUILDING(LOCAL,1050, 1059)
	ELSEIF ARG == 1060
		CALL GETMAP_BUILDING(LOCAL,1060, 1072)
	ENDIF
	SIF KEHAI > 1
		BREAK
NEXT
CALL SET_KEHAI_COLOR(KEHAI)
