@DRAW_MAP_BG_3(SLICED)
#DIMS SLICED
SELECTCASE SLICED
;채색 싶을 때는 이런 느낌
CASE "木"
	SETCOLOR 0x90EE90
	PRINTS SLICED
	RESETCOLOR
;다른 문자를 표시시키는 예
CASE "#W"
	SETCOLOR 0x90EE90
	PRINT 木
	RESETCOLOR
CASE "#A"
	IF HATAKE_NAME:0 != ""
		SETCOLOR C_P_GREEN
		PRINT VV
		RESETCOLOR
	ELSE
		PRINT 
