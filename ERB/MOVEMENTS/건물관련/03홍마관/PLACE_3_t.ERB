@BATHROOM_3(ARG)
#FUNCTION
IF CFLAG:MASTER:현재위치 >= 1 && CFLAG:MASTER:현재위치 <= 33
	RETURNF 333
ELSEIF (CFLAG:MASTER:현재위치 >= 34 &&  CFLAG:MASTER:현재위치 <= 42)
	RETURNF 338
ELSE
	RETURNF 333
ENDIF
RETURNF 0



@MAP_UNIQUEMESSAGE_3
IF WITH_MOB()
	IF FLAG:70
		PRINTFORML 홉고블린이 정지해 있다…
	ELSEIF CFLAG:MASTER:현재위치 == 329
		PRINTFORML 홉고블린이 고양이를 돌보고 있다…
	ELSE
		PRINTFORML 홉고블린이 일하고 있다…
	ENDIF
	RETURN 1
ELSEIF !FLAG:70
	IF CFLAG:MASTER:현재위치 == 318
		PRINTFORML 지하로부터 동물의 울음소리가 들려 온다…
		RETURN 1
	ELSEIF CFLAG:MASTER:현재위치 == 329
		PRINTFORML 고양이가 있다
		RETURN 1
	ENDIF
ENDIF

;-----------------------------------------
;비자택시에 갈아넣는 장소 ID를 돌려주는 함수
;ARG, 대상ID
;RETURNF, 갈아넣고가 끝난 대상ID
;-----------------------------------------
@MAP_SEARCH_REPLACEMENT_3(ARG)
#FUNCTION
SELECTCASE ARG
	CASE 343,346,347,348
		RETURNF 310
	CASE 344,345
		RETURNF 320
	CASE 334 TO 342
		RETURNF 330
	CASE 301 TO 308
		RETURNF 340
	CASE 309,315 TO 330,332,333
		RETURNF 350
	CASE 313,314
		RETURNF 360
	CASE 310 TO 312
		RETURNF 370
	CASE 331
		RETURNF 380
ENDSELECT
RETURNF 0



@MAP3_AREANAME(PLACE_ID)
#FUNCTIONS
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM PLACE_ID
IF MAIN_MAP == 3
	SIF INRANGE(PLACE_ID, 301, 333)
		RETURNF "홍마관"
	SIF INRANGE(PLACE_ID, 334, 342)
		RETURNF "소령의 폐양관"
	;SIF INRANGE(PLACE_ID, 343, 348)
	RETURNF "안개의호수"
ELSE
	SELECTCASE PLACE_ID
	CASE 330
		RETURNF "소령의 폐양관"
	CASE 340, 350, 360, 370, 380
		RETURNF "홍마관"
	CASEELSE
		RETURNF "안개의호수"
	ENDSELECT
ENDIF
