@MAP_UNIQUEMESSAGE_1
VARSET LOCAL
SELECTCASE CFLAG:MASTER:현재위치
	CASE 104
		PRINTFORML 큰 목탁이 놓여져 있다
		RETURN 1
	CASE 109
		PRINTFORML 목각의 곰이 놓여져 있다
		RETURN 1
	CASE 110
		PRINTFORML 처마 끝에 무가 매달려 있다.
		RETURN 1
	CASE 119
		FOR LOCAL,1,CHARANUM
			IF CFLAG:LOCAL:현재위치 == 112
				LOCAL:1 = LOCAL
				LOCAL:2 ++
			ENDIF
		NEXT
		IF !LOCAL:2
			PRINTFORML 목욕탕을 들여다 볼 수 있는 비밀 장치가 있다
		ELSE
			PRINTFORML 비밀 장치를 통해서 입욕중의 %CALLNAME:(LOCAL:1)%\@ LOCAL:2 > 1?  일행이# %조사만처리(CALLNAME:(LOCAL:1),"가")%\@  보인다
			PRINTFORML 절경이네!
		ENDIF
		RETURN 1
	CASEELSE
		
ENDSELECT

;-----------------------------------------
;비자택시에 갈아넣는 장소 ID를 돌려주는 함수
;ARG, 대상ID
;RETURNF, 갈아넣고가 끝난 대상ID
;-----------------------------------------
@MAP_SEARCH_REPLACEMENT_1(ARG)
#FUNCTION
SELECTCASE ARG
	CASE 103,104
		RETURNF 140
	CASE 121
		RETURNF 150
	CASE 101 TO 122
		RETURNF 130
	CASE 123 TO 134
		RETURNF 160
ENDSELECT
RETURNF 0



@MAP1_AREANAME(PLACE_ID)
#FUNCTIONS
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM PLACE_ID
IF MAIN_MAP == 1
	SIF INRANGE(PLACE_ID, 123, 134)
		RETURNF "성덕도장"
	;SIF INRANGE(PLACE_ID, 101, 122)
	RETURNF "명련사"
ELSE
	SELECTCASE PLACE_ID
	CASE 160
		RETURNF "성덕도장"
	CASEELSE
		RETURNF "명련사"
	ENDSELECT
ENDIF
