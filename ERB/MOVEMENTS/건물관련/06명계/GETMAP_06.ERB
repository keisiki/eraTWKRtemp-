@GETMAP_6(ARG)
KEHAI = 0
FOR LOCAL, 1, CHARANUM
	IF CFLAG:LOCAL:현재위치 == ARG
		KEHAI = CHECK_HEHAI(LOCAL)
		SIF KEHAI > 1
			BREAK
	ENDIF
	IF ARG == 610
		CALL GETMAP_BUILDING(LOCAL,610, 619)
	ELSEIF ARG == 620
		CALL GETMAP_BUILDING(LOCAL,620, 629)
	ELSEIF ARG == 630
		CALL GETMAP_BUILDING(LOCAL,630, 639)
	ELSEIF ARG == 640
		CALL GETMAP_BUILDING(LOCAL,640, 659)
	ENDIF
	SIF KEHAI > 1
		BREAK
NEXT
CALL SET_KEHAI_COLOR(KEHAI)
