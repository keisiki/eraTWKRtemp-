
;지역간의 소요시간
@TIME_REQUIRED(출발지역, 행선지)
#FUNCTION
#DIM 출발지역
#DIM 행선지

SELECTCASE 출발지역
	CASE 0
		RETURNF TIME_REQUIRED0(행선지 + 5000)
	CASE 1
		RETURNF TIME_REQUIRED1(행선지 + 5000)
	CASE 2
		RETURNF TIME_REQUIRED2(행선지 + 5000)
	CASE 3
		RETURNF TIME_REQUIRED3(행선지 + 5000)
	CASE 4
		RETURNF TIME_REQUIRED4(행선지 + 5000)
	CASE 5
		RETURNF TIME_REQUIRED5(행선지 + 5000)
	CASE 6
		RETURNF TIME_REQUIRED6(행선지 + 5000)
	CASE 7
		RETURNF TIME_REQUIRED7(행선지 + 5000)
	CASE 8
		RETURNF TIME_REQUIRED8(행선지 + 5000)
	CASE 9
		RETURNF TIME_REQUIRED9(행선지 + 5000)
	CASE 10
		RETURNF TIME_REQUIRED10(행선지 + 5000)
	CASE 13
		RETURNF TIME_REQUIRED13(행선지 + 5000)
	CASE 14
		RETURNF TIME_REQUIRED14(행선지 + 5000)
	CASEELSE
		RETURNF 0
ENDSELECT
@TIME_REQUIRED0(ARG)
#FUNCTION
SELECTCASE ARG
	;신사
	CASE 5000
		RETURNF 0
	;명련사
	CASE 5001
		SIF MAIN_MAP != 1 && ((CHK_DATENOW(CFLAG:56:데이트중) == 1 || TARGET == 56) && CFLAG:56:현재위치 == 성덕도장)
			RETURNF 5
		RETURNF 30
	;인간 마을
	CASE 5002
		RETURNF 20
	;안개의호수~홍마관
	CASE 5003
		RETURNF 50
	;미혹의 죽림
	CASE 5004
		RETURNF 40
	;마법의숲
	CASE 5005
		RETURNF 50
	;삼도천
	CASE 5006
		SIF SHORTCUT(3) && CFLAG:MASTER:현재위치 >= 피안 && CFLAG:MASTER:현재위치 <= 백옥루
			RETURNF 5
		SIF SHORTCUT(7) && CFLAG:MASTER:현재위치 >= 지옥 && CFLAG:MASTER:현재위치 <= 영장원
			RETURNF 5
		SIF SHORTCUT(5) == 1 || SHORTCUT(7) == 1
			RETURNF 80
		RETURNF 70
	;요괴의 산(산록)
	CASE 5007
			RETURNF 40
	;요괴의 산(산정)
	CASE 5008
			SIF SHORTCUT(1) && CFLAG:MASTER:현재위치 >= 모리야신사경내 && CFLAG:현재위치 <= 모리야신사본전
				RETURNF 5
			SIF SHORTCUT(2) && CFLAG:MASTER:현재위치 >= 천계
				RETURNF 5
			SIF SHORTCUT(4) == 1
				RETURNF 50
		RETURNF 60
	;지저
	CASE 5009
		RETURNF 20
	;월
	CASE 5010
			SIF SHORTCUT(1) && CFLAG:MASTER:현재위치 >= 모리야신사경내 && CFLAG:현재위치 <= 모리야신사본전
				RETURNF 25
			SIF SHORTCUT(2) && CFLAG:MASTER:현재위치 >= 천계
				RETURNF 25
			SIF SHORTCUT(4) == 1
				RETURNF 70
		RETURNF 80
	;KR판 오리지날 주택/저택
	CASE 5013,5014
		RETURNF 10
ENDSELECT
