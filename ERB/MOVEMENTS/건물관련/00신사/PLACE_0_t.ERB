;-----------------------------------------
;비자택시에 갈아넣는 장소 ID를 돌려주는 함수
;ARG, 대상ID
;RETURNF, 갈아넣고가 끝난 대상ID
;-----------------------------------------
@MAP_SEARCH_REPLACEMENT_0(ARG)
#FUNCTION
SELECTCASE ARG
	CASE 1,22
		RETURNF 10
	CASE 2,5,11,21
		RETURNF 20
	CASE 3,4
		RETURNF 30
	CASE 6 TO 20
		RETURNF 40
	CASE 23 TO 35
		RETURNF 50
	CASE 36 TO 51
		RETURNF 60
	CASE 52 TO 56
		RETURNF 40
ENDSELECT
RETURNF 0


@MAP0_AREANAME(PLACE_ID)
#FUNCTIONS
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM PLACE_ID
IF MAIN_MAP == 0
	SIF INRANGE(PLACE_ID, 26, 35)
		RETURNF "삼요정의집"
	SIF INRANGE(PLACE_ID, 36, 51)
		RETURNF "몽환유적"
	;SIF INRANGE(PLACE_ID, 1, 25) || INRANGE(PLACE_ID, 52, 56)
	RETURNF "하쿠레이 신사"
ELSE
	SELECTCASE PLACE_ID
	CASE 50
		RETURNF "삼요정의집"
	CASE 60
		RETURNF "몽환유적"
	;CASE 10, 20, 30, 40
	CASEELSE
		RETURNF "하쿠레이 신사"
	ENDSELECT
ENDIF
