@GETMAP_0(ARG)
KEHAI = 0
FOR LOCAL, 1, CHARANUM
	IF CFLAG:LOCAL:현재위치 == ARG
		KEHAI = CHECK_HEHAI(LOCAL)
		SIF KEHAI > 1
			BREAK
	ENDIF
	IF ARG == 26
		CALL GETMAP_BUILDING(LOCAL,26, 35)
	ELSEIF ARG == 36 && !INRANGE(CFLAG:MASTER:현재위치, 36, 51)
		IF CFLAG:LOCAL:현재위치 >= 36 && CFLAG:LOCAL:현재위치 <= 44
			KEHAI = CHECK_HEHAI(LOCAL)
		ENDIF
	ELSEIF ARG == 49 && !INRANGE(CFLAG:MASTER:현재위치, 36, 51)
		IF CFLAG:LOCAL:현재위치 >= 45 && CFLAG:LOCAL:현재위치 <= 51
			KEHAI = CHECK_HEHAI(LOCAL)
		ENDIF
	ENDIF
	SIF KEHAI > 1
		BREAK
NEXT
CALL SET_KEHAI_COLOR(KEHAI)
