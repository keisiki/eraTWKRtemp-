@GETMAP_7(ARG)
KEHAI = 0
FOR LOCAL, 1, CHARANUM
	IF CFLAG:LOCAL:현재위치 == ARG
		KEHAI = CHECK_HEHAI(LOCAL)
		SIF KEHAI > 1
			BREAK
	ENDIF
	IF ARG == 716
		CALL GETMAP_BUILDING(LOCAL,716,719)
	ELSEIF ARG == 729
		CALL GETMAP_BUILDING(LOCAL,720,729)
	ENDIF
	SIF KEHAI > 1
		BREAK
NEXT
CALL SET_KEHAI_COLOR(KEHAI)
