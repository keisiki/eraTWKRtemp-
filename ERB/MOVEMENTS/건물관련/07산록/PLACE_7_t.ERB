;-----------------------------------------
;비자택시에 갈아넣는 장소 ID를 돌려주는 함수
;ARG, 대상ID
;RETURNF, 갈아넣고가 끝난 대상ID
;-----------------------------------------

@MAP_SEARCH_REPLACEMENT_7(ARG)
#FUNCTION
SELECTCASE ARG
	CASE 703,704,735
	;현무의늪
		RETURNF 720
	CASE 708
	;큰두꺼비의연못
		RETURNF 730
	CASE 733
	;세이자은신처앞
		RETURNF 740
	CASE 720 TO 730
	;선인의저택
		RETURNF 750
	CASE 712
	;간헐천지하센터
		RETURNF 760
	CASEELSE
	;산록의수해
		RETURNF 710
ENDSELECT
RETURNF 0



@MAP7_AREANAME(PLACE_ID)
#FUNCTIONS
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM PLACE_ID
IF MAIN_MAP == 7
	SIF INRANGE(PLACE_ID, 715, 719)
		RETURNF "아키 자매의 집"
	SIF INRANGE(PLACE_ID, 720, 730)
		RETURNF "선인의저택"
	RETURNF "요괴의 산의 산록"
ELSE
	SELECTCASE PLACE_ID
	CASE 740
		RETURNF "세이자은신처앞"
	CASE 750
		RETURNF "선인의저택"
	CASEELSE
		RETURNF "요괴의 산의 산록"
	ENDSELECT
ENDIF
