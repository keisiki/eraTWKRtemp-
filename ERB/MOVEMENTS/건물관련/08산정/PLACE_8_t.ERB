@KICKOUT_8()
#FUNCTION
SELECTCASE CHK_INHOME(CFLAG:MASTER:현재위치)
	CASE 25
		RETURNF 822
	CASE 37
		RETURNF 834
ENDSELECT

@MAP_UNIQUEMESSAGE_8
IF WITH_MOB()
	RETURN 1
ELSE
	SELECTCASE CFLAG:MASTER:현재위치
		CASE 803
			IF INRANGE(TIME, 540, 1020)
				PRINTFORML 보기에도 의욕이 없는 것 같은 캇파가 삭도의 접수를 하고 있다
			ELSE
				PRINTFORML 삭도역은 닫혀 있다
			ENDIF
	ENDSELECT
ENDIF
RETURN 1
;-----------------------------------------
;비자택시에 갈아넣는 장소 ID를 돌려주는 함수
;ARG, 대상ID
;RETURNF, 갈아넣고가 끝난 대상ID
;-----------------------------------------

@MAP_SEARCH_REPLACEMENT_8(ARG)
#FUNCTION
SELECTCASE ARG
	CASE 830 TO 850
	;천계
		RETURNF 890
	CASE 801,802,803,806 TO 809
	;롱
		RETURNF 810
	CASE 805
	;구
		RETURNF 820
	CASE 814
	;호
		RETURNF 830
	CASE 804,811,813,815
	;경내
		RETURNF 840
	CASEELSE
	;본전
		RETURNF 850
ENDSELECT
RETURNF 0



@MAP8_AREANAME(PLACE_ID)
#FUNCTIONS
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM PLACE_ID
IF MAIN_MAP == 8
	SIF PLACE_ID == 804 || INRANGE(PLACE_ID, 811, 826)
		RETURNF "모리야신사"
	SIF PLACE_ID == 805 || INRANGE(PLACE_ID, 830, 842)
		RETURNF "천계"
	;SIF INRANGE(PLACE_ID, 801, 810)
	RETURNF "요괴의 산의 산정"
ELSE
	SELECTCASE PLACE_ID
	CASE 890
		RETURNF "천계"
	;산의호수는 모리야신사의 부지내에 포함있습니다
	CASE 830, 840, 850
		RETURNF "모리야신사"
	CASEELSE
		RETURNF "요괴의 산의 산정"
	ENDSELECT
ENDIF
