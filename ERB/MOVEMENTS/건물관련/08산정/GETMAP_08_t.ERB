@GETMAP_8(ARG)
KEHAI = 0
SIF ROOMDATA:(ARG % 100) & 장소_모브
	SETCOLOR C_D_PINK
FOR LOCAL, 1, CHARANUM
	IF CFLAG:LOCAL:현재위치 == ARG
		KEHAI = CHECK_HEHAI(LOCAL)
		SIF KEHAI > 1
			BREAK
	ENDIF
	IF ARG == 830
		CALL GETMAP_BUILDING(LOCAL,830,842)
	ELSEIF ARG == 811
		CALL GETMAP_BUILDING(LOCAL,811,826)
	ENDIF
	SIF KEHAI > 1
		BREAK
NEXT
CALL SET_KEHAI_COLOR(KEHAI)
