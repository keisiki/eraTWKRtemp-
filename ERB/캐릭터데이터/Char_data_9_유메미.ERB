	SELECTCASE DAY:2
	CASE 1
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("외투", "케이프"))
	CASE 3
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("외투", "망토"))
