@CHARAMOVE_DATA_124(ARG,ARG:2)
LOCAL = 124
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;특히 이루어
			CASEELSE
				RETURN 0
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 5, 6, 8
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 21, 22, 23, 51, 52, 53, 54, 56, 57, 61, 62, 63, 81, 82, 83
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터124
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터124(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "오키나")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "관모"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "롱 부츠"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "도사옷"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "밤하늘을 투영한")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "별의 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "별의 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:3
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "쌀/조림/건강한/호화/예술적/품위있는/말랑말랑/매운/매운/시큼한/시큼한/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "검소/짠/짠/" + "개구리/벌레고기/수수께끼 요리/" * 3 + "텐구류/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "인챈트/가호/현란/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "노랑빛/고급/혼돈/아름다운/예술적/레트로/화려/지적/위엄/" + "주술/미술품/사계의/" * 3 + "칠성의/후호(後戸)의/숨겨진/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "서양풍/시크/로리타/" + "싸구려/" * 3 + "텐구의/바캉스의/" * 10)
ENDSELECT

