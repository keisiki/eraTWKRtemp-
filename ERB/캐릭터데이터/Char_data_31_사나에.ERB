@CHARAMOVE_DATA_31(ARG,ARG:2)
LOCAL = 31
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;사람이 있는 집의 목욕탕이라든지 화장실에는 가지 않는다
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 51
				RETURN -100
			;모리야신사분사에 자주(잘) 있다
			CASE 21
				RETURN 10
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2, 7, 8
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,4, 21,22,23,27, 71,72,73,76, 81,82,83,84,85
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터31
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터31(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "사나에")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")
CASE "특수 의상 착용"
	;봄이나 여름에 1/7로 세라복 착용
	SIF (DAY:2 == 1 || DAY:2 == 2) && !RAND:7
		CALLF MAKE_STR(V_NAME, 74)
CASE "평상복：액세서리"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("액세서리", "소매"))
CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "머리 장식"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "니삭스"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
	ENDIF
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "롱 스커트"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "모리야의 무녀옷"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "사나에 비장의")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "사이드 리본 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "프론트 훅 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(3 - (2 * 승부속옷판정([[사나에]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "쌀/생선 요리/일식/튀겨짐/매운/정키/농후/말랑말랑/외계풍/단/단/단/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "시큼한/시큼한/요력/요력/개구리/개구리/개구리/벌레고기/벌레고기/벌레고기/" + "그로테스크/수수께끼 요리/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "기적/외계풍/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "초록빛/열매/질서/귀여운/멋쟁이/팝/미래/화려/불가사의/장대/매니악/" + "빛/신령/주술/부적/개구리의/뱀의/객성의/" * 3 + "무녀의/기적의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "혼돈/심플/레트로/시크/" + "요괴/손목호구/담뱃대/" * 3 + "博霊の/요괴의/" * 10)

;농민 관련
;신사는 토지를 가지고 있음、칸다
CASE "농민：설명"
	CALLF MAKE_STR(V_NAME, "쌀 농가")
CASE "농민：작물"
	CALLF MAKE_STR(V_NAME, "쌀")
CASE "농민：밭의 종류"
	CALLF MAKE_STR(V_NAME, "논")
CASE "농민：밭의 맵"
	CALLF MAKE_STR(V_NAME, "모리야 신사")

CASE "농민：맵 ID"
	CALLF MAKE_INT(V_NAME, 8)
CASE "농민：장소 ID"
	CALLF MAKE_INT(V_NAME, 814)
CASE "농민：보수"
	CALLF MAKE_INT(V_NAME, 500)
CASE "농민：금리"
	CALLF MAKE_INT(V_NAME, 5)
ENDSELECT

;IMAGE_I31_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I31_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF EQUIP:CHARA:상반신윗도리1 == 2
			RESULTS = _セーラー
	ENDIF
ENDIF

