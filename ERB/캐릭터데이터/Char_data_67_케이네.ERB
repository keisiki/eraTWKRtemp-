@CHARAMOVE_DATA_67(ARG,ARG:2)
LOCAL = 67
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;사람이 있는 집의 목욕탕이라든지 화장실에는 가지 않는다
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;개인 방에는 못 들어간다
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;히에다 저택, 자기 방, 서당, 광장
			CASE 221, 224, 222, 202
				RETURN 5
			;당신의 방
			CASE 230
				IF TALENT:LOCAL:연모
					RETURN 5
				ELSE
					RETURN -100
				ENDIF
			;가지 않는다
			CASE 226, 220, 225, 236, 237
				RETURN -100
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 4
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23,24,27, 41,42,43,45
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

