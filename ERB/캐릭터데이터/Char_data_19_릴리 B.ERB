@CHARAMOVE_DATA_19(ARG,ARG:2)
LOCAL = 19
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;삼요정 집에 가기 쉽다
			CASE 26 TO 34
				RETURN 5
			;아지트, 액가, 슈우카, 카센가, 은둔지, 오두막
			CASE 704, 706, 715 TO 719, 720 TO 730, 733, 734
				RETURN -100
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 2
				SIF DAY:2 == 1
					RETURN 1
				RETURN 0
			CASE 5, 6, 7
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23, 56,57, 61,62, 71,72,73
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터19
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터19(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "릴리 B")
