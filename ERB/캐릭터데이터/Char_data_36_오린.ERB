@CHARAMOVE_DATA_36(ARG,ARG:2)
LOCAL = 36
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;개인 방에는 못 들어간다
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			CASE 18, 43, 44, 51, 52
				RETURN -100
			;오쿠 이외의 개인 방이나 사용하지 않는 방
			CASE 903, 904, 910, 916, 918, 919, 920, 925, 930, 935, 936, 937, 938, 940, 941, 942, 943
				RETURN -100
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 1, 7, 9
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,4, 15, 76, 91,92,93,96,97
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

