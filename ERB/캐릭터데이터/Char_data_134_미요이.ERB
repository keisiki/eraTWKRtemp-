@CHARAMOVE_DATA_134(ARG,ARG:2)
LOCAL = 134
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 수면시간(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;ひとんちの風呂とかトイレにはいかない
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;私室には入らない
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;鯢呑亭(仕事があるので10までいかない）
			CASE 238
				RETURN 5
			;広場
			CASE 202
				RETURN 5
			;酒屋, 식료품점, 야채가게
			CASE 211, 216, 227
				RETURN 10
			;自室はあまり居ない
			CASE 239
				RETURN -5
			;あなたの部屋
			CASE 230
				IF TALENT:LOCAL:연모
					RETURN 5
				ELSE
					RETURN -100
				ENDIF
			;他のお店とかは行かない
			CASE 212, 213, 217, 218, 220, 221, 222, 223, 224, 225, 226, 228, 229, 231 TO 237
				RETURN -100
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 1, 2
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 1, 2, 3, 4, 11, 12, 13, 14, 21, 22, 23
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터134
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터134(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "미요이")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지＋후드) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "모자"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "와이셔츠"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "롱 스커트"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "작업화"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "수상쩍은")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "고래 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "고래붕새 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(3 - (2 * 승부속옷판정([[미요이]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 3)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "야채/생선 요리/일식/조림/안주/푸짐/건강한/검소/예술적/가정적/쫄깃한/단/단/짠/짠/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "싸구려식재/정키/시큼한/시큼한/치즈/치즈/치즈/수수께끼 요리/수수께끼 요리/수수께끼 요리/수수께끼 요리/수수께끼 요리/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "프로의 맛/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "푸른빛/귀여운/팝/장대/" + "물/술/식기/바다의/記憶の/" * 3 + "고래의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "서양풍/로리타/" + "천둥/" * 3 + "忘却の/" * 10)
ENDSELECT

