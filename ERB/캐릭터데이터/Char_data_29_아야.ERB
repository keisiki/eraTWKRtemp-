	;差し替え画像
	IF FLAG:스탠딩종류
		;顔絵
	ELSE
		;立ち絵
	ENDIF
ELSE
	;通常画像
	IF INRANGE(CFLAG:CHARA:현재위치, 200, 299) && !CHECK_NUDE(CHARA)
		SIF CHECK_BLUSH(CHARA, @"%GET_BASESTYLE(CHARA)%", "_人里")
			RESULTS = 赤面
