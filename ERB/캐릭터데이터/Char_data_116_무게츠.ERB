@CHARAMOVE_DATA_116(ARG,ARG:2)
LOCAL = 116
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;영원정 안으로는 가지 않는다
			CASE 405, 407 TO 430
				RETURN -100
			;남의 집에는 들어가지 않는다
			CASE 432, 437, 439, 440, 442, 443, 445
				RETURN -100
			;倉庫にはあまり行かない
			CASE 456
				RETURN -5
			;ダイニング,リビング,自室によく居る
			CASE 453, 461, 457
				RETURN 5
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 4
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 41, 42, 47, 49
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터116
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터116(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "무게츠")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(네글리제) 세트")

CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "니삭스"))
CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "화이트브림"))
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "에이프런 드레스"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "그야말로 메이드 같은")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "꽃무늬 흰색 레이스 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "꽃무늬 흰색 레이스 브라")
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "빵/알/양식/국물/케이크/시원한/담백/호화/품위있는/선명한/푹신푹신/몽환풍/단/단/매운/매운/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "야취/짠/짠/" + "개구리/벌레고기/스파이스/" * 3 + "수수께끼 요리/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "몽환풍/메이드류/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "푸른빛/혼돈/귀여운/서양풍/시크/위엄/" + "요술/금단의/" * 3 + "메이드의/몽환의/악마의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "질서/일본풍/화려/" + "신령/" * 3)
ENDSELECT

