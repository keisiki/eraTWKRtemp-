@CHARAMOVE_DATA_7(ARG,ARG:2)
LOCAL = 7
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
		;연동 처리(세 명중제일 높은 캐릭터에 맞춘다)
		IF TALENT:7:연모 || (TALENT:7:사모 && !TALENT:6:사모 && !TALENT:5:사모)
			CFLAG:LOCAL:현재위치 = CFLAG:7:만난위치 ?  CFLAG:7:만난위치 # SUKIMA()
		ELSEIF TALENT:6:연모 || (TALENT:6:사모 && (!TALENT:7:사모 || !TALENT:5:사모))
			CFLAG:LOCAL:현재위치 = CFLAG:6:만난위치 ?  CFLAG:6:만난위치 # SUKIMA()
		ELSE
			CFLAG:LOCAL:현재위치 = CFLAG:5:만난위치 ?  CFLAG:5:만난위치 # SUKIMA()
		ENDIF
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		;1시~6시
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			CASEELSE
				RETURN 0
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 3, 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,4,5, 31,34, 51 TO 57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터7
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터7(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "스타")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(네글리제) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "리본"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "원피스"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "별 마크가 수놓인")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "원포인트 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "하프 톱 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(1 + (2 * 승부속옷판정([[스타]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

CASE "고유커맨드"
	CALLF MAKE_STR(V_NAME, "기척을찾는다")
CASE "고유커맨드 실행 가능"
	SIF CFLAG:7:호감도 >= 200 && GET_MAPID(CFLAG:7:현재위치) == MAIN_MAP
		CALLF MAKE_INT(V_NAME, 1)

;농민 관련
;무리인 스타후르츠로부터 오크라(별모양 단면)로 변경
;오크라는 영어였는가···
CASE "농민：설명"
	CALLF MAKE_STR(V_NAME, "오크라 농가")
CASE "농민：작물"
	CALLF MAKE_STR(V_NAME, "오크라")
CASE "농민：밭의 종류"
	CALLF MAKE_STR(V_NAME, "오크라밭")
CASE "농민：밭의 맵"
	CALLF MAKE_STR(V_NAME, "하쿠레이 신사")

CASE "농민：맵 ID"
	CALLF MAKE_INT(V_NAME, 0)
CASE "농민：장소 ID"
	CALLF MAKE_INT(V_NAME, 25)
CASE "농민：보수"
	CALLF MAKE_INT(V_NAME, 200)
CASE "농민：금리"
	CALLF MAKE_INT(V_NAME, 3)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "생선 요리/일식/나베/건강한/예술적/품위있는/가정적/말랑말랑/반짝임/단/단/시큼한/시큼한/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "매운/야취/그로테스크/그로테스크/그로테스크/수수께끼 요리/수수께끼 요리/수수께끼 요리/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "숨김맛/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "푸른빛/열매/귀여운/서양풍/시크/로리타/" + "별/도서/" * 3 + "별의/별빛의/トリニタリアンな/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "어둠/해/달/생체품/손목호구/담뱃대/" * 3)
ENDSELECT

;IMAGE_I7_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I7_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF DAY:2 == 2
			RESULTS = _夏服
	ENDIF
ENDIF

