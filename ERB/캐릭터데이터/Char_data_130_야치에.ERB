@CHARAMOVE_DATA_130(ARG,ARG:2)
LOCAL = 130
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;是非曲直庁、冥界、八雲邸には行かない（入り口部分は出る時用）
			CASE 610, 613, 620, 621, 630, 631
				RETURN -10
			CASE 610 TO 639
				RETURN -100
			;남의 집에는 들어가지 않는다
			CASE 608, 609
				RETURN -100
			;霊長園には行かない
			CASE 642 TO 645
				RETURN -100
			;특히 이루어
			CASEELSE
				RETURN 0
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2, 5, 6
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 1, 2, 3, 4, 21, 22, 23, 51, 53, 54, 56, 57, 61, 62, 64, 67, 68
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT
