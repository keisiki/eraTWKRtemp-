@CHARAMOVE_DATA_85(ARG,ARG:2)
LOCAL = 85
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;사람이 있는 집의 목욕탕이라든지 화장실에는 가지 않는다
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;개인 방에는 못 들어간다
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;명련사 멤버의 방(강당과 창고 제외하다)
			CASE 113, 114, 115, 117, 118
				RETURN -5
			;별채와 묘지와 신령묘내부
			CASE 119, 121, 125 TO 133
				RETURN -100
			;거실에 가기 쉽다
			CASE 108
				RETURN 5
			;뱌쿠렌이 본당에 있을 때 유행해 온다
			CASE 103
				SIF CFLAG:55:현재위치 == 103
					RETURN 10
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 2
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 11 TO 15, 21,22
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터85
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터85(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "이치린")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "두건"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "법의"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "말향 냄새가 나는")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "낡은 무명 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "낡은 무명 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(1 + (2 * 승부속옷판정([[이치린]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 3)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "쌀/스파이스/일식/나베/단/시원한/건강한/담백/검소/가정적/호쾌/푹신푹신/" + "짠/" * 3)
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "양식/품위있는/외계풍/시큼한/시큼한/개구리/개구리/개구리/벌레고기/벌레고기/벌레고기/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "담백한 맛/특기요리＊푹신푹신＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "푸른빛/흰빛/질서/아름다운/파워풀/" + "바람/법구/" * 3 + "구름의/天輪の/법륜/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "혼돈/섬세/로리타/매니악/" + "흙/" * 3 + "거미의/" * 5)
ENDSELECT

