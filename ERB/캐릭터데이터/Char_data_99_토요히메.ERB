@CHARAMOVE_DATA_99(ARG,ARG:2)
LOCAL = 99
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;사람이 있는 집의 목욕탕이라든지 화장실에는 가지 않는다
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;개인 방에는 못 들어간다
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;몽환관에는 가지 않는다
			CASE 450
				RETURN -10
			CASE 450 TO 466
				RETURN -100
			;남의 집에는 들어가지 않는다
			CASE 432, 437, 439, 440, 442, 443, 445
				RETURN -100
			;物置,倉庫,地下室は行かない
			CASE 424, 427, 430
				RETURN -100
			;縁側,居間によく居る
			CASE 409, 416
				RETURN 5
			;静かの海の方へは行かない
			CASE 1016, 1060 TO 1072
				RETURN -100
			;ドレミー宅には行かない
			CASE 1050 TO 1059
				RETURN -100
			;空き家には行かない
			CASE 1007
				RETURN -100
			;詰め所、食料庫は行かない
			CASE 1023 TO 1025, 1028
				RETURN -100
			;書斎によく居る
			CASE 1043
				RETURN 5
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 10
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 105 TO 108
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터99
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터99(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "토요히메")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "모자"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "살로페트 스커트"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "프릴 블라우스"))
CASE "평상복：그외1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외1", "새시 벨트"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "영원불괴의")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "F섬유 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "F섬유 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:3
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 3)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "생선 요리/나베/건강한/담백/품위있는/야취/가정적/말랑말랑/달도시풍/달구경/맛있는물/중화/중화/단/단/매운/매운/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "양식/정키/시큼한/시큼한/" + "개구리/벌레고기/그로테스크/" * 3 + "수수께끼 요리/생명력/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "달도시풍/색배합/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "보랏빛/질서/아름다운/예술적/대륙풍/섬세/지적/" + "물/달/주술/부적/부채/" * 3 + "팸토파이버의/복숭아의/해신의/바다의/때묻지 않은/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "혼돈/기묘/파워풀/로리타/매니악/" + "흙/어둠/사생/생체품/손목호구/" * 3 + "지상의/판데모닉한/" * 10)
ENDSELECT

