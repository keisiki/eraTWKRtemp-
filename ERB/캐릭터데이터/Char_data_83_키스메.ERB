@CHARAMOVE_DATA_83(ARG,ARG:2)
LOCAL = 83
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;좁은 곳이 좋아
			CASE 3, 5, 6, 12, 14, 16, 17, 18, 23, 38, 40, 51, 53,
				RETURN 10
			;가지 않는다
			CASE 901 TO 930, 935, 936, 937, 939, 940, 941, 943
				RETURN -100
			;큰 길과 술집에 가기 쉽다
			CASE 931, 933
				RETURN 5
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 9
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 91,92,93
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터83
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터83(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "키스메")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "머리 장식"))
CASE "평상복：기모노"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("기모노", "기모노"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "작고 사랑스러운")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "분홍색 훈도시")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 착용 조건"
	SIF !RAND:(1 + (2 * 승부속옷판정([[키스메]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 0)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "고기 요리/일식/튀겨짐/시원한/정키/담백/검소/야취/그로테스크/뭉근뭉근/지저풍/단/단/시큼한/시큼한/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "푸짐/매운/매운/스파이스/스파이스/스파이스/수수께끼 요리/수수께끼 요리/수수께끼 요리/수수께끼 요리/수수께끼 요리/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "지저풍/몰카맛/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "흑빛/혼돈/귀여운/레트로/시크/" + "불/물/뼈/생체품/" * 3 + "지저의/뼈로만든/해골의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "화려/파워풀/위엄/" + "손목호구/기계/" * 3)
ENDSELECT

