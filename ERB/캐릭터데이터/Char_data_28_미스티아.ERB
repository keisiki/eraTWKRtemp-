@CHARAMOVE_DATA_28(ARG,ARG:2)
LOCAL = 28
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;접객업이므로 사람이 있는 집의 목욕탕이라든지 화장실에는 가지 않는다
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 51
				RETURN -100
			;몽환관에는 가지 않는다
			CASE 450
				RETURN -10
			CASE 450 TO 466
				RETURN -100
			;영원정 안으로는 가지 않는다
			CASE 405, 407 TO 430
				RETURN -100
			;남의 집에는 들어가지 않는다
			CASE 432, 437, 439, 440, 442, 445
				RETURN -100
			;자기 방으로부터의 거리에 의존
			CASE 443, 444
				RETURN 5
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 2, 4, 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 11,12,13, 21,22,23, 41,42,43,44,46,47, 51,52,53,56,57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터28
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터28(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "미스티아")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "모자"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "니삭스"))
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "원피스"))
CASE "고유커맨드"
	CALLF MAKE_STR(V_NAME, "포장마차에서식사")
CASE "고유커맨드 실행 가능"
	CALLF MAKE_INT(V_NAME, CFLAG:28:행동 == 5 && BASE:28:일량 < 3000)

;쏭(끈 수영복)＝T 백
CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "날개를 본뜬")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "쏭 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "하프컵 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(3 - (2 * 승부속옷판정([[미스티아]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "생선 요리/조림/안주/농후/야취/가정적/선명한/사각사각/단/단/짠/짠/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "튀겨짐/매운/매운/알/알/알/" + "새고기/" * 10)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "특기요리＊생선 요리＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "흑빛/귀여운/팝/" + "어둠/음악/식기/" * 3 + "참새의/독나방의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "빛/탄환/손목호구/기계/" * 3 + "정령은의/" * 10)
ENDSELECT

;IMAGE_I28_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I28_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF CFLAG:CHARA:직종 == 44 && BASE:CHARA:일량 > 0
			RESULTS = _女将
	ENDIF
ENDIF

