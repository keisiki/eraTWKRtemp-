@CHARAMOVE_DATA_39(ARG,ARG:2)
LOCAL = 39
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;개인 방에는 못 들어간다
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			CASE 18, 43, 44, 51, 52
				RETURN -100
			;명련사 멤버의 방(강당과 창고 제외하다)
			CASE 114, 115, 116, 117, 118
				RETURN -5
			;별채와 묘지와 신령묘내부
			CASE 119, 121, 125 TO 133
				RETURN -100
			;거실에 가기 쉽다
			CASE 108
				RETURN 5
			;뱌쿠렌이 본당에 있을 때 유행해 온다
			CASE 103
				SIF CFLAG:55:현재위치 == 103
					RETURN 10
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 2, 5, 6
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 11 TO 15, 21,22,23, 51 TO 57, 61,62
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

