@CHARAMOVE_DATA_68(ARG,ARG:2)
LOCAL = 68
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;영원정 안으로는 가지 않는다
			CASE 405, 407 TO 430
				RETURN -100
			;남의 집에는 들어가지 않는다
			CASE 432, 437, 439, 440, 442, 443, 445
				RETURN -100
			;倉庫にはあまり行かない
			CASE 456
				RETURN -5
			;太陽の畑,ダイニング,リビング,庭によく居る
			CASE 438, 453, 461, 466
				RETURN 5
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 4, 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 41,42,43,46,47, 51,53,56,57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

