@CHARAMOVE_DATA_125(ARG,ARG:2)
LOCAL = 125
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;命蓮寺メンバーの私室（経堂と物置除く）
			CASE 113, 114, 115, 116, 117, 118
				RETURN -5
			;離れと墓地と神霊廟内部
			CASE 119, 121, 125 TO 133
				RETURN -100
			;居間と本堂に行きやすい
			CASE 108
				RETURN 5
			;白蓮が本堂にいる時はやってくる
			CASE 103
				SIF  CFLAG:55:현재위치 == 103
					RETURN 10
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 2
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 9, 11, 12, 13, 14, 16, 21, 22, 23, 27
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

