@CHARAMOVE_DATA_89(ARG,ARG:2)
LOCAL = 89
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
		RETURN 0
	;이동 확률
	CASE 2
		;악천후시, 밖이라면 이동하기 쉽고
		SIF OUTROOF(CFLAG:(NO:89):현재위치) && ((TIME:5 & 4) || (TIME:5 & 8))
			RETURN 10
		RETURN 110
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;명련사 멤버의 방(강당과 창고 제외하다)
			CASE 113, 114, 115, 116, 118
				RETURN -100
			;신령묘 멤버의 방
			CASE 125, 127, 128
				RETURN -100
			;별채
			CASE 119
				RETURN -100
			;묘지에 자주(잘) 있다
			CASE 121
				RETURN 5
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 1
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 11,12,13,15,16
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터89
#LOCALSIZE 1
#LOCALSSIZE 1

