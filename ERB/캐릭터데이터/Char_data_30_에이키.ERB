@CHARAMOVE_DATA_30(ARG,ARG:2)
LOCAL = 30
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;사람이 있는 집의 목욕탕이라든지 화장실에는 가지 않는다
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;개인 방에는 들어가지 않는다
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;코마치의 방
			CASE 616
				RETURN -5
			;야쿠모집 , 축생계는 가지 않는다(입구 부분은 나올 때 용)
			CASE 630, 631, 640, 641
				RETURN -10
			CASE 630 TO 639, 640 TO 652
				RETURN -100
			;타인의 집, 타택의 방등에는 들어가지 않는다
			CASE 608, 609, 623, 624, 627
				RETURN -100
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 2, 6
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 11,12,13, 21,22,23,27, 61,62,64,65,66,67
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT
