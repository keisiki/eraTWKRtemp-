@CHARAMOVE_DATA_69(ARG,ARG:2)
LOCAL = 69
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;명련사 멤버의 방(강당과 창고 제외하다)
			CASE 113, 114, 115, 116, 118
				RETURN -100
			;신령묘 멤버의 방
			CASE 125, 127, 128
				RETURN -100
			;별채와 묘지, 그외가지 않는 장소
			CASE 119, 121, 103, 122, 131, 132
				RETURN -100
			;응접실에 가기 쉽다
			CASE 109, 129
				RETURN 5
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 2
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 11,12,13,15,16, 21,22,23,27
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

