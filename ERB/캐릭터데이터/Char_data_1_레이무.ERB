@CHARAMOVE_DATA_1(ARG,ARG:2)
LOCAL = 1
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		;23시~6시, 13시~14시는 자고 있는 업무중은 잠을 자지 않다
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;주인의 경내, 툇마루에 있다
			CASE 2, 10
				RETURN 5
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2
				RETURN 1
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,3,4, 21,22,23,27
				RETURN 1
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터1
