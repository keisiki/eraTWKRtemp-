@CHARAMOVE_DATA_118(ARG,ARG:2)
LOCAL = 118
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;3 요정 가정에 가기 쉽다
			CASE 26 TO 34
				RETURN 5
			;夢幻館には庭以外行かない
			CASE 450
				RETURN -10
			CASE 450 TO 465
				RETURN -100
			;영원정 안으로는 가지 않는다
			CASE 405, 407 TO 430
				RETURN -100
			;남의 집에는 들어가지 않는다
			CASE 432, 437, 440, 442, 443, 445
				RETURN -100
			;自宅,無明の丘,太陽の畑,夢幻館庭によく居る
			CASE 439, 436, 438, 466
				RETURN 5
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 3, 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 31, 32, 34, 51 TO 57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터118
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터118(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "라바")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "원피스"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "벽색에 반짝이는")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "인분(나비가루) 염색 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "캐미숄")
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "이파리/드링크/시원한/여름철/예술적/야취/선명한/말랑말랑/야채/야채/단/단/단/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "푸짐/짠/짠/고기 요리/고기 요리/고기 요리/생선 요리/생선 요리/생선 요리/" + "벌레고기/" * 10)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "특기요리＊여름철＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "노랑빛/귀여운/팝/화려/로리타/" + "벌레/" * 3  + "호랑나비의/해바라기의/여름의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "시크/지적/" + "얼음/새/담뱃대/" * 3 + "거미줄의/거미의/겨울의/" * 10)
ENDSELECT

