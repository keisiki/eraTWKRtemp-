@CHARAMOVE_DATA_77(ARG,ARG:2)
LOCAL = 77
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
		;연동 처리(양자가 높을 때는 양자를 참조)
		IF TALENT:78:연모 || (TALENT:78:사모 && !TALENT:77:사모)
			CFLAG:LOCAL:현재위치 = CFLAG:78:만난위치 ?  CFLAG:78:만난위치 # SUKIMA()
		ELSE
			CFLAG:LOCAL:현재위치 = CFLAG:77:만난위치 ?  CFLAG:77:만난위치 # SUKIMA()
		ENDIF
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;사람이 있는 집의 목욕탕이라든지 화장실에는 가지 않는다
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;개인 방에는 못 들어간다
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;개인 방
			CASE 717
				;기상 후와 취침전 이외는 피한다
				IF BETWEENTIME(1200,420)
					RETURN 10
				ELSE
					RETURN -10
				ENDIF
			;전
			CASE 715
				;밭에 자주(잘) 있다
				IF BETWEENTIME(540,1400)
					RETURN 10
				ELSE
					RETURN -5
				ENDIF
			;아지트, 액가, 치유리 집, 카센가, 은둔지, 오두막
			CASE 704, 706, 709, 720 TO 730, 733, 734
				RETURN -100
			CASEELSE
				;가을은 활발하게 된다
				IF DAY:2 == 3
					CFLAG:LOCAL:이동율보정 = 5
				ELSE
					CFLAG:LOCAL:이동율보정 = -5
				ENDIF
				RETURN 0
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 7, 8
				RETURN 1
			CASE 2
				SIF DAY:2 == 3
					RETURN 1
				RETURN 0
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23, 71,72,73, 81,82,83,84
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

