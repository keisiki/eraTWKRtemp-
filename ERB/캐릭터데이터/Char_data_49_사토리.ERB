@CHARAMOVE_DATA_49(ARG,ARG:2)
LOCAL = 49
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;사람이 있는 집의 목욕탕이라든지 화장실에는 가지 않는다
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 53, 44, 51, 52
				RETURN -100
			;개인 방에는 못 들어간다
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;개인 방
			CASE 911, 912, 916, 918
				RETURN -100
			;지령전으로부터 나오지 않지만 나왔을 경우 미주하지 않게 요소만 억제
			CASE 901, 931
				IF GROUPMATCH(CFLAG:LOCAL:현재위치, 931, 932)
					RETURN 10
				ELSE
					RETURN -100
				ENDIF
			CASE 927
				IF CFLAG:LOCAL:현재위치 == 928
					RETURN 10
				ELSE
					RETURN -100
				ENDIF
			CASE 923, 928 TO 930, 935, 936, 937, 938, 940, 941, 942, 943
				RETURN -100
			;식당, 개인 방, 서재에 자주 간다
			CASE 913, 919, 920
				RETURN 5
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 9
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 91,92,93,96
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터49
#LOCALSIZE 1
