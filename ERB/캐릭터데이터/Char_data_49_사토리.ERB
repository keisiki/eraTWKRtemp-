@캐릭터데이터49(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "사토리")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(네글리제) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "캬츄샤(헤어밴드)"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "블라우스"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "장미 자수가 아름다운")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "쿠튀르 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "쿠튀르 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(3 - (2 * 승부속옷판정([[사토리]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 1)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "야채/조림/파이/시원한/담백/검소/품위있는/가정적/푹신푹신/지저풍/양식/양식/단/단/짠/짠/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "고기 요리/중화/농후/야취/영장/매운/매운/푸짐/푸짐/푸짐/외계풍/외계풍/외계풍/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "지저풍/특기요리＊가정적＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "핑크/질서/귀여운/심플/서양풍/시크/지적/" + "정신/동물/생체품/" * 3 + "하트의/눈동자의/지저의/마음의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "혼돈/일본풍/화려/파워풀/" + "싸구려/날붙이/손목호구/" * 3)

;농민 관련
;농가라고나 할까 정원사???
;출하용이 아니라 사토리님의 취미 범주이기 때문에 보수는 적게
;분명 방에 놔두거나 여동생이 가져온 시체에 데코레이션하고 있다
CASE "농민：설명"
	CALLF MAKE_STR(V_NAME, "장미 농가")
CASE "농민：작물"
	CALLF MAKE_STR(V_NAME, "장미꽃")
CASE "농민：밭의 종류"
	CALLF MAKE_STR(V_NAME, "장미원")
CASE "농민：밭의 맵"
	CALLF MAKE_STR(V_NAME, "지저")

CASE "농민：맵 ID"
	CALLF MAKE_INT(V_NAME, 9)
CASE "농민：장소 ID"
	CALLF MAKE_INT(V_NAME, 925)
CASE "농민：보수"
	CALLF MAKE_INT(V_NAME, 350)
CASE "농민：금리"
	CALLF MAKE_INT(V_NAME, 6)

;여기는 의뢰 ID가 관련하지 않는 정보
;의뢰 발생율 스카2/일반3/고유 5
CASE "의뢰 없음 발생율"
	CALLF MAKE_INT(V_NAME, 2)
CASE "일반의뢰발생율"
	CALLF MAKE_INT(V_NAME, 3)
CASE "고유의뢰발생율"
	CALLF MAKE_INT(V_NAME, 5)
CASE "고유의뢰최종 요소"
	CALLF MAKE_INT(V_NAME, 103)
CASEELSE
	;고유의뢰관련의 데이터에 바톤 터치
	SIF STRCOUNT(O_DATA, "의뢰")
		CALLF 고유의뢰49(ARG, O_DATA, V_NAME)
ENDSELECT

