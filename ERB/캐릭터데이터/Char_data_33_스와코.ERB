@CHARAMOVE_DATA_33(ARG,ARG:2)
LOCAL = 33
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;모리야신사분사에 자주(잘) 있다
			CASE 21
				RETURN 10
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 3, 7, 8
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 31, 71,72,73,76, 81,82,83,84,85
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터33
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터33(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "스와코")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지＋후드) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "모자"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "니삭스"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
	ENDIF
CASE "평상복：상반신속옷1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신속옷1", "사라시"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "블라우스"))
CASE "평상복：상반신윗도리1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리1", "민소매 윗도리"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "소녀의 향기가 나는")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "케로짱 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 착용 조건"
	SIF !RAND:(1 + (2 * 승부속옷판정([[스와코]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "짐승고기/일식/국물/단/시원한/검소/야취/가정적/뭉근뭉근/요력/맛있는물/시큼한/시큼한/시큼한/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "중화/푸짐/매운/매운/" + "개구리/" * 10)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "가호/맛있는물/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "푸른빛/초록빛/주술/질서/아름다운/레트로/" + "흙/물/요술/대지의/" * 3 + "비취의/개구리의/백사의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "파워풀/" + "바람/날붙이/손목호구/" * 3 + "가시밭의/풍신의/하쿠레이의/" * 10)

;농민 관련
;늪 같은 곳에서 자랄 것 같아서
;개굴개굴인 스와코님의 협력을 받자
;재력도 있을 것 같지만 원래 단가가 싸다는 뇌내설정
CASE "농민：설명"
	CALLF MAKE_STR(V_NAME, "마름 농가")
CASE "농민：작물"
	CALLF MAKE_STR(V_NAME, "마름")
CASE "농민：밭의 종류"
	CALLF MAKE_STR(V_NAME, "저수지")
CASE "농민：밭의 맵"
	CALLF MAKE_STR(V_NAME, "요괴의 산·산록")

CASE "농민：맵 ID"
	CALLF MAKE_INT(V_NAME, 7)
CASE "농민：장소 ID"
	CALLF MAKE_INT(V_NAME, 708)
CASE "농민：보수"
	CALLF MAKE_INT(V_NAME, 400)
CASE "농민：금리"
	CALLF MAKE_INT(V_NAME, 12)
ENDSELECT

