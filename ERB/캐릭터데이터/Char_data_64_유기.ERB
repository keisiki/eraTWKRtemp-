@CHARAMOVE_DATA_64(ARG,ARG:2)
LOCAL = 64
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;가지 않는다
			CASE 901 TO 930, 935, 937, 939, 940, 941, 942, 943
				RETURN -100
			;술집과 오니의 집합소에 가기 쉽다
			CASE 933, 938
				RETURN 100
			
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 9
				RETURN 1
			CASE 2, 7
				SIF DAY:2 == 1 || DAY:2 == 3
					RETURN 1
				RETURN 0
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 23, 71,72,73, 91,92,93,96
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터64
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터64(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "유기")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "나막신"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "블라우스"))
CASE "평상복：그외1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외1", "구속도구"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "매우 강한")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "호피 도깨비 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "사라시")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(1 + (2 * 승부속옷판정([[유기]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 3)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "고기 요리/스파이스/일식/나베/안주/매운/푸짐/농후/예술적/야취/호쾌/사각사각/지저풍/텐구류/" + "짠/" * 3)
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "시원한/담백/단/단/수수께끼 요리/수수께끼 요리/수수께끼 요리/화학적/화학적/화학적/" + "소인족 비전의 맛/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "지저풍/특기요리＊호쾌＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "빨강빛/금빛/혼돈/근사함/파워풀/장대/" + "식기/사슬의/술/" * 3 + "오니의/꽃의/귀력의/힘의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "정신/팝/섬세/지적/로리타/" + "날붙이/탄환/도서/기계/" * 3 + "캇파의/텐구의/" * 10)
ENDSELECT

