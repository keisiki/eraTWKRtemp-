@CHARAMOVE_DATA_95(ARG,ARG:2)
LOCAL = 95
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
		;연동 처리(벤벤과 함께)
		IF TALENT:95:연모 || (TALENT:95:사모 && !TALENT:95:사모)
			CFLAG:LOCAL:현재위치 = CFLAG:95:만난위치 ? CFLAG:95:만난위치 # SUKIMA()
		ELSE
			CFLAG:LOCAL:현재위치 = CFLAG:94:만난위치 ? CFLAG:94:만난위치 # SUKIMA()
		ENDIF
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;목욕탕이나 화장실에는 가지 않는다
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;사실에는 들어가지 않는다
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;자기 방, 츠쿠모 자매의 방, 광장
			CASE 208, 209, 210, 202
				RETURN 5
			;당신의 방
			CASE 230
				IF TALENT:LOCAL:연모
					RETURN 5
				ELSE
					RETURN -100
				ENDIF
			;안 간다
			CASE 220, 225, 221, 222, 224, 226
				RETURN -100
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 5
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23, 51,52,53,56,57
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터95
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터95(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "야츠하시")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "캬츄샤(헤어밴드)"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "블라우스"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "헌 옷을 리메이크한")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "느슨한 끈 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "소박한 끈브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(1 + (2 * 승부속옷판정([[야츠하시]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "쌀/고기 요리/일식/조림/농후/호화/예술적/가정적/말랑말랑/단/단/짠/짠/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "담백/매운/매운/" + "개구리/벌레고기/버섯/그로테스크/수수께끼 요리/" * 3)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "진한 맛/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "찻빛/귀여운/예술적/일본풍/시크/" + "음악/악기/" * 3 + "폭풍의/선율의/손톱/피크/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "서양풍/섬세/" + "날붙이/생체품/손목호구/" * 3 + "술대/드럼스틱/" * 5 + "정령은의/" * 10)
ENDSELECT

