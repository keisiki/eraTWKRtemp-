@CHARAMOVE_DATA_133(ARG,ARG:2)
LOCAL = 133
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 수면시간(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;是非曲直庁、冥界、八雲邸には行かない（入り口部分は出る時用）
			CASE 610, 613, 620, 621, 630, 631
				RETURN -10
			CASE 610 TO 639
				RETURN -100
			;남의 집에는 들어가지 않는다
			CASE 608, 609
				RETURN -100
			;霊長園には行かない
			CASE 642 TO 645
				RETURN -100
			;とくになし
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 6, 7, 8
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 61, 62, 64, 67, 68, 71, 72, 73, 75, 81, 82, 83, 84, 85
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터133
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터133(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "사키")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "카우보이 모자"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "블라우스"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "롱 부츠"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：그외1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외1", "스카프"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "편자 도장이 찍힌")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "블랙 쉐이프 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "검은날개 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(3 - (2 * 승부속옷판정([[사키]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "고기 요리/스파이스/중화/나베/푸짐/정키/농후/따뜻한/호화/야취/호쾌/사각사각/축생류/" + "매운/당근/" * 3)
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "검소/" + "단/시큼한/시원한/담백/" * 2 + "천계풍/" * 3)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "축생류/특기요리＊호쾌＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "흑빛/빨강빛/혼돈/근사함/심플/파워풀/장대/" + "말의/독수리의/천마의/스카프/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "질서/섬세/지적/" + "신령/도서/기계/" * 3 + "하니와의/수달의/독수리의/거북의/용의/하니와/" * 10)
ENDSELECT

