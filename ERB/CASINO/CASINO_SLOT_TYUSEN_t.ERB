;=====================================================
;통상, 고확, 초고확(모든 레어역은 보너스 추첨을 실시하고 있다)
;보너스 당선으로 1을 돌려주어 비당선으로 0을 돌려준다
;=====================================================
@보너스추첨_비32G련(당선한소역 , 설정, 내부상태)
#FUNCTION
#DIM REF 당선한소역
#DIM REF 설정
#DIM REF 내부상태

#DIM 당선율

;당선소역의 종류
#DIM CONST 당선소역_강체리 = 4
#DIM CONST 당선소역_약체리 = 5
#DIM CONST 당선소역_강수박 = 6
#DIM CONST 당선소역_약수박 = 7
#DIM CONST 당선소역_벨 = 8
#DIM CONST 당선소역_리플레이 = 9
#DIM CONST 당선소역_탈락 = 99

SELECTCASE 당선한소역
	;===================================
	;강체리
	;	설정	1	2	3	4	5	6
	;	확률	18%19%21%22%24%25%
	;===================================
	CASE 당선소역_강체리
		SELECTCASE 설정
			CASE 1
				당선율 = 18
			CASE 2
				당선율 = 19
			CASE 3
				당선율 = 21
			CASE 4
				당선율 = 22
			CASE 5
				당선율 = 24
			CASE 6
				당선율 = 25
			CASEELSE
				당선율 = 0
		ENDSELECT
		FOR LOCAL, 0, 내부상태
			SIF RAND(100) < 당선율
				RETURNF 1
		NEXT
		RETURNF 0
	;===================================
	;약체리
	;	설정	1	2	3	4	5	6
	;	확률	09%09%10%10%12%12%
	;===================================
	CASE 당선소역_약체리
		SELECTCASE 설정
			CASE 1
				당선율 = 9
			CASE 2
				당선율 = 9
			CASE 3
				당선율 = 10
			CASE 4
				당선율 = 10
			CASE 5
				당선율 = 12
			CASE 6
				당선율 = 12
			CASEELSE
				당선율 = 0
		ENDSELECT
		FOR LOCAL, 0, 내부상태
			SIF RAND(100) < 당선율
				RETURNF 1
		NEXT
		RETURNF 0
	;===================================
	;강수박
	;	설정	1	2	3	4	5	6
	;	확률	15%16%17%18%19%20%
	;===================================
	CASE 당선소역_강수박
		SELECTCASE 설정
			CASE 1
				당선율 = 15
			CASE 2
				당선율 = 16
			CASE 3
				당선율 = 17
			CASE 4
				당선율 = 18
			CASE 5
				당선율 = 19
			CASE 6
				당선율 = 20
			CASEELSE
				당선율 = 0
		ENDSELECT
		FOR LOCAL, 0, 내부상태
			SIF RAND(100) < 당선율
				RETURNF 1
		NEXT
		RETURNF 0
	;===================================
	;약수박
	;	설정	1	2	3	4	5	6
	;	확률	07%08%08%09%09%10%
	;===================================
	CASE 당선소역_약수박
		SELECTCASE 설정
			CASE 1
				당선율 = 7
			CASE 2
				당선율 = 8
			CASE 3
				당선율 = 8
			CASE 4
				당선율 = 9
			CASE 5
				당선율 = 9
			CASE 6
				당선율 = 10
			CASEELSE
				당선율 = 0
		ENDSELECT
		FOR LOCAL, 0, 내부상태
			SIF RAND(100) < 당선율
				RETURNF 1
		NEXT
		RETURNF 0
	;===================================
	;벨
	;	설정	1	2	3	4	5	6
	;	확률	0%	0%	0%	0%	0%	0%
	;===================================
	CASE 당선소역_벨
		RETURNF 0
	;========================================================
	;리플레이
	;	설정	1		2		3		4		5		6
	;	확률	0.2%	0.2%	0.2%	0.2%	0.3%	0.3%
	;========================================================
	CASE 당선소역_리플레이
		SELECTCASE 설정
			CASE 1
				당선율 = 2
			CASE 2
				당선율 = 2
			CASE 3
				당선율 = 2
			CASE 4
				당선율 = 2
			CASE 5
				당선율 = 3
			CASE 6
				당선율 = 3
			CASEELSE
				당선율 = 0
		ENDSELECT
		FOR LOCAL, 0, 내부상태
			SIF RAND(1000) < 당선율
				RETURNF 1
		NEXT
		RETURNF 0
	;========================================================
	;탈락
	;	설정	1		2		3		4		5		6
	;	확률	0.02%	0.02%	0.02%	0.02%	0.03%	0.03%
	;========================================================
	CASE 당선소역_탈락
		SELECTCASE 설정
			CASE 1
				당선율 = 2
			CASE 2
				당선율 = 2
			CASE 3
