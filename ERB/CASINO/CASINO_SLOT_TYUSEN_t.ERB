;@슬롯도안결정(당선한소역 , 도안_좌상, 도안_왼쪽안, 도안_좌하, 도안_나카가미, 도안_상당히, 도안_나카시타, 도안_우상, 도안_오른쪽안, 도안_우하)
;릴 배열
;	왼쪽안 오른쪽 
;1	REP 	MEL 	BEL
;2	BL7 	BL7 	BL7
;3	CHE 	REP 	MEL
;4	MEL 	BEL 	CHE
;5	BEL 	CHE 	REP
;6	REP 	CHE 	BEL
;7	CHE 	BAR 	RD7
;8	BAR 	REP 	MEL
;9	MEL 	BEL 	BAR
;10	BEL 	CHE 	REP
;11	REP 	RD7 	BEL
;12	BAR 	CHE 	BEL
;13	CHE 	REP 	MEL
;14	MEL 	BEL 	CHE
;15	BEL 	BAR 	REP
;16	REP 	MEL 	BEL
;17	RD7 	CHE 	BAR
;18	CHE 	REP 	MEL
;19	MEL 	BEL 	CHE
;20	BEL 	CHE 	REP
;
;
;당선한소역 
;0:스칼렛BB당선
;1:사쿠야 BB당선
;2:REG 당선
;3:중단 체리 당선
;4:강체리 당선
;5:약체리 당선
;6:강수박 당선
;7:약수박 당선
;8:벨 당선
;9:리플레이 당선
;99:탈락
;
;왼쪽 릴의 기본적인 정지형
;BEL
;REP
;RD7
;수박 및 벨의 일부
;MEL
;BEL
;REP
;체리의 정지계
;REP
;RD7
;CHE
;중단 체리
;RD7
;CHE
;MEL
;=========================================================================================================================
@슬롯도안결정(당선한소역 , 도안_좌상, 도안_왼쪽안, 도안_좌하, 도안_나카가미, 도안_상당히, 도안_나카시타, 도안_우상, 도안_오른쪽안, 도안_우하)

#DIM REF 당선한소역

#DIM REF 도안_좌상
#DIM REF 도안_왼쪽안
#DIM REF 도안_좌하
#DIM REF 도안_나카가미
#DIM REF 도안_상당히
#DIM REF 도안_나카시타
#DIM REF 도안_우상
#DIM REF 도안_오른쪽안
#DIM REF 도안_우하

;각 도안의 약칭(B_E_R만 5 문자인데 주의!!)
#DIM CONST RD7 = 1
#DIM CONST BL7 = 2
#DIM CONST BEL = 3
#DIM CONST CHE = 4
#DIM CONST MEL = 5
#DIM CONST B_A_R = 6
#DIM CONST REP = 7
#DIM CONST NUL = 8

;당선소역의 종류
#DIM CONST 당선소역_적7보너스 = 0
#DIM CONST 당선소역_청7보너스 = 1
#DIM CONST 당선소역_레귤러보너스 = 2
#DIM CONST 당선소역_중체리 = 3
#DIM CONST 당선소역_강체리 = 4
#DIM CONST 당선소역_약체리 = 5
#DIM CONST 당선소역_강수박 = 6
#DIM CONST 당선소역_약수박 = 7
#DIM CONST 당선소역_벨 = 8
#DIM CONST 당선소역_리플레이 = 9
#DIM CONST 당선소역_탈락 = 99

SELECTCASE 당선한소역
	;===================================
	;당선소역_적7보너스는 이하의 1방법
	;	REP		CHE		BEL
	;	RD7		RD7		RD7
	;	CHE		CHE		MEL
	;===================================
	CASE 당선소역_적7보너스
		도안_좌상 = REP
		도안_왼쪽안 = RD7
		도안_좌하 = CHE
		도안_나카가미 = CHE
		도안_상당히 = RD7
		도안_나카시타 = CHE
		도안_우상 = BEL
		도안_오른쪽안 = RD7
		도안_우하 = MEL
	;===================================
	;당선소역_적7보너스는 이하의 1방법
	;	REP		MEL		BEL
	;	BL7		BL7		BL7
	;	CHE		REP		MEL
	;===================================
	CASE 당선소역_청7보너스
		도안_좌상 = REP
		도안_왼쪽안 = BL7
		도안_좌하 = CHE
		도안_나카가미 = MEL
		도안_상당히 = BL7
		도안_나카시타 = REP
		도안_우상 = BEL
		도안_오른쪽안 = BL7
		도안_우하 = MEL
	;===================================
	;당선소역_레귤러보너스는 이하의 1방법
	;	REP		CHE		BEL
	;	RD7		RD7		B_A_R
	;	CHE		CHE		MEL
	;===================================
	CASE 당선소역_레귤러보너스
		도안_좌상 = REP
		도안_왼쪽안 = RD7
		도안_좌하 = CHE
		도안_나카가미 = CHE
		도안_상당히 = RD7
		도안_나카시타 = CHE
		도안_우상 = BEL
		도안_오른쪽안 = B_A_R
		도안_우하 = MEL
	;===================================
	;당선소역_중체리
	;	B_A_R	---		---
	;	CHE		B_A_R	---
	;	MEL		---		B_A_R
	;===================================
	CASE 당선소역_중체리
		도안_좌상 = B_A_R
		도안_왼쪽안 = CHE
		도안_좌하 = MEL
		;안릴에서 위의 정지계는 2방법(6,14)
		IF !RAND(2)
			도안_나카가미 = CHE
			도안_상당히 = B_A_R
			도안_나카시타 = REP
		ELSE
			도안_나카가미 = BEL
			도안_상당히 = B_A_R
			도안_나카시타 = MEL
		ENDIF
		;오른쪽 릴에서 위의 정지계는 2방법(8,16)
		IF !RAND(2)
			도안_우상 = RD7
			도안_오른쪽안 = MEL
			도안_우하 = B_A_R
		ELSE
			도안_우상 = REP
			도안_오른쪽안 = BEL
			도안_우하 = B_A_R
		ENDIF
	;===================================
	;당선소역_강체리
	;	REP		---		CHE
	;	RD7		CHE		REP
	;	CHE		---		BEL
	;===================================
	CASE 당선소역_강체리
		도안_좌상 = REP
		도안_왼쪽안 = RD7
		도안_좌하 = CHE
		;안릴에서 위의 정지계는 6방법(4,5,9,11,16,19)
		SELECTCASE RAND(6)
		CASE 0
			도안_나카가미 = BEL
			도안_상당히 = CHE
			도안_나카시타 = CHE
		CASE 1
			도안_나카가미 = CHE
			도안_상당히 = CHE
			도안_나카시타 = B_A_R
		CASE 2
			도안_나카가미 = BEL
			도안_상당히 = CHE
			도안_나카시타 = RD7
		CASE 3
			도안_나카가미 = RD7
			도안_상당히 = CHE
			도안_나카시타 = REP
		CASE 4
			도안_나카가미 = MEL
			도안_상당히 = CHE
			도안_나카시타 = REP
		CASEELSE
			도안_나카가미 = BEL
			도안_상당히 = CHE
			도안_나카시타 = MEL
		ENDSELECT
		도안_우상 = CHE
		도안_오른쪽안 = REP
		도안_우하 = BEL
	;===================================
	;당선소역_약체리
	;오른쪽 릴은 체리 제외해
	;	REP		---		---
	;	RD7		CHE		---
	;	CHE		---		---
	;===================================
	CASE 당선소역_약체리
		도안_좌상 = REP
		도안_왼쪽안 = RD7
		도안_좌하 = CHE
		;안릴에서 위의 정지계는 6방법(4,5,9,11,16,19)
		SELECTCASE RAND(6)
		CASE 0
			도안_나카가미 = BEL
			도안_상당히 = CHE
			도안_나카시타 = CHE
		CASE 1
			도안_나카가미 = CHE
			도안_상당히 = CHE
			도안_나카시타 = B_A_R
		CASE 2
			도안_나카가미 = BEL
			도안_상당히 = CHE
			도안_나카시타 = RD7
		CASE 3
			도안_나카가미 = RD7
			도안_상당히 = CHE
			도안_나카시타 = REP
		CASE 4
			도안_나카가미 = MEL
			도안_상당히 = CHE
			도안_나카시타 = REP
		CASEELSE
			도안_나카가미 = BEL
			도안_상당히 = CHE
			도안_나카시타 = MEL
		ENDSELECT
		;오른쪽 릴에서 체리 엎지름은 2방법
		IF !RAND(2)
			도안_우상 = MEL
			도안_오른쪽안 = CHE
			도안_우하 = REP
		ELSE
			도안_우상 = REP
			도안_오른쪽안 = BEL
			도안_우하 = B_A_R
		ENDIF
	;===================================
	;당선소역_강수박
	;소V자 수박
	;	MEL		---		MEL
	;	BEL		MEL		---
	;	REP		---		REP
	;===================================
	CASE 당선소역_강수박
		도안_좌상 = MEL
		도안_왼쪽안 = BEL
		도안_좌하 = REP
		;안릴에서 위의 정지계는 2방법
		IF !RAND(2)
			도안_나카가미 = CHE
			도안_상당히 = MEL
			도안_나카시타 = BL7
		ELSE
			도안_나카가미 = B_A_R
			도안_상당히 = MEL
			도안_나카시타 = CHE
		ENDIF
		;오른쪽 릴에서 위의 정지계는 2방법
		IF !RAND(2)
			도안_우상 = MEL
			도안_오른쪽안 = CHE
			도안_우하 = REP
		ELSE
			도안_우상 = MEL
			도안_오른쪽안 = B_A_R
			도안_우하 = REP
		ENDIF
	;===================================
	;당선소역_약수박
	;오른쪽 밑 수박
	;	MEL		---		BEL
	;	BEL		MEL		---
	;	REP		---		MEL
	;===================================
	CASE 당선소역_약수박
		도안_좌상 = MEL
		도안_왼쪽안 = BEL
		도안_좌하 = REP
		;안릴에서 위의 정지계는 2방법
		IF !RAND(2)
			도안_나카가미 = CHE
			도안_상당히 = MEL
			도안_나카시타 = BL7
		ELSE
			도안_나카가미 = B_A_R
			도안_상당히 = MEL
			도안_나카시타 = CHE
		ENDIF
		;오른쪽 릴에서 위의 정지계는 4방법
		SELECTCASE RAND(4)
		CASE 0
			도안_우상 = BEL
			도안_오른쪽안 = B_A_R
			도안_우하 = MEL
		CASE 1
			도안_우상 = BEL
			도안_오른쪽안 = BL7
			도안_우하 = MEL
		CASE 2
			도안_우상 = BEL
			도안_오른쪽안 = RD7
			도안_우하 = MEL
		CASEELSE
			도안_우상 = BEL
			도안_오른쪽안 = BEL
			도안_우하 = MEL
		ENDSELECT
	;===================================
	;당선소역_벨
	;오른쪽 밑 벨
	;	BEL		---		---
	;	REP		BEL		---
	;	RD7		---		BEL
	;
	;상단 벨
	;	BEL		BEL		BEL
	;	REP		---		---
	;	RD7		---		---
	;
	;수박 미끄러짐으로부터의 중단 벨
	;	MEL		REP		REP
	;	BEL		BEL		BEL
	;	REP		B_A_R	---
	;===================================
	CASE 당선소역_벨
		;수박 미끄러짐으로부터의 중단 벨
		;출현 빈도를 내리고 싶기 위해(때문에) 벨 당선시의 것1/20으로 출현
		IF !RAND(20)
			도안_좌상 = MEL
			도안_왼쪽안 = BEL
			도안_좌하 = REP
			도안_나카가미 = REP
			도안_상당히 = BEL
			도안_나카시타 = B_A_R
			;오른쪽 릴에서 위의 정지계는 4방법
			SELECTCASE RAND(4)
			CASE 0
				도안_우상 = REP
				도안_오른쪽안 = BEL
				도안_우하 = BL7
			CASE 1
				도안_우상 = REP
				도안_오른쪽안 = BEL
				도안_우하 = RD7
			CASE 2
				도안_우상 = REP
				도안_오른쪽안 = BEL
				도안_우하 = BEL
			CASEELSE
				도안_우상 = REP
				도안_오른쪽안 = BEL
				도안_우하 = B_A_R
			ENDSELECT
		;오른쪽 밑 벨
		ELSEIF !RAND(2)
			도안_좌상 = BEL
			도안_왼쪽안 = REP
			도안_좌하 = RD7
			;안릴에서 위의 정지계는 2방법
			IF !RAND(2)
				도안_나카가미 = REP
				도안_상당히 = BEL
				도안_나카시타 = B_A_R
			ELSE
				도안_나카가미 = REP
				도안_상당히 = BEL
				도안_나카시타 = CHE
			ENDIF
			;오른쪽 릴에서 위의 정지계는 3방법
			SELECTCASE RAND(3)
			CASE 0
				도안_우상 = CHE
				도안_오른쪽안 = REP
				도안_우하 = BEL
			CASE 1
				도안_우상 = B_A_R
				도안_오른쪽안 = REP
				도안_우하 = BEL
			CASEELSE
				도안_우상 = REP
				도안_오른쪽안 = BEL
				도안_우하 = BEL
			ENDSELECT
		;상단 벨
		ELSE
			도안_좌상 = BEL
			도안_왼쪽안 = REP
			도안_좌하 = RD7
			;안릴에서 위의 정지계는 4방법
			SELECTCASE RAND(4)
			CASE 0
				도안_나카가미 = BEL
				도안_상당히 = CHE
				도안_나카시타 = CHE
			CASE 1
				도안_나카가미 = BEL
				도안_상당히 = CHE
				도안_나카시타 = RD7
			CASE 2
				도안_나카가미 = BEL
				도안_상당히 = B_A_R
				도안_나카시타 = MEL
			CASEELSE
				도안_나카가미 = BEL
				도안_상당히 = CHE
				도안_나카시타 = MEL
			ENDSELECT
			;오른쪽 릴에서 위의 정지계는 5방법
			SELECTCASE RAND(5)
			CASE 0
				도안_우상 = BEL
				도안_오른쪽안 = BL7
				도안_우하 = MEL
			CASE 1
				도안_우상 = BEL
				도안_오른쪽안 = RD7
				도안_우하 = MEL
			CASE 2
				도안_우상 = BEL
				도안_오른쪽안 = BEL
				도안_우하 = MEL
			CASE 3
				도안_우상 = BEL
				도안_오른쪽안 = MEL
				도안_우하 = CHE
			CASEELSE
				도안_우상 = BEL
				도안_오른쪽안 = B_A_R
				도안_우하 = MEL
			ENDSELECT
		ENDIF
	;===================================
	;당선소역_리플레이
	;중단 리플레이
	;	BEL		---		---
	;	REP		REP		REP
	;	RD7		BEL		BEL
	;===================================
	CASE 당선소역_리플레이
		도안_좌상 = BEL
		도안_왼쪽안 = REP
		도안_좌하 = RD7
		;안릴에서 위의 정지계는 3방법
		SELECTCASE RAND(3)
		CASE 0
			도안_나카가미 = BL7
			도안_상당히 = REP
			도안_나카시타 = BEL
		CASE 1
			도안_나카가미 = B_A_R
			도안_상당히 = REP
			도안_나카시타 = BEL
		CASEELSE
			도안_나카가미 = CHE
			도안_상당히 = REP
			도안_나카시타 = BEL
		ENDSELECT
		;오른쪽 릴에서 위의 정지계는 2방법
		IF !RAND(2)
			도안_우상 = B_A_R
