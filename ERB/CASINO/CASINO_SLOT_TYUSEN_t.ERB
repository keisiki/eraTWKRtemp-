		;벨 엎지름 패턴
		IF !RAND(5)
			;오른쪽 밑 벨 엎지름
			IF !RAND(2)
				;안릴에서 위의 정지계는 2방법
				IF !RAND(2)
					도안_나카가미 = REP
					도안_상당히 = BEL
					도안_나카시타 = B_A_R
				ELSE
					도안_나카가미 = REP
					도안_상당히 = BEL
					도안_나카시타 = CHE
				ENDIF
				;오른쪽 릴에서 위의 정지계는 4방법
				SELECTCASE RAND(4)
				CASE 0
					도안_우상 = REP
					도안_오른쪽안 = BEL
					도안_우하 = RD7
				CASE 1
					도안_우상 = BEL
					도안_오른쪽안 = BEL
					도안_우하 = MEL
				CASE 2
					도안_우상 = REP
					도안_오른쪽안 = BEL
					도안_우하 = B_A_R
				CASEELSE
					도안_우상 = REP
					도안_오른쪽안 = BEL
					도안_우하 = BL7
				ENDSELECT
			;상단 벨 엎지름
			ELSE
				;안릴에서 위의 정지계는 3방법
				SELECTCASE RAND(3)
				CASE 0
					도안_나카가미 = BEL
					도안_상당히 = CHE
					도안_나카시타 = CHE
				CASE 1
					도안_나카가미 = BEL
					도안_상당히 = B_A_R
					도안_나카시타 = MEL
				CASEELSE
					도안_나카가미 = BEL
					도안_상당히 = CHE
					도안_나카시타 = MEL
				ENDSELECT
				;오른쪽 릴에서 위의 정지계는 4방법
				SELECTCASE RAND(4)
				CASE 0
					도안_우상 = REP
					도안_오른쪽안 = BEL
					도안_우하 = RD7
				CASE 1
					도안_우상 = REP
					도안_오른쪽안 = BEL
					도안_우하 = BEL
				CASE 2
					도안_우상 = REP
					도안_오른쪽안 = BEL
					도안_우하 = B_A_R
				CASEELSE
					도안_우상 = REP
					도안_오른쪽안 = BEL
					도안_우하 = BL7
				ENDSELECT
			ENDIF
		;리플레이 엎지름 패턴
		ELSEIF !RAND(4)
			;안릴에서 위의 정지계는 3방법
			SELECTCASE RAND(3)
			CASE 0
				도안_나카가미 = BL7
				도안_상당히 = REP
				도안_나카시타 = BEL
			CASE 1
				도안_나카가미 = B_A_R
				도안_상당히 = REP
				도안_나카시타 = BEL
			CASEELSE
				도안_나카가미 = CHE
				도안_상당히 = REP
				도안_나카시타 = BEL
			ENDSELECT
			
			;중단 리플레이상 엎지름
			IF !RAND(2)
				;오른쪽 릴에서 위의 정지계는 4방법
				SELECTCASE RAND(4)
				CASE 0
					도안_우상 = REP
					도안_오른쪽안 = BEL
					도안_우하 = RD7
				CASE 1
					도안_우상 = REP
					도안_오른쪽안 = BEL
					도안_우하 = BEL
				CASE 2
					도안_우상 = REP
					도안_오른쪽안 = BEL
					도안_우하 = B_A_R
				CASEELSE
					도안_우상 = REP
					도안_오른쪽안 = BEL
					도안_우하 = BL7
				ENDSELECT
			;중단 리플레이하 엎지름
			ELSE
				;오른쪽 릴에서 위의 정지계는 2방법
				IF !RAND(2)
					도안_우상 = MEL
					도안_오른쪽안 = CHE
					도안_우하 = REP
				ELSE
					도안_우상 = MEL
					도안_오른쪽안 = B_A_R
					도안_우하 = REP
				ENDIF
			ENDIF
		;단순한 꽝 패턴(안릴 BEL가 들어 오지 않는다)
		ELSE
			;안릴의 배치의 방법에 의해 변화
			SELECTCASE RAND(3)
			;안릴 보너스 도안 상단
			CASE 0
				IF !RAND(2)
					도안_나카가미 = RD7
					도안_상당히 = CHE
					도안_나카시타 = REP
				ELSE
					도안_나카가미 = B_A_R
					도안_상당히 = MEL
					도안_나카시타 = CHE
				ENDIF
				;오른쪽 릴 보너스 도안이 상단에 오도록(듯이) 한다
				SELECTCASE RAND(6)
				CASE 0
					도안_우상 = BL7
					도안_오른쪽안 = MEL
					도안_우하 = CHE
				CASE 1
					도안_우상 = RD7
					도안_오른쪽안 = MEL
					도안_우하 = B_A_R
				CASE 2
					도안_우상 = B_A_R
					도안_오른쪽안 = REP
					도안_우하 = BEL
				CASE 3
					도안_우상 = B_A_R
					도안_오른쪽안 = MEL
					도안_우하 = CHE
				;예외 1( 오른쪽 릴은 4개 미끄러져도 보너스 도안이 상단에 오지 않을 때가 있다)
				CASE 4
					도안_우상 = BEL
					도안_오른쪽안 = MEL
					도안_우하 = CHE
				;예외 2( 오른쪽 릴은 4개 미끄러져도 보너스 도안이 상단에 오지 않을 때가 있다)
				CASEELSE
					도안_우상 = MEL
					도안_오른쪽안 = CHE
					도안_우하 = REP
				ENDSELECT
			;안릴 보너스 도안 중단
			CASE 1
				SELECTCASE RAND(3)
				CASE 0
					도안_나카가미 = MEL
					도안_상당히 = BL7
					도안_나카시타 = REP
				CASE 1
					도안_나카가미 = CHE
					도안_상당히 = B_A_R
					도안_나카시타 = REP
				CASE 2
					도안_나카가미 = CHE
					도안_상당히 = RD7
					도안_나카시타 = CHE
				ENDSELECT
				;오른쪽 릴 보너스 도안이 중단에 오도록(듯이) 한다
				SELECTCASE RAND(5)
				CASE 0
					도안_우상 = BEL
					도안_오른쪽안 = BL7
					도안_우하 = MEL
				CASE 1
					도안_우상 = BEL
					도안_오른쪽안 = RD7
					도안_우하 = MEL
				CASE 2
					도안_우상 = MEL
					도안_오른쪽안 = B_A_R
					도안_우하 = REP
				CASE 3
					도안_우상 = BEL
					도안_오른쪽안 = B_A_R
					도안_우하 = MEL
				;이만큼 예외( 오른쪽 릴은 4개 미끄러져도 보너스 도안이 중단에 오지 않을 때가 있다)
				CASEELSE
					도안_우상 = BEL
					도안_오른쪽안 = BEL
					도안_우하 = MEL
				ENDSELECT
			;안릴 보너스 도안 하단
			CASEELSE
				IF !RAND(2)
					도안_나카가미 = CHE
					도안_상당히 = CHE
					도안_나카시타 = B_A_R
				ELSE
					도안_나카가미 = CHE
					도안_상당히 = MEL
					도안_나카시타 = BL7
				ENDIF
				;오른쪽 릴 보너스 도안이 카미나카단에 오도록(듯이) 한다
				SELECTCASE RAND(10)
				CASE 0
					도안_우상 = BL7
					도안_오른쪽안 = MEL
					도안_우하 = CHE
				CASE 1
					도안_우상 = B_A_R
					도안_오른쪽안 = REP
					도안_우하 = BEL
				CASE 2
					도안_우상 = B_A_R
					도안_오른쪽안 = MEL
					도안_우하 = CHE
				CASE 3
					도안_우상 = BEL
					도안_오른쪽안 = BL7
					도안_우하 = MEL
				CASE 4
					도안_우상 = BEL
					도안_오른쪽안 = RD7
					도안_우하 = MEL
				CASE 5
					도안_우상 = MEL
					도안_오른쪽안 = B_A_R
					도안_우하 = REP
				CASE 6
					도안_우상 = BEL
					도안_오른쪽안 = B_A_R
					도안_우하 = MEL
				CASE 7
					도안_우상 = BEL
					도안_오른쪽안 = MEL
					도안_우하 = CHE
				CASE 8
					도안_우상 = MEL
					도안_오른쪽안 = CHE
					도안_우하 = REP
				CASEELSE
					도안_우상 = BEL
					도안_오른쪽안 = BEL
					도안_우하 = MEL
				ENDSELECT
			ENDSELECT
		ENDIF
	CASEELSE
ENDSELECT

;============================================
;당선게임수
;천정 999G
;1/6그리고 333 G1/6으로 666 G1/3으로 999 G 후의 것1/3은 등 확률로 선택
;레어소역에 의한 단축은 있어
;============================================
@당선게임수설정()
#FUNCTION
#DIM 당선게임
IF !RAND(6)
	RETURNF 333
ELSEIF !RAND(5)
	RETURNF 666
ELSEIF !RAND(2)
	RETURNF 999
ELSE
	당선게임 = RAND(996) +1
	SIF 당선게임 >= 333
		당선게임 ++
	SIF 당선게임 >= 666
		당선게임 ++
	RETURNF 당선게임
ENDIF
