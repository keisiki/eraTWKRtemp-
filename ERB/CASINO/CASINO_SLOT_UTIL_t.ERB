	FONTSTYLE 1
	SELECTCASE 행수
	CASE 1,9
		PRINTFORM ;;;;;;;;;;;;;;;;;;;;;;;;;
	CASE 2,3,4
		PRINT ;;;
		SETCOLOR 색_RED7
		PRINTFORM %UNICODE(0x2589)*19%
		RESETCOLOR
