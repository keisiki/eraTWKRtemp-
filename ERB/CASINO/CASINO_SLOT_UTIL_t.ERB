;=============================
;여기에서(보다) 슬롯 화면의 표시
;=============================
;########
;상틀분
;########
FONTSTYLE 1
SETCOLOR 색_범위
PRINTFORML %UNICODE(0x2582)*82%
RESETCOLOR
FONTREGULAR

;############
;슬롯 상단
;############
FOR LOCAL, 1, 10
	SETCOLOR 색_범위
	PRINTFORM %UNICODE(0x2589)%
	RESETCOLOR
	CALL SLOT_DISPLAY(도안_좌상, LOCAL, 암전_좌상)
	SETCOLOR 색_범위
	PRINTFORM |%UNICODE(0x2589)%|
	RESETCOLOR
	CALL SLOT_DISPLAY(도안_나카가미, LOCAL, 암전_나카가미)
	SETCOLOR 색_범위
	PRINTFORM |%UNICODE(0x2589)%|
	RESETCOLOR
	CALL SLOT_DISPLAY(도안_우상, LOCAL, 암전_우상)
	SETCOLOR 색_범위
	PRINTFORML %UNICODE(0x2589)%
	RESETCOLOR
NEXT

;############
;슬롯 중단
;############
FOR LOCAL, 1, 10
	SETCOLOR 색_범위
	PRINTFORM %UNICODE(0x2589)%
	RESETCOLOR
	CALL SLOT_DISPLAY(도안_왼쪽안, LOCAL, 암전_왼쪽안)
	SETCOLOR 색_범위
	PRINTFORM |%UNICODE(0x2589)%|
	RESETCOLOR
	CALL SLOT_DISPLAY(도안_상당히, LOCAL, 암전_상당히)
	SETCOLOR 색_범위
