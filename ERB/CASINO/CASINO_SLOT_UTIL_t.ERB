ELSEIF 내부상태 == 5 && 당선한BB종류 == 0
	FONTSTYLE 3
	SETCOLOR 0xE6B422
	PRINT 프리즈다!
	SETCOLOR 0xFF0000
	PRINTL 스칼렛BB확정!400매 획득
	RESETCOLOR
	FONTREGULAR
;=======================
;Freeze(얼다) (내부상태 == 5)
;=======================
ELSEIF 내부상태 == 5
	FONTSTYLE 1
	SETCOLOR 0xFF00FF
	PRINTL 으응!!?
	RESETCOLOR
	FONTREGULAR
;=======================
;단주기 무리 (내부상태 == 4)
;=======================
ELSEIF 내부상태 == 4
	FONTSTYLE 1
	SETCOLOR 0xFF0000
	SIF 단주기련세트수 >= 10
		SETCOLOR 0xE6B422
	PRINT 불야성 타임!
	SETCOLOR 0xE6B422
	PRINTFORM {단주기련세트수}세트눈나머지{32 - 전회당선으로부터의회전수}G 
	SETCOLOR 0xFFFFFF
	PRINTFORM Total {MONEY:2 - 단주기련개시시소지카리스마}
	IF 단주기련계속율 >= 90
		SETCOLOR 0xE6B422
	ELSEIF 단주기련계속율 >= 80
		SETCOLOR 0xFF0000
	ELSEIF 단주기련계속율 >= 70
		SETCOLOR 0x00FF00
	ELSEIF 단주기련계속율 >= 60
		SETCOLOR 0xFFFF00
	ELSE
		SETCOLOR 0xFFFFFF
	ENDIF
	SIF 단주기련계속율 >= 50
		PRINTFORM  계속율{단주기련계속율 / 10 * 10}\%이상!
	PRINTL 
	RESETCOLOR
	FONTREGULAR
;=======================
;소역이 당선했을 때
;=======================
ELSEIF 당선한소역 != 99
	FONTSTYLE 1
	SELECTCASE 당선한소역
	CASE 3
		SETCOLOR 0xFF00FF
		PRINTL 중단 체리 당선!+3매
		RESETCOLOR
	CASE 4
		SETCOLOR 0xFF0000
		PRINTL 강체리 당선!+3매
		RESETCOLOR
	CASE 5
		SETCOLOR 0xFF0000
		PRINTL 약체리 당선!+3매
		RESETCOLOR
	CASE 6
		SETCOLOR 0x70C070
		PRINTL 강수박 당선!+5매
		RESETCOLOR
	CASE 7
		SETCOLOR 0x70C070
		PRINTL 약수박 당선!+5매
		RESETCOLOR
	CASE 8
		SETCOLOR 0xFFFF00
		PRINTL 벨 +8매
		RESETCOLOR
	CASE 9
		SETCOLOR 0xCCFFFF
		PRINTL REPLAY
		RESETCOLOR
	ENDSELECT
	FONTREGULAR
;=====================================
;여기에서(보다) 아래는 확률로 메시지가 나올지가 정해진다
;=====================================
;=======================
;징조 확정 메시지
;=======================
ELSEIF 징조 < 32 && 징조 > 0 && !RAND(32)
	FONTSTYLE 1
	SETCOLOR 0xFF00FF
	PRINTL 격 아뜨!
	RESETCOLOR
	FONTREGULAR
;=======================
;초고확확정 메시지
;=======================
ELSEIF 내부상태 == 3 && !RAND(100)
	FONTSTYLE 1
	SETCOLOR 0xFF00FF
	PRINTL 상태는 매우 좋을 것 같다, 여기서 그만두는 것은 아깝다
	RESETCOLOR
	FONTREGULAR
;=======================
;고확확정 메시지
;=======================
ELSEIF 내부상태 == 2 && !RAND(100)
	FONTSTYLE 1
	SETCOLOR 0xFF00FF
	PRINTL 상태는 좋을 것 같다, 좀 더 쳐도 괜찮을 것이다
	RESETCOLOR
	FONTREGULAR

;=======================
;설정 시사 메시지
;=======================
ELSEIF 설정 > 3 && !RAND(32768)
	FONTSTYLE 1
	SIF 설정 == 6
		SETCOLOR 0xE6B422
	SIF 설정 == 5
		SETCOLOR 0xFF00FF
	PRINTFORML 설정{설정}확정!
	RESETCOLOR
	FONTREGULAR
ELSEIF 설정 > 3 && !RAND(1000)
	FONTSTYLE 1
	PRINTL 아무래도 고설정과 같다
	FONTREGULAR

;=======================
;여기에서 아래는 통상 메시지
;=======================
ELSE
	SELECTCASE RAND(50)
	CASE 0
		PRINTL 격한 말투!!의 문자가 나오면 엄청 짜증나
	CASE 1
		PRINTL 보너스 내역：스칼렛BB 400매 이자요이BB 200매 레귤러 B 100매
	CASE 2
		PRINTL 카리스마의 이용은 계획적으로
	CASE 3
		PRINTL 32 G련의 최대 계속율은 95%
	CASEELSE
		PRINTL 
	ENDSELECT
ENDIF
