@DAILY_CONDITION_EV1
;영원정거주지라면1/10, 그 이외로1/30로 발생
SIF MAIN_MAP == 4 && RAND:10 > 0
	RETURN -1
SIF MAIN_MAP != 4 && RAND:30 > 0
	RETURN -1
CALL DAILY_EV1
RETURN 1


@DAILY_EV1
