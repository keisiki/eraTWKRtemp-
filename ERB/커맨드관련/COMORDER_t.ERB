;궁합 체크
R = NO:PLAYER
IF RELATION:R > 0 && RELATION:R < 30
	PRINT  - 
	A -= 10
	PRINT 궁합 최악
	PRINTV '(, 10, ')
	S = 1
ELSEIF RELATION:R > 0 && RELATION:R < 70
	PRINT  - 
	A -= 6
	PRINT 궁합 나쁘다
	PRINTV '(, 10, ')
	S = 1
ELSEIF RELATION:R > 0 && RELATION:R < 100
	PRINT  - 
	A -= 3
	PRINT 궁합 좀
	PRINTV '(, 3, ')
	S = 1
ELSEIF RELATION:R >= 100 && RELATION:R < 130
	SIF S
		PRINT  + 
	A += 3
	PRINT 궁합 약간 좋다
	PRINTV '(, 3, ')
	S = 1
ELSEIF RELATION:R >= 100 && RELATION:R < 170
	SIF S
		PRINT  + 
	A += 6
	PRINT 궁합이야 있고
	PRINTV '(, 6, ')
	S = 1
ELSEIF RELATION:R >= 100 && RELATION:R >= 170
	SIF S
		PRINT  + 
	A += 10
	PRINT 궁합 최고
	PRINTV '(, 10, ')
	S = 1
ENDIF