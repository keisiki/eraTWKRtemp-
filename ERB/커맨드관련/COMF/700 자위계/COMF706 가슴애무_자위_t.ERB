;-------------------------------------------------
;가슴애무
;자위계 커맨드, 레벨 2
;-------------------------------------------------
@COM706
;-------------------------------------------------
;COM별 FLAG의 설정
;-------------------------------------------------
;행위자
TCVAR:116 = PLAYER
;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비 체력, 기력
DOWNBASE:PLAYER:체력 += 5
DOWNBASE:PLAYER:기력 += 50

SOURCE:PLAYER:쾌B = 100
SOURCE:PLAYER:사랑 = 50
SOURCE:PLAYER:성행동 = 60
SOURCE:PLAYER:노출 = 10
SOURCE:PLAYER:불결 = 20
SOURCE:PLAYER:반감 = 20
;-------------------------------------------------
;특수소스처리
;-------------------------------------------------
IF ABL:PLAYER:손가락 >= 4
	SOURCE:PLAYER:쾌B += 70
ELSE
	SOURCE:PLAYER:쾌B += ABL:PLAYER:손가락 * 15
ENDIF
;억압, 저항 소유에는, 억울소스 추가
SIF TALENT:PLAYER:자기애 < 0 || TALENT:PLAYER:저항
	SOURCE:PLAYER:울굴 += 20
;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------

;노예의 B⇔조교자의 손가락의 더러움이 이동
STAIN:PLAYER:5 |= STAIN:PLAYER:손
STAIN:PLAYER:손 |= STAIN:PLAYER:5

;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
SIF !FLAG:시간정지
	TIME += 5
;자위경험
CFLAG:PLAYER:자위했다 ++
EXP:PLAYER:수음경험 ++
RETURN 1

;-------------------------------------------------
;실행 가능 판정
;-------------------------------------------------
@COM_ABLE706
;가슴애무 실행 판정
SIF !TFLAG:100
	RETURN 0
;일괄관리
SIF GLOBAL_COMABLE(706)
	RETURN RESULT
RETURN 1

