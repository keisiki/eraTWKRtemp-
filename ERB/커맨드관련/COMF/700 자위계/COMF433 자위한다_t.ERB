;-------------------------------------------------
;자위한다
;-------------------------------------------------
@COM433
CFLAG:MASTER:지금바쁨 = 1
SIF !FLAG:시간정지
	TIME += 5
TARGET = MASTER
RETURN 1

;-------------------------------------------------
;실행 판정
;-------------------------------------------------
@COM_ABLE433
;목격자가 있으면(자) 안 돼
SIF TARGET > 0 && !FLAG:70
	RETURN 0
SIF !TFLAG:100
	RETURN 0
;일괄관리
SIF GLOBAL_COMABLE(433)
	RETURN RESULT
;기력 0
SIF BASE:MASTER:기력 <= 0
	RETURN 0
SIF CFLAG:MASTER:지금바쁨
	RETURN 0
RETURN 1

