;-------------------------------------------------
;유두로터
;도구 사용 커맨드, 레벨 2
;-------------------------------------------------
@COM749
;-------------------------------------------------
;특수 파생 처리
;-------------------------------------------------
PRINTL 유두로터
STR:0 = 유두로터
;-------------------------------------------------
;COM별 FLAG의 설정
;-------------------------------------------------
;행위자
TCVAR:116 = PLAYER
;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비 체력, 기력

	DOWNBASE:PLAYER:체력 += 10
	DOWNBASE:PLAYER:기력 += 80

	SOURCE:PLAYER:쾌B = 250
	SOURCE:PLAYER:노출 = 20
	SOURCE:PLAYER:일탈 = 30
	SOURCE:PLAYER:반감 = 30
	;-------------------------------------------------
	;특수소스처리
	;-------------------------------------------------
	;억압, 저항 소유에는, 억울소스 추가
	SIF TALENT:PLAYER:자기애 < 0 || TALENT:PLAYER:저항
		SOURCE:PLAYER:울굴 += 30
;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
SIF !FLAG:시간정지
	TIME += 5
;자위경험
CFLAG:PLAYER:자위했다 ++
EXP:PLAYER:도구사용경험 ++
RETURN 1

;-------------------------------------------------
;실행 가능 판정
;-------------------------------------------------
@COM_ABLE749
;유두로터 실행 판정
SIF !TFLAG:100
	RETURN 0
;일괄관리
SIF GLOBAL_COMABLE(749)
	RETURN RESULT
;아이템 을 가지고 있는인가, 아이템 없음 설정이 아니면 안 돼
SIF ITEM:0 == 0 && NOITEM == 0
	RETURN 0
;상반신착의
SIF (TEQUIP:1 & 2) && TEQUIP:3 == 0
	RETURN 0
;착유기 사용중은 불가
SIF TEQUIP:17
	RETURN 0
;유방 전극 장착중은 안 돼
SIF TEQUIP:24
	RETURN 0
RETURN 1

