;-------------------------------------------------
;애널바이브
;도구 사용 커맨드, 레벨 2
;-------------------------------------------------
@COM745
;착탈의처리
TEQUIP:PLAYER:애널바이브 = !TEQUIP:PLAYER:애널바이브 
;-------------------------------------------------
;COM별 FLAG의 설정
;-------------------------------------------------
;행위자
TCVAR:116 = PLAYER

;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비 체력, 기력
DOWNBASE:PLAYER:체력 += 60
DOWNBASE:PLAYER:기력 += 150

SOURCE:PLAYER:쾌A = 250
SOURCE:PLAYER:고통 = 250
SOURCE:PLAYER:노출 = 50
SOURCE:PLAYER:굴종 = 50
SOURCE:PLAYER:일탈 = 50
SOURCE:PLAYER:반감 = 200
;-------------------------------------------------
;특수소스처리
;-------------------------------------------------
;억압, 저항 소유에는, 억울소스 추가
SIF TALENT:PLAYER:자기애 < 0 || TALENT:PLAYER:저항
	SOURCE:PLAYER:울굴 += 70

;A에의 고통은 먼저 처리해 둔다
CALL PAIN_CHECK_A(PLAYER)
;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
STAIN:PLAYER:장내 |= STAIN:PLAYER:4
STAIN:PLAYER:4 |= STAIN:PLAYER:장내
;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
SIF !FLAG:시간정지
	TIME += 5
;A경험
EXP:PLAYER:A경험 ++

;자위경험
CFLAG:PLAYER:자위했다 ++

EXP:PLAYER:도구사용경험 ++

RETURN 1
;-------------------------------------------------
;장착 계속의 처리
;-------------------------------------------------
@EQUIP_COM745(PLAYER)
;애널바이브 삽입중
PRINTL ＜애널바이브 삽입중＞

DOWNBASE:PLAYER:체력 += 30
DOWNBASE:PLAYER:기력 += 80

SOURCE:PLAYER:쾌A += 125
SOURCE:PLAYER:고통 += 100
SOURCE:PLAYER:노출 += 25
SOURCE:PLAYER:일탈 += 25
SOURCE:PLAYER:반감 += 40

;-------------------------------------------------
;특수소스처리
;-------------------------------------------------
;억압, 저항 소유에는, 억울소스 추가
SIF TALENT:PLAYER:자기애 < 0 || TALENT:PLAYER:저항
	SOURCE:PLAYER:울굴 += 35

;A에의 고통은 먼저 처리해 둔다
;EXP:A경험을 본다
LOCAL = 100
LOCAL:1 = 40
IF EXP:PLAYER:A경험 < EXPLV:1
	TIMES LOCAL , 3.00
	TIMES LOCAL:1 , 3.00
ELSEIF EXP:PLAYER:A경험 < EXPLV:2
	TIMES LOCAL , 1.20
	TIMES LOCAL:1 , 2.00
ELSEIF EXP:PLAYER:A경험 < EXPLV:3
	TIMES LOCAL , 0.80
	TIMES LOCAL:1 , 1.50
ELSEIF EXP:PLAYER:A경험 < EXPLV:4
	TIMES LOCAL , 0.60
	TIMES LOCAL:1 , 1.20
ELSEIF EXP:PLAYER:A경험 < EXPLV:5
	TIMES LOCAL , 0.40
ELSEIF EXP:PLAYER:A경험 >= EXPLV:5
	TIMES LOCAL , 0.20
ENDIF

;PALAM:윤활을 본다
IF PALAM:PLAYER:윤활 < PALAMLV:1
	TIMES LOCAL , 1.20
	TIMES LOCAL:1 , 2.00
ELSEIF PALAM:PLAYER:윤활 < PALAMLV:2
	TIMES LOCAL , 1.00
	TIMES LOCAL:1 , 1.50
ELSEIF PALAM:PLAYER:윤활 < PALAMLV:3
	TIMES LOCAL , 0.60
ELSEIF PALAM:PLAYER:윤활 < PALAMLV:4
	TIMES LOCAL , 0.30
ELSEIF PALAM:PLAYER:윤활 >= PALAMLV:4
	TIMES LOCAL , 0.20
ENDIF
SOURCE:PLAYER:고통 += LOCAL
SOURCE:PLAYER:반감 += LOCAL:1

;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;자위 실행시는 경험＋1
SIF EXP_UP(22, PLAYER)
	EXP:PLAYER:A경험 ++

RETURN 1

;-------------------------------------------------
;실행 가능 판정
;-------------------------------------------------
@COM_ABLE745
;애널바이브 실행 판정
SIF !TFLAG:100
	RETURN 0
;일괄관리
SIF GLOBAL_COMABLE(745)
	RETURN RESULT
;해제는 언제라도 가능
SIF TEQUIP:14
	RETURN 1
;아이템 을 가지고 있는인가, 아이템 없음 설정이 아니면 안 돼
SIF ITEM:5 == 0 && NOITEM == 0
	RETURN 0
;윤활 부족해, 순종 3 이하·레즈끼 3 이하의 ASSI의 경우 애널바이브는 할 수 없다(새드라면 OK)
IF PALAM:윤활 < PALAMLV:2 && ASSIPLAY
	SIF (ABL:ASSI:순종 <= 3 || ABL:ASSI:레즈끼 <= 3) && TALENT:ASSI:새드 == 0
		RETURN 0
ENDIF

;하반신착의
SIF TEQUIP:0 & 4
	RETURN 0
;관장＋애널 플러그 사용중은 안 돼
SIF TEQUIP:21
	RETURN 0
;애널비즈 사용중은 불가
SIF TEQUIP:15
	RETURN 0
;풍선 사용중은 불가
SIF TEQUIP:22
	RETURN 0
;전극 사용중은 불가
SIF TEQUIP:23
	RETURN 0
RETURN 1

