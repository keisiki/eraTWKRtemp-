@COM461
;망상 자위를 그만두는 용
VARSET LOCAL

STAIN:MASTER:구 = 0
STAIN:MASTER:4 = 8

IF TALENT:MASTER:2 == 2
	STAIN:MASTER:1 = 6
	STAIN:MASTER:2 = 6
	STAIN:MASTER:5 = 0
ELSEIF TALENT:MASTER:2 == 1
	STAIN:MASTER:1 = 1
	STAIN:MASTER:3 = 1
	STAIN:MASTER:5 = 1
ELSE
	STAIN:MASTER:1 = 7
	STAIN:MASTER:2 = 7
	STAIN:MASTER:3 = 7
	STAIN:MASTER:5 = 7
ENDIF

STAIN:구 = 0
STAIN:1 = 0
STAIN:2 = 2
STAIN:3 = 1
STAIN:4 = 8
STAIN:5 = 0
STAIN:6 = 1
STAIN:7 = 8


EXP:MASTER:14 = BEFORE_MOUSOU_EXP14
EXP:MASTER:15 = BEFORE_MOUSOU_EXP15
EXP:MASTER:16 = BEFORE_MOUSOU_EXP16
EXP:MASTER:20 = BEFORE_MOUSOU_EXP20
EXP:MASTER:21 = BEFORE_MOUSOU_EXP21
EXP:MASTER:24 = BEFORE_MOUSOU_EXP24
EXP:MASTER:25 = BEFORE_MOUSOU_EXP25
EXP:MASTER:27 = BEFORE_MOUSOU_EXP27
EXP:MASTER:28 = BEFORE_MOUSOU_EXP28
EXP:MASTER:29 = BEFORE_MOUSOU_EXP29
EXP:MASTER:53 = BEFORE_MOUSOU_EXP53

SIF BEFORE_MOUSOU_EXP29 == 0 && TALENT:MASTER:2 >= 2
	TALENT:MASTER:1 = 0
SIF BEFORE_MOUSOU_EXP20 == 0 && TALENT:MASTER:2 != 2
	TALENT:MASTER:0 = 1

TFLAG:망상중 = 2
CFLAG:(CHARANUM - 1):현재위치 = SUKIMA()
CFLAG:(CHARANUM - 1):동행 = 0
CFLAG:(CHARANUM - 1):출금 = 1
CFLAG:TARGET:데이트중 = MAIN_MAP
CFLAG:MASTER:데이트중 = MAIN_MAP

CFLAG:MASTER:현재위치 = CFLAG:MASTER:311
	FOR LOCAL, 1, CHARANUM
		SIF CFLAG:LOCAL:현재위치 == CFLAG:MASTER:311
			CFLAG:LOCAL:현재위치 = CFLAG:LOCAL:311
	NEXT

;-------------------------------------------------
;실행 판정
