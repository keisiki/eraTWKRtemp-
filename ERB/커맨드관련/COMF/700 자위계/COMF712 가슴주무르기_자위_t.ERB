;-------------------------------------------------
;가슴주무르기
;애무계 커맨드, 레벨 1
;-------------------------------------------------
@COM712
;착탈의처리
TEQUIP:PLAYER:가슴주무르기자위 = !TEQUIP:PLAYER:가슴주무르기자위

;-------------------------------------------------
;COM별 FLAG의 설정
;-------------------------------------------------
;행위자
TCVAR:116 = PLAYER
;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비 체력, 기력

	DOWNBASE:PLAYER:체력 += 10
	DOWNBASE:PLAYER:기력 += 50

	SOURCE:PLAYER:사랑 = 15
	SOURCE:PLAYER:노출 = 20
	SOURCE:PLAYER:일탈 = 30
	SOURCE:PLAYER:반감 = 50

	SOURCE:PLAYER:쾌B += ABL:PLAYER:손가락 * 20 + 30

	SIF ABL:PLAYER:친밀 >= 7
		SOURCE:PLAYER:사랑 += (ABL:PLAYER:친밀 * 10)

	SIF TALENT:PLAYER:능숙한손가락
		TIMES SOURCE:PLAYER:쾌B , 1.50

	;-------------------------------------------------
	;특수소스처리
	;-------------------------------------------------
	;억압, 저항 소유에는, 억울소스 추가
	SIF TALENT:PLAYER:자기애 < 0 || TALENT:PLAYER:저항
		SOURCE:PLAYER:울굴 += 100

;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
SIF !FLAG:시간정지
	TIME += 5
;자위경험
CFLAG:PLAYER:자위했다 ++
;수음경험
EXP:PLAYER:수음경험 ++
RETURN 1
;-------------------------------------------------
;장착 계속의 처리
;-------------------------------------------------
@EQUIP_COM712(PLAYER)

SELECTCASE SELECTCOM
	CASE 700 TO 709,711,740 TO 748
		PRINTL ＜가슴주무르기 해제(위)＞
		TEQUIP:PLAYER:가슴주무르기자위 = !TEQUIP:PLAYER:가슴주무르기자위
	CASEELSE
;가슴주무르기 안
PRINTL ＜가슴주무르기 안(위)＞
;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------

	DOWNBASE:PLAYER:체력 += 5
	DOWNBASE:PLAYER:기력 += 20
	SOURCE:PLAYER:쾌B += (ABL:PLAYER:손가락 + 1) * 10
	SOURCE:PLAYER:노출 = 20
	SOURCE:PLAYER:일탈 = 30
	SOURCE:PLAYER:반감 = 30
	SOURCE:PLAYER:사랑 += ABL:PLAYER:손가락 * 5

EXP:PLAYER:수음경험 ++
ENDSELECT
RETURN 1

@MESSAGE_EQUIP_COM712
	PRINTFORM %OPPAI_DESCRIPTION(PLAYER)%를 비비어 하기 만

;-------------------------------------------------
;실행 가능 판정
;-------------------------------------------------
@COM_ABLE712
;가슴주무르기 실행 판정
SIF !TFLAG:100
	RETURN 0
;일괄관리
SIF GLOBAL_COMABLE(712)
	RETURN RESULT
;유방 전극 장착중은 안 돼
SIF TEQUIP:24
	RETURN 0
;[절벽]이라면 불가
SIF TALENT:가슴사이즈 == -2
	RETURN 0
RETURN 1

