;-------------------------------------------------
;수음
;자위계 커맨드, 레벨 3
;-------------------------------------------------
@COM709
;-------------------------------------------------
;COM별 FLAG의 설정
;-------------------------------------------------
;행위자
TCVAR:116 = PLAYER
;TCVAR:2 사정장소 플래그(-1=콘돔 1=질내 2=애널 3=수음 4=펠라티오 5=파이즈리 6=가랑이 7=다리 코키 8=체표 9=애널 봉사 10=도구 11=촉수)

LOCALS = {PLAYER}/
CSTR:PLAYER:10 += LOCALS
TCVAR:PLAYER:2 = 3
;강도
TCVAR:PLAYER:4 = 500
;봉사 쾌락
TFLAG:30 = 1
;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비 체력, 기력
DOWNBASE:PLAYER:체력 += 10
DOWNBASE:PLAYER:기력 += 100

SOURCE:PLAYER:사랑 = 50
SOURCE:PLAYER:성행동 = 250
SOURCE:PLAYER:달성 = 50
SOURCE:PLAYER:굴종 = 350
SOURCE:PLAYER:일탈 = 100
SOURCE:PLAYER:반감 = 100

;위 쪽으로 계산한 더러운 데이터
SOURCE:PLAYER:불결 = Y*10 + 60


;-------------------------------------------------
;특수 소스 처리
;-------------------------------------------------
;억압, 저항 소유에는, 억울소스 추가
SIF TALENT:PLAYER:자기애 < 0 || TALENT:PLAYER:저항
	SOURCE:PLAYER:울굴 += 200
;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
;사정에 관계없이 행해지는 처리는 여기에서
;노예의 손가락⇔조교자의 P의 더러움이 이동
STAIN:PLAYER:손 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:PLAYER:손

;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
SIF !FLAG:시간정지
	TIME += 5
;수음경험
EXP:PLAYER:수음경험 ++
;자위경험
CFLAG:PLAYER:자위했다 ++

RETURN 1

;-------------------------------------------------
;실행 가능 판정
;-------------------------------------------------
@COM_ABLE709
;수음 실행 판정
SIF !TFLAG:100
	RETURN 0
;SIF GLOBAL_COMABLE(709)
	RETURN RESULT
;남자인가 양성이 아니면 안 돼
SIF !(TALENT:2 & 2)
	RETURN 0
;밧줄 사용 중에는 불가
SIF TEQUIP:밧줄
	RETURN 0
RETURN 1

