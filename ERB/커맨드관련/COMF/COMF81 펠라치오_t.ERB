;-------------------------------------------------
;펠라치오
;봉사계 커맨드, 레벨 2
;-------------------------------------------------
@COM81
;-------------------------------------------------
;COM별 FLAG의 설정
;-------------------------------------------------
;행위자
TCVAR:116 = TARGET
;봉사 쾌락
TCVAR:108 = 1

CSTR:PLAYER:10 += @"{TARGET}/"
;TCVAR:2 사정장소 플래그(-1=콘돔 1=질내 2=애널 3=수음 4=펠라티오 5=파이즈리 6=가랑이 7=풋잡 8=체표 9=애널 봉사 10=도구 11=촉수 12=A고문 13=V 꾸짖고)
TCVAR:PLAYER:2 = 4
;강도
TCVAR:PLAYER:4 = 1000

;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비 체력, 기력
IF FLAG:70 == 1
	BASE:MASTER:TSP -= 50
	SOURCE:쾌M = 50

	SOURCE:PLAYER:쾌C = 110
	;성기술보정
	SOURCE:PLAYER:쾌C += MIN(ABL:혀 * 25,100)

	;조교자의 TALENT:고양이혀를 본다
	IF TALENT:고양이혀
		TIMES SOURCE:PLAYER:쾌C , 0.75
		;조교자가 고양이혀 소유의 경우는, 조교자에게 가학쾌락경험을 추가
		EXP:가학쾌락경험 += 1
	ENDIF

ELSE
	DOWNBASE:체력 += 10
	DOWNBASE:기력 += 80
	SOURCE:PLAYER:쾌C = 110
	SOURCE:쾌M = 50
	SOURCE:사랑 = 100
	SOURCE:성행동 = 300
	SOURCE:달성 = 150
	SOURCE:굴종 = 700
	SOURCE:반감 = 400
	SOURCE:일탈 = 100

	;위 쪽으로 계산한 더러운 데이터
	SOURCE:불결 = Y*10 + 60

	;-------------------------------------------------
	;특수소스처리
	;-------------------------------------------------
	;억압, 저항 소유에는, 억울소스 추가
	SIF TALENT:자기애 < 0 || TALENT:저항
		SOURCE:울굴 += 400

	;성기술보정
	IF ABL:혀 >= 4
		SOURCE:PLAYER:쾌C += 100
	ELSE
		SOURCE:PLAYER:쾌C += ABL:혀 * 25
	ENDIF

	;조교자의 TALENT:혀놀림을 본다
	SIF TALENT:혀놀림
		TIMES SOURCE:PLAYER:쾌C , 1.50

	;조교자의 TALENT:고양이혀를 본다
	SIF TALENT:고양이혀
		TIMES SOURCE:PLAYER:쾌C , 0.75

ENDIF

;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
;사정에 관계없이 행해지는 처리는 여기에서
;노예의 손가락⇔조교자의 P의 더러움이 이동
STAIN:구 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:구

;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;펠라 경험
;우선 M경험 추가
;SOURCE에 쾌M가 있으면 자동으로 경험 추가되므로, 사실 경험＋2 취급은 이상하지 않을까
;EXP:M경험 += 1

;동성애 레즈비언이라면 반환값 1 동성애자라면 반환값 2
SIF HETEROSEX(TARGET, PLAYER)
	EXP:PLAYER:(50 + HETEROSEX(TARGET, PLAYER)) += 7

IF !FLAG:70 && !CFLAG:장난
	EXP:펠라티오경험 += 3
	;노예가 고양이혀 소유의 경우는, 조교자에게 가학쾌락경험을 추가
	SIF TALENT:TARGET:고양이혀
		EXP:가학쾌락경험 += 1
	;동성애 레즈비언이라면 반환값 1 동성애자라면 반환값 2
	SIF HETEROSEX(TARGET, PLAYER)
		EXP:PLAYER:(50 + HETEROSEX(TARGET, PLAYER)) += 7
	IF HETEROSEX(TARGET, PLAYER)
		EXP:(50 + HETEROSEX(TARGET, PLAYER)) += 7
		EXP:PLAYER:(50 + HETEROSEX(TARGET, PLAYER)) += 7
	ENDIF
	;애정경험
	SIF TALENT:연모 && ASSIPLAY == 0
		EXP:애정경험 += 1
ELSE
	SIF !CFLAG:장난
		EXP:PLAYER:시간경험 += 1
ENDIF
IF CFLAG:PLAYER:장난 == 2
	EXP:PLAYER:수면간경험 += 1
	EXP:TARGET:수면간경험 += 1
ENDIF

;정액 더러움
SIF TCVAR:PLAYER:112 == 1 || TCVAR:PLAYER:112 == 2
	EXP:정음경험 ++
TCVAR:TARGET:FK상실 = 2
RETURN 1


@CAN_COM81(ARG)
SKIPDISP ARG
PRINTL 펠라치오
;-------------------------------------------------
;실행 판정 처리
;-------------------------------------------------
IF FLAG:70 == 1 || CFLAG:장난

ELSE
A = 0
S = 0

;모든 명령에 공통의 요소를 고려
;(순종이 높다고 명령에 따르기 쉬운 등)
CALL COM_ORDER

;ABL:욕망
IF ABL:욕망
	SIF S
		PRINT  + 
	A += ABL:욕망 * 1
	PRINT 욕망
	PRINTV 'LV, ABL:욕망, '(, ABL:욕망 * 1, ')
	S = 1
ENDIF
;ABL:봉사정신
IF ABL:봉사정신
	SIF S
		PRINT  + 
	A += ABL:봉사정신 * 4
	PRINT 봉사정신
	PRINTV 'LV, ABL:봉사정신, '(, ABL:봉사정신 * 4, ')
	S = 1
ENDIF
;ABL:정액중독
IF ABL:정액중독
	SIF S
		PRINT  + 
	A += ABL:정액중독 * 3
	PRINT 정액중독
	PRINTV 'LV, ABL:정액중독, '(, ABL:정액중독 * 3, ')
	S = 1
ENDIF

IF MARK:쾌락각인
	SIF S
		PRINT  + 
	A += MARK:쾌락각인 * 1
	PRINT 쾌락각인
	PRINTV 'LV, MARK:쾌락각인, '(, MARK:쾌락각인 * 1, ')
	S = 1
ENDIF
;PALAM:욕정
IF GETPALAMLV(PALAM:욕정, 5)
	SIF S
		PRINT  + 
	A += GETPALAMLV(PALAM:욕정, 5) * 1
	PRINT 욕정
	PRINTV 'LV, GETPALAMLV(PALAM:욕정, 5), '(, GETPALAMLV(PALAM:욕정, 5) * 1, ')
	S = 1
ENDIF
;수줍음
IF TALENT:수치심 > 0
	PRINT  - 
	A -= 1
	PRINT 수줍음
	PRINTV '(, 1, ')
	S = 1
ENDIF
;악취 둔감
IF TALENT:악취내성 > 0
	SIF S
		PRINT  + 
	A += 1
	PRINT 악취 둔감
	PRINTV '(, 1, ')
	S = 1
ENDIF
;악취 민감
IF TALENT:악취내성 < 0
	PRINT  - 
	A -= 3
	PRINT 악취 민감
	PRINTV '(, 3, ')
	S = 1
ENDIF
;헌신적
IF TALENT:헌신적
	SIF S
		PRINT  + 
	A += 6
	PRINT 헌신적
	PRINTV '(, 6, ')
	S = 1
ENDIF
;쾌감에 부정(음란, 창부로 무효)
IF TALENT:쾌감응답 < 0 && !TALENT:음란
	PRINT  - 
	A -= 1
	PRINT 쾌감에 부정
	PRINTV '(, 1, ')
	S = 1
ENDIF
;남성기피(연모, 친애로 무효)
IF OTOKOGIRAI(TARGET)
	PRINT  - 
	A -= 12
	PRINT 남성기피
	PRINTV '(, 12, ')
	S = 1
ENDIF
;연모
IF TALENT:연모 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 연모
	PRINTV '(, 5, ')
	S = 5
ENDIF
;친애
IF TALENT:연모 > 1 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 친애
	PRINTV '(, 5, ')
	S = 5
ENDIF

;조교자가 후타나리
IF (TALENT:PLAYER:2 & 3) == 3
	SIF S
		PRINT  + 
	A += 8
	PRINT 후타나리
	PRINTV '(, 8, ')
	S = 1
ENDIF
IF CFLAG:패배자
	SIF S
		PRINT  + 
	A += 20
	PRINT 패배자
	PRINTV '(,20,')
	S = 1
ENDIF
Y = 0
;애액의 더러움
SIF STAIN:PLAYER:2 & 1
	Y += 1
;정액의 더러움
SIF STAIN:PLAYER:2 & 4
	Y += 3
;애널의 더러움
SIF STAIN:PLAYER:2 & 8
	Y += 7
SIF STAIN:PLAYER:2 & 16
	Y += 15

Y = Y * (2 - TALENT:악취내성) / 2

;더러움 있음
IF Y
	PRINT  - 
	A -= Y
	PRINT 더러움 있음
	;악취 둔감
	IF TALENT:악취내성 > 0
		PRINT , 악취 둔감
	ELSEIF TALENT:악취내성 < 0
		PRINT , 악취 민감
	ENDIF
	PRINTV '(, Y, ')
	S = 1
ENDIF

;합계를 표시(24이상으로 실행)
PRINT  = 
PRINTV A

V = 24
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 실행치
PRINTV V

WAIT
SKIPDISP 0
;실행할 수 없다
SIF A < V
	RETURN 0
ENDIF
RETURN 1

;-------------------------------------------------
;실행 판정
;-------------------------------------------------
@COM_ABLE81
;펠라치오 실행 판정
SIF !TFLAG:100
	RETURN 0
SIF GLOBAL_COMABLE(81)
	RETURN RESULT
;조교자가 남자나 반인반요인가 후타나리이 아니면 안 돼
SIF !(TALENT:PLAYER:2 & 2)
	RETURN 0
;시간정지중은 허가
IF FLAG:70 != 1
	;고양이혀의 노예의 경우는 기교 Lv2 이상 필요
	SIF TALENT:고양이혀 && ABL:기교 < 2
		RETURN 0
	;난쟁이 사이즈라고 입속에 들어오지 않는다
	SIF 삽입불가(TARGET) == 3
		RETURN 0
ENDIF
;볼개그 사용중은 불가
SIF TEQUIP:볼개그
	RETURN 0
RETURN 1
