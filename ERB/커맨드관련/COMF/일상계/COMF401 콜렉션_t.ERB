;콜렉션
;일상계 커맨드, 레벨 1
;-------------------------------------------------
@COM401
#DIM 종류
FLAG:700 = 0
FOR LOCAL, 1, CHARANUM
	FOR 종류, 0, MAXPANTS
		CFLAG:LOCAL:(종류 + 팬티관리) += PANTS_TEMP:LOCAL:종류
		SIF PANTS_TEMP:LOCAL:종류
		PRINTFORML %CALLNAME:LOCAL%의 %조사처리(PANTSNAME(종류, LOCAL),"를")% 콜렉션에 추가했다
		PANTS_TEMP:LOCAL:종류 = 0
		CFLAG:LOCAL:293 = 0
		SIF CFLAG:LOCAL:(종류 + 팬티관리)
			FLAG:700 ++
	NEXT
NEXT
FLAG:팬티수송 = 0

PRINTFORMW 콜렉션은 현재{FLAG:700}종이다
RETURN 1

;콜렉션
@COM_ABLE401
;이동 실행 판정
SIF !TFLAG:100
	RETURN 0
;일괄관리
SIF GLOBAL_COMABLE(401)
	RETURN RESULT
;개인 방 설정
SIF CFLAG:MASTER:현재위치 != CFLAG:MASTER:311
	RETURN 0
;SIF CSTR:MASTER:12 == ""
SIF !FLAG:팬티수송
	RETURN 0
SIF CFLAG:우후후 == 2
	RETURN 0
RETURN 1