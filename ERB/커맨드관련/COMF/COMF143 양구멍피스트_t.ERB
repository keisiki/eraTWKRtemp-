;-------------------------------------------------
;두 구멍 피스트
;하드한 조교 커맨드, 레벨 4
;-------------------------------------------------
@COM143
;-------------------------------------------------
;COM별 FLAG의 설정
;-------------------------------------------------
;행위자
TCVAR:116 = PLAYER

CSTR:TARGET:10 += @"{PLAYER}/"
;TCVAR:2 사정장소 플래그(-1=콘돔 1=질내 2=애널 3=수음 4=펠라티오 5=파이즈리 6=가랑이 7=풋잡 8=체표 9=애널 봉사 10=도구 11=촉수 12=A고문 13=V 꾸짖고)
IF ABL:V감각 >= ABL:A감각
	TCVAR:TARGET:2 = 13
ELSE
	TCVAR:TARGET:2 = 12
ENDIF
;강도
TCVAR:TARGET:4 = 1800

;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비 체력, 기력
IF FLAG:70 == 1
	BASE:MASTER:TSP -= 500
	SOURCE:쾌V = 200
	SOURCE:쾌A = 200
	SOURCE:고통 = 900

	;피스트등의 V에의 큰 고통은 먼저 처리해 둔다
	;EXP:V경험을 본다
	IF EXP:V경험 < EXPLV:1
		TIMES SOURCE:고통 , 2.50
	ELSEIF EXP:V경험 < EXPLV:2
		TIMES SOURCE:고통 , 2.00
	ELSEIF EXP:V경험 < EXPLV:3
		TIMES SOURCE:고통 , 1.80
	ELSEIF EXP:V경험 < EXPLV:4
		TIMES SOURCE:고통 , 1.60
	ELSEIF EXP:V경험 < EXPLV:5
		TIMES SOURCE:고통 , 1.40
	ELSEIF EXP:V경험 >= EXPLV:5
		TIMES SOURCE:고통 , 1.20
	ENDIF

	;PALAM:윤활을 본다
	IF PALAM:윤활 < PALAMLV:1
		TIMES SOURCE:고통 , 3.00
	ELSEIF PALAM:윤활 < PALAMLV:2
		TIMES SOURCE:고통 , 2.50
	ELSEIF PALAM:윤활 < PALAMLV:3
		TIMES SOURCE:고통 , 2.00
	ELSEIF PALAM:윤활 < PALAMLV:4
		TIMES SOURCE:고통 , 1.50
	ELSEIF PALAM:윤활 >= PALAMLV:4
		TIMES SOURCE:고통 , 1.00
	ENDIF


	;피스트등의 A에의 큰 고통은 먼저 처리해 둔다
	;EXP:A경험을 본다
	IF EXP:A경험 < EXPLV:1
		TIMES SOURCE:고통 , 3.00
	ELSEIF EXP:A경험 < EXPLV:2
		TIMES SOURCE:고통 , 2.50
	ELSEIF EXP:A경험 < EXPLV:3
		TIMES SOURCE:고통 , 2.40
	ELSEIF EXP:A경험 < EXPLV:4
		TIMES SOURCE:고통 , 2.00
	ELSEIF EXP:A경험 < EXPLV:5
		TIMES SOURCE:고통 , 1.80
	ELSEIF EXP:A경험 >= EXPLV:5
		TIMES SOURCE:고통 , 1.60
	ENDIF

	;PALAM:윤활을 본다
	IF PALAM:윤활 < PALAMLV:1
		TIMES SOURCE:고통 , 3.00
	ELSEIF PALAM:윤활 < PALAMLV:2
		TIMES SOURCE:고통 , 2.50
	ELSEIF PALAM:윤활 < PALAMLV:3
		TIMES SOURCE:고통 , 2.00
	ELSEIF PALAM:윤활 < PALAMLV:4
		TIMES SOURCE:고통 , 1.50
	ELSEIF PALAM:윤활 >= PALAMLV:4
		TIMES SOURCE:고통 , 1.00
	ENDIF

ELSE

	DOWNBASE:체력 += 500
	DOWNBASE:기력 += 300

	SOURCE:쾌V = 200
	SOURCE:쾌A = 200
	SOURCE:고통 = 1800
	SOURCE:노출 = 100
	SOURCE:굴종 = 2000
	SOURCE:일탈 = 2000
	SOURCE:반감 = 2000


	;-------------------------------------------------
	;특수소스처리
	;-------------------------------------------------
	;피스트등의 V에의 큰 고통은 먼저 처리해 둔다
	;EXP:V경험을 본다
	IF EXP:V경험 < EXPLV:1
		TIMES SOURCE:고통 , 2.50
		TIMES SOURCE:반감 , 4.00
	ELSEIF EXP:V경험 < EXPLV:2
		TIMES SOURCE:고통 , 2.00
		TIMES SOURCE:반감 , 3.50
	ELSEIF EXP:V경험 < EXPLV:3
		TIMES SOURCE:고통 , 1.80
		TIMES SOURCE:반감 , 3.00
	ELSEIF EXP:V경험 < EXPLV:4
		TIMES SOURCE:고통 , 1.60
		TIMES SOURCE:반감 , 2.50
	ELSEIF EXP:V경험 < EXPLV:5
		TIMES SOURCE:고통 , 1.40
		TIMES SOURCE:반감 , 2.00
	ELSEIF EXP:V경험 >= EXPLV:5
		TIMES SOURCE:고통 , 1.20
		TIMES SOURCE:반감 , 2.00
	ENDIF

	;PALAM:윤활을 본다
	IF PALAM:윤활 < PALAMLV:1
		TIMES SOURCE:고통 , 3.00
		TIMES SOURCE:반감 , 2.00
	ELSEIF PALAM:윤활 < PALAMLV:2
		TIMES SOURCE:고통 , 2.50
		TIMES SOURCE:반감 , 1.50
	ELSEIF PALAM:윤활 < PALAMLV:3
		TIMES SOURCE:고통 , 2.00
		TIMES SOURCE:반감 , 1.20
	ELSEIF PALAM:윤활 < PALAMLV:4
		TIMES SOURCE:고통 , 1.50
	ELSEIF PALAM:윤활 >= PALAMLV:4
		TIMES SOURCE:고통 , 1.00
	ENDIF


	;피스트등의 A에의 큰 고통은 먼저 처리해 둔다
	;EXP:A경험을 본다
	IF EXP:A경험 < EXPLV:1
		TIMES SOURCE:고통 , 3.00
		TIMES SOURCE:반감 , 4.00
	ELSEIF EXP:A경험 < EXPLV:2
		TIMES SOURCE:고통 , 2.50
		TIMES SOURCE:반감 , 3.50
	ELSEIF EXP:A경험 < EXPLV:3
		TIMES SOURCE:고통 , 2.40
		TIMES SOURCE:반감 , 3.00
	ELSEIF EXP:A경험 < EXPLV:4
		TIMES SOURCE:고통 , 2.00
		TIMES SOURCE:반감 , 2.50
	ELSEIF EXP:A경험 < EXPLV:5
		TIMES SOURCE:고통 , 1.80
		TIMES SOURCE:반감 , 2.00
	ELSEIF EXP:A경험 >= EXPLV:5
		TIMES SOURCE:고통 , 1.60
		TIMES SOURCE:반감 , 1.50
	ENDIF

	;PALAM:윤활을 본다
	IF PALAM:윤활 < PALAMLV:1
		TIMES SOURCE:고통 , 3.00
		TIMES SOURCE:반감 , 2.00
	ELSEIF PALAM:윤활 < PALAMLV:2
		TIMES SOURCE:고통 , 2.50
		TIMES SOURCE:반감 , 1.50
	ELSEIF PALAM:윤활 < PALAMLV:3
		TIMES SOURCE:고통 , 2.00
		TIMES SOURCE:반감 , 1.20
	ELSEIF PALAM:윤활 < PALAMLV:4
		TIMES SOURCE:고통 , 1.50
	ELSEIF PALAM:윤활 >= PALAMLV:4
		TIMES SOURCE:고통 , 1.00
	ENDIF
ENDIF

;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;V경험
EXP:V경험 += 24

;A경험
EXP:A경험 += 24

;V, A확장경험
IF !EXP:A확장경험 && !EXP:V확장경험 && !GETBIT(CFLAG:3, 5)
	EXP:이상경험 += 1
	SETBIT CFLAG:3, 5
ENDIF

EXP:A확장경험 += 5
EXP:V확장경험 += 5
;동성애 레즈비언이라면 반환값 1 동성애자라면 반환값 2
IF FLAG:70 == 1
	IF HETEROSEX(TARGET, PLAYER)
		EXP:PLAYER:(50 + HETEROSEX(TARGET, PLAYER)) += 1
	ENDIF
ELSE
	IF HETEROSEX(TARGET, PLAYER)
		EXP:(50 + HETEROSEX(TARGET, PLAYER)) += 1
		EXP:PLAYER:(50 + HETEROSEX(TARGET, PLAYER)) += 1
	ENDIF
ENDIF
RETURN 1

;-------------------------------------------------
;실행 판정
;-------------------------------------------------
@COM_ABLE143
;두 구멍 피스트
SIF !TFLAG:100
	RETURN 0
;일괄관리
SIF GLOBAL_COMABLE(143)
	RETURN RESULT
;바이브 사용중은 불가
SIF TEQUIP:13
	RETURN 0
;애널바이브 사용중은 불가
SIF TEQUIP:14
	RETURN 0
;관장＋애널 플러그 사용중은 불가
SIF TEQUIP:21
	RETURN 0
;애널비즈 사용중은 불가
SIF TEQUIP:15
	RETURN 0
;풍선 사용중은 불가
SIF TEQUIP:22
	RETURN 0
;전극 사용중은 불가
SIF TEQUIP:23
	RETURN 0
;남자라면 안 돼
SIF !(TALENT:2 & 1)
	RETURN 0
;ASSI의 경우, 순종 4이상이나 새드가 아니면 할 수 없다.
IF ASSIPLAY
	SIF ABL:ASSI:순종 < 4 && TALENT:ASSI:새드 == 0
		RETURN 0
ENDIF
IF FLAG:70 == 1
ELSE
	;일정한 V·A경험이 필요
	IF EXP:0 < 300 || EXP:1 < 300
		RETURN 0
	ENDIF
	;작은 체형이라면 순종이 4이상 또한 마조끼이 4이상 또한 확장경험 10이상이 아니면 실행할 수 없다
	IF TALENT:체형 < 0
		SIF ABL:순종 <= 3 || ABL:마조끼 <= 3 || EXP:V확장경험 < 10 || EXP:A확장경험 < 10
			RETURN 0
	ENDIF
ENDIF
;조교 대상이 난쟁이 체형의 경우는 주인이 금단의지식 소유가 아니면 안 돼
SIF ABS(TALENT:체형 - TALENT:PLAYER:체형) > 2 && TALENT:MASTER:금단의지식 == 0
	RETURN 0

;하반신착의
SIF TEQUIP:0 > 1
	RETURN 0
;샤워중은 안 돼
SIF TEQUIP:32
	RETURN 0
;목욕탕플레이중은 플레이매트가 없으면 안 돼
IF TEQUIP:31
	SIF ITEM:20 == 0 && NOITEM == 0
		RETURN 0
ENDIF
;새댁플레이중은 안 돼
SIF TEQUIP:33
	RETURN 0
;침대 룸이 아니면 안 돼
SIF !BEDROOM(CFLAG:PLAYER:현재위치)
	RETURN 0
RETURN 1

