@COM632
PRINTFORML 전투의 체험판입니다. 해보겠습니까?
CALL ASK_M("안 한다", 1,"엄청 약한 적", 1,"약한 적", 1)
IF !RESULT
	RETURN -1
ELSE
	CALL BATTLE_SYSTEM(RESULT)
	IF RESULT == -1
		PRINTFORMW 졌다!
	ELSEIF RESULT == 1
		PRINTFORMW 이겼다! 장하다!
	ENDIF
ENDIF


@COM_ABLE632
;일괄관리
SIF GLOBAL_COMABLE(632)
	RETURN RESULT
SIF !TFLAG:100
	RETURN 0
;정지 중에는 불가
SIF FLAG:70 == 1
	RETURN 0
;업무중
SIF CFLAG:행동 == 5
	RETURN 0
SIF !전투훈련가능(CFLAG:MASTER:현재위치)
	RETURN 0
;기력 0
SIF BASE:MASTER:기력 <= 0
	RETURN 0
SIF CFLAG:우후후 == 2
	RETURN 0
RETURN 1
