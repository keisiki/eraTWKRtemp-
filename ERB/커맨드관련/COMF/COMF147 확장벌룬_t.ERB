;-------------------------------------------------
;확장벌룬
;하드한 조교 커맨드, 레벨 4
;-------------------------------------------------
@COM147
;착탈의처리
TEQUIP:확장벌룬 = !TEQUIP:확장벌룬

CSTR:TARGET:10 += @"{PLAYER}/"
;TCVAR:2 사정장소 플래그(-1=콘돔 1=질내 2=애널 3=수음 4=펠라티오 5=파이즈리 6=가랑이 7=풋잡 8=체표 9=애널 봉사 10=도구 11=촉수 12=A고문 13=V 꾸짖고)
TCVAR:TARGET:2 = 12
;강도
TCVAR:TARGET:4 = 1200

;-------------------------------------------------
;COM별 FLAG의 설정
;-------------------------------------------------
;행위자
TCVAR:116 = PLAYER
;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비 체력, 기력
IF FLAG:70 == 1
	BASE:MASTER:TSP -= 150
	SOURCE:쾌A = 200
	SOURCE:고통 = 400

	;A에의 고통은 먼저 처리해 둔다
	;EXP:A경험을 본다
	IF EXP:A경험 < EXPLV:1
		TIMES SOURCE:고통 , 3.00
	ELSEIF EXP:A경험 < EXPLV:2
		TIMES SOURCE:고통 , 1.20
	ELSEIF EXP:A경험 < EXPLV:3
		TIMES SOURCE:고통 , 0.80
	ELSEIF EXP:A경험 < EXPLV:4
		TIMES SOURCE:고통 , 0.60
	ELSEIF EXP:A경험 < EXPLV:5
		TIMES SOURCE:고통 , 0.40
	ELSEIF EXP:A경험 >= EXPLV:5
		TIMES SOURCE:고통 , 0.20
	ENDIF

	;PALAM:윤활을 본다
	IF PALAM:윤활 < PALAMLV:1
		TIMES SOURCE:고통 , 1.20
	ELSEIF PALAM:윤활 < PALAMLV:2
		TIMES SOURCE:고통 , 1.00
	ELSEIF PALAM:윤활 < PALAMLV:3
		TIMES SOURCE:고통 , 0.60
	ELSEIF PALAM:윤활 < PALAMLV:4
		TIMES SOURCE:고통 , 0.30
	ELSEIF PALAM:윤활 >= PALAMLV:4
		TIMES SOURCE:고통 , 0.20
	ENDIF

ELSE

	DOWNBASE:체력 += 150
	DOWNBASE:기력 += 200

	SOURCE:쾌A = 200
	SOURCE:고통 = 800
	SOURCE:노출 = 300
	SOURCE:굴종 = 400
	SOURCE:일탈 = 700
	SOURCE:반감 = 700


	;-------------------------------------------------
	;특수소스처리
	;-------------------------------------------------
	;억압, 저항 소유에는, 억울소스 추가
	SIF TALENT:자기애 < 0 || TALENT:저항
		SOURCE:울굴 += 700

	;A에의 고통은 먼저 처리해 둔다
	;EXP:A경험을 본다
	IF EXP:A경험 < EXPLV:1
		TIMES SOURCE:고통 , 3.00
		TIMES SOURCE:반감 , 3.00
	ELSEIF EXP:A경험 < EXPLV:2
		TIMES SOURCE:고통 , 1.20
		TIMES SOURCE:반감 , 2.00
	ELSEIF EXP:A경험 < EXPLV:3
		TIMES SOURCE:고통 , 0.80
		TIMES SOURCE:반감 , 1.50
	ELSEIF EXP:A경험 < EXPLV:4
		TIMES SOURCE:고통 , 0.60
		TIMES SOURCE:반감 , 1.20
	ELSEIF EXP:A경험 < EXPLV:5
		TIMES SOURCE:고통 , 0.40
	ELSEIF EXP:A경험 >= EXPLV:5
		TIMES SOURCE:고통 , 0.20
	ENDIF

	;PALAM:윤활을 본다
	IF PALAM:윤활 < PALAMLV:1
		TIMES SOURCE:고통 , 1.20
		TIMES SOURCE:반감 , 2.00
	ELSEIF PALAM:윤활 < PALAMLV:2
		TIMES SOURCE:고통 , 1.00
		TIMES SOURCE:반감 , 1.50
	ELSEIF PALAM:윤활 < PALAMLV:3
		TIMES SOURCE:고통 , 0.60
	ELSEIF PALAM:윤활 < PALAMLV:4
		TIMES SOURCE:고통 , 0.30
	ELSEIF PALAM:윤활 >= PALAMLV:4
		TIMES SOURCE:고통 , 0.20
	ENDIF
ENDIF
;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
STAIN:7 |= STAIN:4
STAIN:4 |= STAIN:7
;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
EXP:A경험 += 4

IF !EXP:V확장경험 && !EXP:A확장경험 && !GETBIT(CFLAG:3, 5)
	EXP:이상경험 += 1
	SETBIT CFLAG:3, 5
ENDIF

EXP:A확장경험 += 1
;동성애 레즈비언이라면 반환값 1 동성애자라면 반환값 2
IF FLAG:70 == 1
	IF HETEROSEX(TARGET, PLAYER)
		EXP:PLAYER:(50 + HETEROSEX(TARGET, PLAYER)) += 1
	ENDIF
ELSE
	IF HETEROSEX(TARGET, PLAYER)
		EXP:(50 + HETEROSEX(TARGET, PLAYER)) += 1
		EXP:PLAYER:(50 + HETEROSEX(TARGET, PLAYER)) += 1
	ENDIF
ENDIF
RETURN 1
;-------------------------------------------------
;장착 계속의 처리
;-------------------------------------------------
@EQUIP_COM147(ARG)
;확장벌룬

PRINTL ＜확장벌룬 삽입중＞

CSTR:TARGET:10 += @"{PLAYER}/"
;TCVAR:2 사정장소 플래그(-1=콘돔 1=질내 2=애널 3=수음 4=펠라티오 5=파이즈리 6=가랑이 7=풋잡 8=체표 9=애널 봉사 10=도구 11=촉수 12=A고문 13=V 꾸짖고)
TCVAR:TARGET:2 = 12
;강도
TCVAR:TARGET:4 = 600

IF FLAG:70 == 1
	BASE:MASTER:TSP -= 70
	SOURCE:ARG:쾌A += 100

	;A에의 고통은 먼저 처리해 둔다
	;EXP:A경험을 본다
	LOCAL = 400
	IF EXP:ARG:A경험 < EXPLV:1
		TIMES LOCAL , 3.00
	ELSEIF EXP:ARG:A경험 < EXPLV:2
		TIMES LOCAL , 1.20
	ELSEIF EXP:ARG:A경험 < EXPLV:3
		TIMES LOCAL , 0.80
	ELSEIF EXP:ARG:A경험 < EXPLV:4
		TIMES LOCAL , 0.60
	ELSEIF EXP:ARG:A경험 < EXPLV:5
		TIMES LOCAL , 0.40
	ELSEIF EXP:ARG:A경험 >= EXPLV:5
		TIMES LOCAL , 0.20
	ENDIF

	;PALAM:윤활을 본다
	IF PALAM:ARG:윤활 < PALAMLV:1
		TIMES LOCAL , 1.20
	ELSEIF PALAM:ARG:윤활 < PALAMLV:2
		TIMES LOCAL , 1.00
	ELSEIF PALAM:ARG:윤활 < PALAMLV:3
		TIMES LOCAL , 0.60
	ELSEIF PALAM:ARG:윤활 < PALAMLV:4
		TIMES LOCAL , 0.30
	ELSEIF PALAM:ARG:윤활 >= PALAMLV:4
		TIMES LOCAL , 0.20
	ENDIF
	SOURCE:ARG:고통 += LOCAL

ELSE


	DOWNBASE:ARG:체력 += 75
	DOWNBASE:ARG:기력 += 100

	SOURCE:ARG:쾌A += 100
	SOURCE:ARG:노출 += 150
	SOURCE:ARG:굴종 += 200
	SOURCE:ARG:일탈 += 350

	;-------------------------------------------------
	;특수소스처리
	;-------------------------------------------------
	;억압, 저항 소유에는, 억울소스 추가
	SIF TALENT:ARG:자기애 < 0 || TALENT:ARG:저항
		SOURCE:ARG:울굴 += 350

	;A에의 고통은 먼저 처리해 둔다
	;EXP:A경험을 본다
	LOCAL = 400
	LOCAL:1 = 350
	IF EXP:ARG:A경험 < EXPLV:1
		TIMES LOCAL , 3.00
		TIMES LOCAL:1 , 3.00
	ELSEIF EXP:ARG:A경험 < EXPLV:2
		TIMES LOCAL , 1.20
		TIMES LOCAL:1 , 2.00
	ELSEIF EXP:ARG:A경험 < EXPLV:3
		TIMES LOCAL , 0.80
		TIMES LOCAL:1 , 1.50
	ELSEIF EXP:ARG:A경험 < EXPLV:4
		TIMES LOCAL , 0.60
		TIMES LOCAL:1 , 1.20
	ELSEIF EXP:ARG:A경험 < EXPLV:5
		TIMES LOCAL , 0.40
	ELSEIF EXP:ARG:A경험 >= EXPLV:5
		TIMES LOCAL , 0.20
	ENDIF

	;PALAM:윤활을 본다
	IF PALAM:ARG:윤활 < PALAMLV:1
		TIMES LOCAL , 1.20
		TIMES LOCAL:1 , 2.00
	ELSEIF PALAM:ARG:윤활 < PALAMLV:2
		TIMES LOCAL , 1.00
		TIMES LOCAL:1 , 1.50
	ELSEIF PALAM:ARG:윤활 < PALAMLV:3
		TIMES LOCAL , 0.60
	ELSEIF PALAM:ARG:윤활 < PALAMLV:4
		TIMES LOCAL , 0.30
	ELSEIF PALAM:ARG:윤활 >= PALAMLV:4
		TIMES LOCAL , 0.20
	ENDIF
	SOURCE:ARG:고통 += LOCAL
	SOURCE:ARG:반감 += LOCAL:1
ENDIF
;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
EXP:ARG:A경험 += 1
EXP:ARG:A확장경험 += 1

;-------------------------------------------------
;실행 판정
;-------------------------------------------------
@COM_ABLE147
;확장벌룬 실행 판정
SIF !TFLAG:100
	RETURN 0
;일괄관리
SIF GLOBAL_COMABLE(147)
	RETURN RESULT
;하반신착의
SIF TEQUIP:0 > 1
	RETURN 0
;해제는 언제라도 가능
SIF TEQUIP:22
	RETURN 1
;아이템 을 가지고 있는인가, 아이템 없음 설정이 아니면 안 돼
SIF ITEM:8 == 0 && NOITEM == 0
	RETURN 0
;애널바이브 사용중은 불가
SIF TEQUIP:14
	RETURN 0
;애널비즈 사용중은 불가
SIF TEQUIP:15
	RETURN 0
;보통 관장 사용중은 불가
SIF TEQUIP:21
	RETURN 0
;전극 사용중은 불가
SIF TEQUIP:23
	RETURN 0
RETURN 1

