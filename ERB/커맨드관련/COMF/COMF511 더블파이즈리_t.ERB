;-------------------------------------------------
;2 개 펠라(노예에 2 개빨아야만 하는)
;봉사, 레즈비언계 파생 커맨드
;-------------------------------------------------
@COM511
PRINTL 2 개 펠라
CALL CAN_COM511(1)
SIF !RESULT
	RETURN 0
;-------------------------------------------------
;실행 결정
;-------------------------------------------------
STR:0 = 2 개 펠라
SELECTCOM = 511
;-------------------------------------------------
;COM별 FLAG의 설정
;-------------------------------------------------
;행위자
TCVAR:116 = PLAYER
;TCVAR:0 사정 대상 플래그(비트 0=TARGET 1=PLAYER 2=MASTER 3=ASSI)
;TCVAR:1 예비 사정 대상 플래그(비트를 사용하고 싶지 않은 인용, CFLAG:100으로 번역됩니다)	(1=TARGET 2=PLAYER 3=MASTER 4=ASSI 5=TARGET+ASSI 6=MASTER+ASSI)
;TCVAR:2 사정장소 플래그(-1=콘돔 1=질내 2=애널 3=수음 4=펠라티오 5=파이즈리 6=가랑이 7=풋잡 8=체표 9=애널 봉사 10=도구 11=촉수)

SETBIT TCVAR:MASTER:0, 0
TCVAR:MASTER:2 = 4
;강도
TCVAR:MASTER:4 = 750

SETBIT TCVAR:ASSI:0, 0
TCVAR:ASSI:2 = 4
;강도
TCVAR:ASSI:4 = 750

;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비 체력, 기력
DOWNBASE:체력 += 80
DOWNBASE:기력 += 250

SOURCE:사랑 = 150
SOURCE:성행동 = 400
SOURCE:달성 = 300
SOURCE:굴종 = 1000
SOURCE:일탈 = 500
SOURCE:반감 = 500


;-------------------------------------------------
;특수소스처리
;-------------------------------------------------
;억압, 저항 소유에는, 억울소스 추가
SIF TALENT:자기애 < 0 || TALENT:저항
	SOURCE:울굴 += 700
;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
;노예의 입⇔주인의 P의 더러움이 이동
STAIN:구 |= STAIN:MASTER:2
STAIN:MASTER:2 |= STAIN:구

;노예의 입⇔조수의 P의 더러움이 이동
STAIN:구 |= STAIN:ASSI:2
STAIN:ASSI:2 |= STAIN:구

;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;펠라 경험
EXP:펠라티오경험 += 2

;동성애 레즈비언이라면 반환값 1 동성애자라면 반환값 2
SIF HETEROSEX(TARGET, PLAYER)
	EXP:(50 + HETEROSEX(TARGET, PLAYER)) += 5
;동성애 레즈비언이라면 반환값 1 동성애자라면 반환값 2
SIF HETEROSEX(TARGET, ASSI)
	EXP:(50 + HETEROSEX(TARGET, ASSI)) += 5

;노예가 고양이혀 소유의 경우는, 조교자에게 가학쾌락경험을 추가
SIF TALENT:TARGET:고양이혀
	EXP:가학쾌락경험 += 1
TCVAR:TARGET:FK상실 = 2

RETURN 1

@CAN_COM511(ARG)
SKIPDISP ARG
;-------------------------------------------------
;실행 판정 처리
;-------------------------------------------------
A = 0
S = 0

;모든 명령에 공통의 요소를 고려
;(순종이 높다고 명령에 따르기 쉬운 등)
CALL COM_ORDER

;ABL:욕망
IF ABL:욕망
	SIF S
		PRINT  + 
	A += ABL:욕망 * 1
	PRINT 욕망
	PRINTV 'LV, ABL:욕망, '(, ABL:욕망 * 1, ')
	S = 1
ENDIF
;ABL:봉사정신
IF ABL:봉사정신
	SIF S
		PRINT  + 
	A += ABL:봉사정신 * 4
	PRINT 봉사정신
	PRINTV 'LV, ABL:봉사정신, '(, ABL:봉사정신 * 4, ')
	S = 1
ENDIF
;ABL:정액중독
IF ABL:정액중독
	SIF S
		PRINT  + 
	A += ABL:정액중독 * 3
	PRINT 정액중독
	PRINTV 'LV, ABL:정액중독, '(, ABL:정액중독 * 3, ')
	S = 1
ENDIF

IF MARK:쾌락각인
	SIF S
		PRINT  + 
	A += MARK:쾌락각인 * 1
	PRINT 쾌락각인
	PRINTV 'LV, MARK:쾌락각인, '(, MARK:쾌락각인 * 1, ')
	S = 1
ENDIF
;PALAM:욕정
IF GETPALAMLV(PALAM:욕정, 5)
	SIF S
		PRINT  + 
	A += GETPALAMLV(PALAM:욕정, 5) * 1
	PRINT 욕정
	PRINTV 'LV, GETPALAMLV(PALAM:욕정, 5), '(, GETPALAMLV(PALAM:욕정, 5) * 1, ')
	S = 1
ENDIF
;수줍음
IF TALENT:수치심 > 0
	PRINT  - 
	A -= 1
 	PRINT 수줍음
 	PRINTV '(, 1, ')
	S = 1
ENDIF
;악취 둔감
IF TALENT:악취내성 > 0
	SIF S
		PRINT  + 
	A += 1
	PRINT 악취 둔감
	PRINTV '(, 1, ')
	S = 1
ENDIF
;악취 민감
IF TALENT:악취내성 < 0
	PRINT  - 
	A -= 3
	PRINT 악취 민감
	PRINTV '(, 3, ')
	S = 1
ENDIF
;헌신적
IF TALENT:헌신적
	SIF S
		PRINT  + 
	A += 6
	PRINT 헌신적
	PRINTV '(, 6, ')
	S = 1
ENDIF
;쾌감에 부정(음란, 창부로 무효)
IF TALENT:쾌감응답 < 0 && !TALENT:음란
	PRINT  - 
	A -= 1
	PRINT 쾌감에 부정
	PRINTV '(, 1, ')
	S = 1
ENDIF
;음란
IF TALENT:음란 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 음란
	PRINTV '(, 5, ')
	S = 5
ENDIF
;창부
IF TALENT:음란 > 1 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 창부
	PRINTV '(, 5, ')
	S = 1
ENDIF
;조교자가 후타나리
IF (TALENT:PLAYER:2 & 3) == 3
	SIF S
		PRINT  + 
	A += 8
	PRINT 후타나리
	PRINTV '(, 8, ')
	S = 1
ENDIF

Y = 0
;애액의 더러움
SIF STAIN:PLAYER:2 & 1
	Y += 1
;정액의 더러움
SIF STAIN:PLAYER:2 & 4
	Y += 3
;애널의 더러움
SIF STAIN:PLAYER:2 & 8
	Y += 7
SIF STAIN:PLAYER:2 & 16
	Y += 15

Y = Y * (2 - TALENT:악취내성) / 2

;서로 더러워져
IF Y
	PRINT  - 
	A -= Y
	PRINT 서로 더러워져
	;악취 둔감
	IF TALENT:악취내성 > 0
		PRINT , 악취 둔감
	ELSEIF TALENT:악취내성 < 0
		PRINT , 악취 민감
	ENDIF
	PRINTV '(, Y, ')
	S = 1
ENDIF


;합계를 표시(32이상으로 실행)
PRINT  = 
PRINTV A

V = 32
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 실행치
PRINTV V

WAIT
SKIPDISP 0
;실행할 수 없다
SIF A < V
	RETURN 0
RETURN 1

