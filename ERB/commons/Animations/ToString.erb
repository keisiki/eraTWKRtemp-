@Animations_ToString(animation)
#FUNCTIONS
#DIM DYNAMIC animation

SIF !Animations_IsValid(animation)
    CALLF ThrowInvalidTypeException("animation")

SELECTCASE animation
    CASE ANIMATION_MISSIONARY
        RETURNF "正常位"
    CASE ANIMATION_DOGGY
        RETURNF "後背位"
ENDSELECT

CALLF ENDFUNCTION()