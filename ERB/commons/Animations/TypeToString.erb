@Animations_TypeToString(type)
#FUNCTIONS
#DIM DYNAMIC type

SIF !Animations_IsValidType(type)
    CALLF ThrowInvalidTypeException("type")

SELECTCASE type
    CASE ANIMATION_TYPE_INSERT
        RETURNF "挿入"
    CASE ANIMATION_TYPE_CLIMAX
        RETURNF "絶頂"
    CASE ANIMATION_TYPE_EJACULATION_CREAMPIE
        RETURNF "絶頂"
    CASE ANIMATION_TYPE_AFTER_EJACULATION_CREAMPIE
        RETURNF "中出し"
ENDSELECT

CALLF ENDFUNCTION()