@Align_ToString(align)
#FUNCTIONS
#DIM DYNAMIC align

SIF !Align_IsValid(align)
    CALLF ThrowInvalidTypeException("align")

SELECTCASE align
    CASE ALIGN_LEFT
        RETURNF ALIGN_LEFT_STRING
    CASE ALIGN_CENTER
        RETURNF ALIGN_CENTER_STRING
    CASE ALIGN_RIGHT
        RETURNF ALIGN_RIGHT_STRING
ENDSELECT

CALLF ENDFUNCTION()